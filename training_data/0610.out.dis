    ( Satellite (span 87 103) (rel2par elaboration-additional)
      ( Nucleus (span 87 89) (rel2par span)
        ( Nucleus (leaf 87) (rel2par span) (text _!Last Friday's announcement was the first official word_!) )
        ( Satellite (span 88 89) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 88) (rel2par List) (text _!that the project was in trouble_!) )
          ( Nucleus (leaf 89) (rel2par List) (text _!and that the company's plans for a surge in market share may have been overly optimistic._!) )
        )
      )
      ( Satellite (span 90 103) (rel2par antithesis)
        ( Nucleus (span 90 94) (rel2par span)
          ( Nucleus (span 90 92) (rel2par span)
            ( Satellite (leaf 90) (rel2par attribution) (text _!Until recently, Kellogg had been telling its sales force and Wall Street_!) )
            ( Nucleus (span 91 92) (rel2par span)
              ( Nucleus (leaf 91) (rel2par span) (text _!that by 1992 it intended to achieve a 50% share of market,_!) )
              ( Satellite (leaf 92) (rel2par elaboration-additional-e) (text _!measured in dollar volume._!) )
            )
          )
          ( Satellite (span 93 94) (rel2par elaboration-additional)
            ( Satellite (leaf 93) (rel2par antithesis) (text _!Although he called current market conditions "highly competitive,"_!) )
            ( Nucleus (leaf 94) (rel2par span) (text _!Mr. LaMothe, Kellogg's chairman and chief executive officer, forecast an earnings increase for the full year._!) )
          )
        )
        ( Satellite (span 95 103) (rel2par background)
          ( Nucleus (span 95 99) (rel2par span)
            ( Nucleus (span 95 98) (rel2par span)
              ( Nucleus (leaf 95) (rel2par span) (text _!Last year, the company earned $480.4 million, or $3.90 a share, on sales of $4.3 billion._!) )
              ( Satellite (span 96 98) (rel2par elaboration-additional)
                ( Nucleus (span 96 97) (rel2par span)
                  ( Satellite (leaf 96) (rel2par circumstance) (text _!As expected,_!) )
                  ( Nucleus (leaf 97) (rel2par span) (text _!Kellogg reported lower third-quarter earnings._!) )
                )
                ( Satellite (leaf 98) (rel2par elaboration-additional) (text _!Net fell 16% to $123.1 million, or $1.02 a share, from $145.7 million, or $1.18 a share._!) )
              )
            )
            ( Satellite (leaf 99) (rel2par elaboration-additional) (text _!Sales rose 4.8% to $1.20 billion from $1.14 billion._!) )
