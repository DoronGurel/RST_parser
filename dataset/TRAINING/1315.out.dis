        ( Satellite (span 26 60) (rel2par elaboration-additional)
          ( Nucleus (span 26 55) (rel2par span)
            ( Nucleus (span 26 45) (rel2par span)
              ( Nucleus (span 26 27) (rel2par span)
                ( Nucleus (leaf 26) (rel2par span) (text _!Mr. Fernandez doesn't take control in New York until January,_!) )
                ( Satellite (leaf 27) (rel2par antithesis) (text _!but already he's roiling the waters._!) )
              )
              ( Satellite (span 28 45) (rel2par example)
                ( Nucleus (span 28 37) (rel2par span)
                  ( Nucleus (span 28 35) (rel2par Contrast)
                    ( Nucleus (span 28 31) (rel2par span)
                      ( Nucleus (leaf 28) (rel2par span) (text _!He's attacked the concept of "building tenure," one of the most disgraceful institutions in American public schools._!) )
                      ( Satellite (span 29 31) (rel2par explanation-argumentative)
                        ( Nucleus (leaf 29) (rel2par span) (text _!It means it is virtually impossible to fire or even transfer incompetent principals._!) )
                        ( Satellite (span 30 31) (rel2par elaboration-additional)
                          ( Satellite (leaf 30) (rel2par condition) (text _!Once they are in the building,_!) )
                          ( Nucleus (leaf 31) (rel2par span) (text _!they stay._!) )
                        )
                      )
                    )
                    ( Satellite (span 32 35) (rel2par example)
                      ( Nucleus (span 32 33) (rel2par span)
                        ( Nucleus (leaf 32) (rel2par span) (text _!One South Bronx principal kept his job for 16 years,_!) )
                        ( Satellite (leaf 33) (rel2par antithesis) (text _!despite a serious drinking problem and rarely showing up for work._!) )
                      )
                      ( Satellite (span 34 35) (rel2par elaboration-additional)
                        ( Nucleus (leaf 34) (rel2par span) (text _!He was finally given leave_!) )
                        ( Satellite (leaf 35) (rel2par consequence-n) (text _!when he was arrested for allegedly buying crack._!) )
                      )
                    )
                  )
                  ( Nucleus (span 36 37) (rel2par Contrast)
                    ( Nucleus (leaf 36) (rel2par List) (text _!Naturally, the principals' union loves building tenure,_!) )
                    ( Nucleus (leaf 37) (rel2par List) (text _!and tenure has withstood previous challenge._!) )
                  )
                )
                ( Satellite (span 38 45) (rel2par elaboration-additional)
                  ( Nucleus (span 38 43) (rel2par span)
                    ( Satellite (leaf 38) (rel2par attribution) (text _!We suggest_!) )
                    ( Nucleus (span 39 43) (rel2par span)
                      ( Nucleus (leaf 39) (rel2par Sequence) (text _!that Mr. Fernandez find an incompetent principal,_!) )
                      ( Nucleus (leaf 40) (rel2par Sequence) (text _!toss him out of the building_!) )
                      ( Nucleus (span 41 43) (rel2par Sequence)
                        ( Nucleus (leaf 41) (rel2par span) (text _!and let the forces of the status quo explain to the parents_!) )
                        ( Satellite (span 42 43) (rel2par elaboration-additional-e)
                          ( Nucleus (leaf 42) (rel2par span) (text _!whatever it is_!) )
                          ( Satellite (leaf 43) (rel2par elaboration-additional) (text _!they're defending._!) )
                        )
                      )
                    )
                  )
                  ( Satellite (span 44 45) (rel2par background)
                    ( Nucleus (leaf 44) (rel2par List) (text _!In his old job, as Dade County chief, Mr. Fernandez forced out 92 teachers and reshuffled 48 principals._!) )
                    ( Nucleus (leaf 45) (rel2par List) (text _!He cut the dropout rate by 5.5%._!) )
                  )
