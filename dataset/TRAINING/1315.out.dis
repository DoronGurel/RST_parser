            ( Nucleus (span 7 10) (rel2par span)
              ( Nucleus (leaf 7) (rel2par span) (text _!Chicago's new school chief is the hard-nosed Ted Kimbrough._!) )
              ( Satellite (span 8 10) (rel2par example)
                ( Nucleus (span 8 9) (rel2par List)
                  ( Nucleus (leaf 8) (rel2par Cause-Result) (text _!At his old job in Compton, Calif., he took a bitter teachers' strike_!) )
                  ( Nucleus (leaf 9) (rel2par Cause-Result) (text _!and nearly came to blows with a school-board member._!) )
                )
                ( Nucleus (leaf 10) (rel2par List) (text _!At his first Chicago press conference, he berated the reporters._!) )
              )
            )
            ( Satellite (span 11 12) (rel2par example)
              ( Nucleus (leaf 11) (rel2par span) (text _!In New York City, the new Chancellor, Joseph Fernandez, has landed like a 16-inch shell in the middle of a system_!) )
              ( Satellite (leaf 12) (rel2par elaboration-object-attribute-e) (text _!that has been impervious to serious reform._!) )
