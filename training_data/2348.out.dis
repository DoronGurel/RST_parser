( Root (span 1 56)
  ( Nucleus (span 1 32) (rel2par Topic-Shift)
    ( Nucleus (span 1 2) (rel2par span)
      ( Nucleus (leaf 1) (rel2par Contrast) (text _!The government sold the deposits of four savings-and-loan institutions, in its first wave of sales of big, sick thrifts,_!) )
      ( Nucleus (leaf 2) (rel2par Contrast) (text _!but low bids prevented the sale of a fifth._!) )
    )
    ( Satellite (span 3 32) (rel2par summary-s)
      ( Nucleus (span 3 22) (rel2par span)
        ( Nucleus (span 3 9) (rel2par span)
          ( Nucleus (span 3 6) (rel2par span)
            ( Nucleus (leaf 3) (rel2par span) (text _!The four S&Ls were sold to large banks,_!) )
            ( Satellite (span 4 6) (rel2par manner)
              ( Nucleus (leaf 4) (rel2par span) (text _!as was the case with most of the 28 previous transactions_!) )
              ( Satellite (span 5 6) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 5) (rel2par span) (text _!initiated by the Resolution Trust Corp._!) )
                ( Satellite (leaf 6) (rel2par elaboration-additional-e) (text _!since it was created in the S&L bailout legislation two months ago._!) )
              )
            )
          )
          ( Satellite (span 7 9) (rel2par elaboration-set-member)
            ( Nucleus (span 7 8) (rel2par List)
              ( Nucleus (leaf 7) (rel2par span) (text _!Two of the four big thrifts were sold to NCNB Corp., Charlotte, N.C.,_!) )
              ( Satellite (leaf 8) (rel2par elaboration-additional) (text _!which has aggressively expanded its markets, particularly in Texas and Florida._!) )
            )
            ( Nucleus (leaf 9) (rel2par List) (text _!A Canadian bank bought another thrift, in the first RTC transaction with a foreign bank._!) )
          )
        )
        ( Satellite (span 10 22) (rel2par elaboration-additional)
          ( Nucleus (span 10 13) (rel2par span)
            ( Nucleus (leaf 10) (rel2par span) (text _!Under these deals, the RTC sells just the deposits and the healthy assets._!) )
            ( Satellite (span 11 13) (rel2par comparison)
              ( Nucleus (span 11 12) (rel2par span)
                ( Nucleus (leaf 11) (rel2par span) (text _!These "clean-bank" transactions leave the bulk of bad assets, mostly real estate, with the government,_!) )
                ( Satellite (leaf 12) (rel2par elaboration-additional-e) (text _!to be sold later._!) )
              )
              ( Satellite (leaf 13) (rel2par elaboration-additional) (text _!In these four, for instance, the RTC is stuck with $4.51 billion in bad assets._!) )
            )
          )
          ( Satellite (span 14 22) (rel2par elaboration-additional)
            ( Nucleus (span 14 15) (rel2par List)
              ( Nucleus (leaf 14) (rel2par span) (text _!Acquirers paid premiums_!) )
              ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!ranging from 1.5% to 3.7% for the deposits and branch systems, roughly in line with what analysts were expecting._!) )
            )
            ( Nucleus (span 16 22) (rel2par List)
              ( Nucleus (span 16 19) (rel2par span)
                ( Nucleus (span 16 17) (rel2par Sequence)
                  ( Nucleus (leaf 16) (rel2par span) (text _!The buyers will also be locked into deposit rates for just two weeks,_!) )
                  ( Satellite (leaf 17) (rel2par manner) (text _!as has been the case with previous deals._!) )
                )
                ( Nucleus (span 18 19) (rel2par Sequence)
                  ( Nucleus (leaf 18) (rel2par span) (text _!After that, the buyers may repudiate the rates_!) )
                  ( Satellite (leaf 19) (rel2par elaboration-object-attribute-e) (text _!paid by the former thrifts._!) )
                )
              )
              ( Satellite (span 20 22) (rel2par interpretation-s)
                ( Nucleus (leaf 20) (rel2par span) (text _!But it's uncertain whether these institutions will take those steps._!) )
                ( Satellite (span 21 22) (rel2par example)
                  ( Nucleus (leaf 21) (rel2par List) (text _!NCNB, for example, has been one of the highest rate payers in the Texas market,_!) )
                  ( Nucleus (leaf 22) (rel2par List) (text _!and in Florida, rates are especially sensitive in retirement communities._!) )
                )
              )
            )
          )
        )
      )
      ( Satellite (span 23 32) (rel2par circumstance)
        ( Nucleus (span 23 24) (rel2par span)
          ( Nucleus (leaf 23) (rel2par span) (text _!The RTC had previously targeted five thrifts for quick sales_!) )
          ( Satellite (leaf 24) (rel2par purpose) (text _!in order to spend cash by certain budgetary deadlines,_!) )
        )
        ( Satellite (span 25 32) (rel2par concession)
          ( Nucleus (span 25 26) (rel2par span)
            ( Nucleus (leaf 25) (rel2par span) (text _!but the delays illustrate the tough chore_!) )
            ( Satellite (leaf 26) (rel2par elaboration-object-attribute-e) (text _!facing the agency._!) )
          )
          ( Satellite (span 27 32) (rel2par explanation-argumentative)
            ( Nucleus (span 27 29) (rel2par span)
              ( Nucleus (leaf 27) (rel2par span) (text _!"These thrifts are beached whales,"_!) )
              ( Satellite (span 28 29) (rel2par attribution)
                ( Nucleus (leaf 28) (rel2par span) (text _!said Bert Ely, an industry consultant_!) )
                ( Satellite (leaf 29) (rel2par elaboration-additional-e) (text _!based in Alexandria, Va._!) )
              )
            )
            ( Satellite (span 30 32) (rel2par example)
              ( Nucleus (span 30 31) (rel2par Same-Unit)
                ( Nucleus (leaf 30) (rel2par span) (text _!For example, the delay_!) )
                ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!in selling People's Heritage Savings, Salina, Kan., with $1.7 billion in assets,_!) )
              )
              ( Nucleus (leaf 32) (rel2par Same-Unit) (text _!has forced the RTC to consider selling off the thrift branch-by-branch, instead of as a whole institution._!) )
            )
          )
        )
      )
    )
  )
  ( Nucleus (span 33 56) (rel2par Topic-Shift)
    ( Nucleus (span 33 45) (rel2par List)
      ( Nucleus (leaf 33) (rel2par span) (text _!NCNB continued its foray into the Florida and Texas markets._!) )
      ( Satellite (span 34 45) (rel2par elaboration-general-specific)
        ( Nucleus (span 34 40) (rel2par List)
          ( Nucleus (span 34 35) (rel2par span)
            ( Nucleus (leaf 34) (rel2par span) (text _!NCNB will acquire University Federal Savings Association, Houston,_!) )
            ( Satellite (leaf 35) (rel2par elaboration-additional-e) (text _!which had assets of $2.8 billion._!) )
          )
          ( Satellite (span 36 40) (rel2par elaboration-additional-e)
            ( Nucleus (leaf 36) (rel2par List) (text _!NCNB Texas National Bank will pay the RTC a premium of $129 million for $3.5 billion in deposits._!) )
            ( Nucleus (span 37 40) (rel2par List)
              ( Nucleus (span 37 38) (rel2par Same-Unit)
                ( Nucleus (leaf 37) (rel2par span) (text _!As a measure of the depths_!) )
                ( Satellite (leaf 38) (rel2par elaboration-object-attribute-e) (text _!to which the Texas real estate market has sunk,_!) )
              )
              ( Nucleus (span 39 40) (rel2par Same-Unit)
                ( Nucleus (leaf 39) (rel2par span) (text _!the RTC will pay $3.8 billion to NCNB_!) )
                ( Satellite (leaf 40) (rel2par purpose) (text _!to take $750 million of bad assets._!) )
              )
            )
          )
        )
        ( Nucleus (span 41 45) (rel2par List)
          ( Nucleus (span 41 42) (rel2par span)
            ( Nucleus (leaf 41) (rel2par span) (text _!NCNB also acquired Freedom Savings & Loan Association, Tampa, Fla.,_!) )
            ( Satellite (leaf 42) (rel2par elaboration-additional-e) (text _!which had total assets of $900 million._!) )
          )
          ( Satellite (span 43 45) (rel2par elaboration-additional)
            ( Nucleus (leaf 43) (rel2par List) (text _!NCNB will pay the RTC a premium of $40.4 million for $1.1 billion in deposits._!) )
            ( Nucleus (span 44 45) (rel2par List)
              ( Nucleus (leaf 44) (rel2par span) (text _!NCNB will also acquire $266 million of Freedom's assets from the RTC,_!) )
              ( Satellite (leaf 45) (rel2par elaboration-additional-e) (text _!which will require $875 million in assistance._!) )
            )
          )
        )
      )
    )
    ( Nucleus (span 46 50) (rel2par List)
      ( Nucleus (span 46 47) (rel2par span)
        ( Nucleus (leaf 46) (rel2par span) (text _!Meridian Bancorp Inc., Reading, Pa., will acquire Hill Financial Savings Association, Red Hill, Pa.,_!) )
        ( Satellite (leaf 47) (rel2par elaboration-additional-e) (text _!which had $2.3 billion in assets._!) )
      )
      ( Satellite (span 48 50) (rel2par elaboration-additional)
        ( Nucleus (span 48 49) (rel2par List)
          ( Nucleus (leaf 48) (rel2par span) (text _!Meridian will pay a premium of $30.5 million_!) )
          ( Satellite (leaf 49) (rel2par purpose) (text _!to assume $2 billion in deposits._!) )
        )
        ( Nucleus (leaf 50) (rel2par List) (text _!It will also purchase $845 million of the thrift's assets, with $1.9 billion in RTC assistance._!) )
      )
    )
    ( Nucleus (span 51 56) (rel2par List)
      ( Nucleus (span 51 52) (rel2par span)
        ( Nucleus (leaf 51) (rel2par span) (text _!In the first RTC transaction with a foreign buyer, Royal Trustco Ltd., Toronto, will acquire Pacific Savings Bank, Costa Mesa, Calif.,_!) )
        ( Satellite (leaf 52) (rel2par elaboration-additional-e) (text _!which had $949 million in assets._!) )
      )
      ( Satellite (span 53 56) (rel2par elaboration-additional)
        ( Nucleus (span 53 54) (rel2par List)
          ( Nucleus (leaf 53) (rel2par span) (text _!Royal Trustco will pay the RTC $25 million_!) )
          ( Satellite (leaf 54) (rel2par purpose) (text _!to assume $989 million in deposits._!) )
        )
        ( Nucleus (span 55 56) (rel2par List)
          ( Nucleus (leaf 55) (rel2par List) (text _!It will also purchase $473 million in assets,_!) )
          ( Nucleus (leaf 56) (rel2par List) (text _!and receive $550 million in assistance from the RTC._!) )
        )
      )
    )
  )
)
