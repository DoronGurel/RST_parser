          ( Satellite (span 27 38) (rel2par elaboration-additional)
            ( Nucleus (span 27 29) (rel2par span)
              ( Nucleus (leaf 27) (rel2par List) (text _!Exxon's net income during the third quarter dropped to $1.11 billion, or 87 cents a share, from $1.22 billion, or 93 cents a share, a year earlier._!) )
              ( Nucleus (leaf 28) (rel2par List) (text _!Revenue rose 8.1%, to $23.65 billion from $21.88 billion._!) )
              ( Nucleus (leaf 29) (rel2par List) (text _!During the third quarter, Exxon purchased 8.34 million shares of its stock at a cost of $373 million._!) )
            )
            ( Satellite (span 30 38) (rel2par elaboration-additional)
              ( Nucleus (span 30 32) (rel2par span)
                ( Nucleus (leaf 30) (rel2par span) (text _!Exxon's profitability, like that of many other oil companies, was hurt during the third quarter by declining returns from the chemicals and refining and marketing businesses._!) )
                ( Satellite (span 31 32) (rel2par elaboration-general-specific)
                  ( Nucleus (leaf 31) (rel2par List) (text _!Exxon's earnings from chemicals operations fell $90 million, to $254 million,_!) )
                  ( Nucleus (leaf 32) (rel2par List) (text _!while refining and marketing profits declined $180 million, to $357 million._!) )
                )
              )
              ( Satellite (span 33 38) (rel2par elaboration-additional)
                ( Nucleus (span 33 36) (rel2par span)
                  ( Nucleus (span 33 35) (rel2par span)
                    ( Satellite (leaf 33) (rel2par antithesis) (text _!Although crude oil prices were significantly higher this year,_!) )
                    ( Nucleus (span 34 35) (rel2par span)
                      ( Satellite (leaf 34) (rel2par consequence-n) (text _!they weren't strong enough_!) )
                      ( Nucleus (leaf 35) (rel2par span) (text _!to offset the declining profits in those business sectors at most oil companies,_!) )
                    )
