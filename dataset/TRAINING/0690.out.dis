  ( Satellite (span 69 90) (rel2par elaboration-additional)
    ( Nucleus (span 69 84) (rel2par span)
      ( Satellite (span 69 70) (rel2par concession)
        ( Nucleus (leaf 69) (rel2par span) (text _!The FDA already requires drug manufacturers to include warnings with insulin products_!) )
        ( Satellite (leaf 70) (rel2par elaboration-object-attribute-e) (text _!that symptoms of hypoglycemia are less pronounced with human insulin than with animal-based products._!) )
