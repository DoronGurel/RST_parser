( Root (span 1 47)
  ( Nucleus (span 1 3) (rel2par span)
    ( Nucleus (span 1 2) (rel2par span)
      ( Nucleus (leaf 1) (rel2par span) (text _!Texas Instruments Inc., once a pioneer in portable computer technology, today will make a bid_!) )
      ( Satellite (leaf 2) (rel2par purpose-e) (text _!to reassert itself in that business_!) )
    )
    ( Satellite (leaf 3) (rel2par manner) (text _!by unveiling three small personal computers._!) )
  )
  ( Satellite (span 4 47) (rel2par elaboration-additional)
    ( Nucleus (span 4 8) (rel2par span)
      ( Nucleus (leaf 4) (rel2par span) (text _!The announcements are scheduled to be made in Temple, Texas,_!) )
      ( Satellite (span 5 8) (rel2par elaboration-additional-e)
        ( Nucleus (leaf 5) (rel2par span) (text _!and include a so-called "notebook" PC_!) )
        ( Satellite (span 6 8) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 6) (rel2par List) (text _!that weighs less than seven pounds,_!) )
          ( Nucleus (leaf 7) (rel2par List) (text _!has a built-in hard disk drive_!) )
          ( Nucleus (leaf 8) (rel2par List) (text _!and is powered by Intel Corp. 's 286 microprocessor._!) )
        )
      )
    )
    ( Satellite (span 9 47) (rel2par elaboration-additional)
      ( Nucleus (span 9 13) (rel2par span)
        ( Nucleus (leaf 9) (rel2par span) (text _!That introduction comes_!) )
        ( Satellite (span 10 13) (rel2par temporal-after)
          ( Nucleus (span 10 12) (rel2par Same-Unit)
            ( Nucleus (leaf 10) (rel2par span) (text _!only two weeks after Compaq Computer Corp.,_!) )
            ( Satellite (span 11 12) (rel2par elaboration-additional-e)
              ( Satellite (leaf 11) (rel2par attribution) (text _!believing_!) )
              ( Nucleus (leaf 12) (rel2par span) (text _!it had a lead of three to six months on competitors,_!) )
            )
          )
          ( Nucleus (leaf 13) (rel2par Same-Unit) (text _!introduced the first U.S. notebook computer with such features._!) )
        )
      )
      ( Satellite (span 14 47) (rel2par elaboration-additional)
        ( Nucleus (span 14 17) (rel2par span)
          ( Nucleus (span 14 15) (rel2par span)
            ( Satellite (leaf 14) (rel2par antithesis) (text _!Despite the inevitable comparison with Compaq, however,_!) )
            ( Nucleus (leaf 15) (rel2par span) (text _!Texas Instruments' new notebook won't be a direct competitor._!) )
          )
          ( Satellite (span 16 17) (rel2par explanation-argumentative)
            ( Nucleus (leaf 16) (rel2par Contrast) (text _!While Compaq sells its machines to businesses through computer retailers,_!) )
            ( Nucleus (leaf 17) (rel2par Contrast) (text _!Texas Instruments will be selling most of its machines to the industrial market and to value-added resellers and original-equipment manufacturers._!) )
          )
        )
        ( Satellite (span 18 47) (rel2par elaboration-additional)
          ( Nucleus (span 18 45) (rel2par span)
            ( Nucleus (span 18 31) (rel2par span)
              ( Nucleus (span 18 19) (rel2par span)
                ( Nucleus (leaf 18) (rel2par span) (text _!The introductions also mark Texas Instruments' plunge back into a technology_!) )
                ( Satellite (leaf 19) (rel2par elaboration-object-attribute-e) (text _!it has all but ignored for the past several years._!) )
              )
              ( Satellite (span 20 31) (rel2par circumstance)
                ( Nucleus (span 20 25) (rel2par span)
                  ( Satellite (span 20 22) (rel2par concession)
                    ( Nucleus (span 20 21) (rel2par Same-Unit)
                      ( Nucleus (leaf 20) (rel2par span) (text _!Although the Dallas-based computer giant introduced the first portable data terminal in 1971_!) )
                      ( Satellite (leaf 21) (rel2par elaboration-object-attribute-e) (text _!-- a 38-pound monster --_!) )
                    )
                    ( Nucleus (leaf 22) (rel2par Same-Unit) (text _!and the world's first microprocessor-based portable in 1976,_!) )
                  )
                  ( Nucleus (span 23 25) (rel2par span)
                    ( Nucleus (span 23 24) (rel2par Same-Unit)
                      ( Nucleus (leaf 23) (rel2par span) (text _!the only portable machines_!) )
                      ( Satellite (leaf 24) (rel2par elaboration-object-attribute-e) (text _!it has introduced since the first part of the decade_!) )
                    )
                    ( Nucleus (leaf 25) (rel2par Same-Unit) (text _!have been "dumb" terminals with limited on-board processing ability._!) )
                  )
                )
                ( Satellite (span 26 31) (rel2par elaboration-additional)
                  ( Nucleus (leaf 26) (rel2par Contrast) (text _!It stopped selling a standard personal computer a while ago._!) )
                  ( Nucleus (span 27 31) (rel2par Contrast)
                    ( Nucleus (leaf 27) (rel2par span) (text _!Now that is about to change,_!) )
                    ( Satellite (span 28 31) (rel2par elaboration-additional)
                      ( Nucleus (leaf 28) (rel2par span) (text _!as Texas Instruments begins marketing two 14-pound laptop PCs with 20 megabyte and 40 megabyte hard drives._!) )
                      ( Satellite (span 29 31) (rel2par elaboration-additional)
                        ( Nucleus (leaf 29) (rel2par List) (text _!The laptops are not revolutionary_!) )
                        ( Nucleus (span 30 31) (rel2par List)
                          ( Nucleus (leaf 30) (rel2par span) (text _!and, indeed, are tardy in a market_!) )
                          ( Satellite (leaf 31) (rel2par elaboration-object-attribute) (text _!first opened by GRiD Systems Corp., now a unit of Tandy Corp., almost two years ago._!) )
                        )
                      )
                    )
                  )
                )
              )
            )
            ( Satellite (span 32 45) (rel2par elaboration-additional)
              ( Nucleus (leaf 32) (rel2par span) (text _!But the notebook, with the more advanced microprocessor and hard disk, is more innovative._!) )
              ( Satellite (span 33 45) (rel2par example)
                ( Nucleus (span 33 43) (rel2par Contrast)
                  ( Nucleus (leaf 33) (rel2par List) (text _!Weighing 6.7 pounds with battery,_!) )
                  ( Nucleus (leaf 34) (rel2par List) (text _!the notebook measures 8.2 by 11.7 inches,_!) )
                  ( Nucleus (leaf 35) (rel2par List) (text _!has a 20-megabyte hard disk drive_!) )
                  ( Nucleus (span 36 37) (rel2par List)
                    ( Nucleus (leaf 36) (rel2par Comparison) (text _!and boasts a backlit screen_!) )
                    ( Nucleus (leaf 37) (rel2par Comparison) (text _!that is 22% larger than Compaq's._!) )
                  )
                  ( Nucleus (span 38 43) (rel2par List)
                    ( Nucleus (span 38 41) (rel2par List)
                      ( Nucleus (span 38 40) (rel2par Comparison)
                        ( Nucleus (span 38 39) (rel2par Same-Unit)
                          ( Nucleus (leaf 38) (rel2par span) (text _!Its keyboard,_!) )
                          ( Satellite (leaf 39) (rel2par attribution) (text _!according to industry consultants,_!) )
                        )
                        ( Nucleus (leaf 40) (rel2par Same-Unit) (text _!is better than Compaq's,_!) )
                      )
                      ( Nucleus (leaf 41) (rel2par Comparison) (text _!but its battery life of two to three hours is shorter._!) )
                    )
                    ( Nucleus (span 42 43) (rel2par List)
                      ( Nucleus (leaf 42) (rel2par span) (text _!It doesn't have an internal floppy disk drive,_!) )
                      ( Satellite (leaf 43) (rel2par elaboration-additional) (text _!although a snap-on drive can be purchased separately._!) )
                    )
                  )
                )
                ( Nucleus (span 44 45) (rel2par Contrast)
                  ( Nucleus (leaf 44) (rel2par span) (text _!Its greatest drawback may be its 3-inch thickness, big enough for one consultant_!) )
                  ( Satellite (leaf 45) (rel2par elaboration-object-attribute-e) (text _!to describe it as "clunky."_!) )
                )
              )
            )
          )
          ( Satellite (span 46 47) (rel2par elaboration-additional)
            ( Nucleus (leaf 46) (rel2par Comparison) (text _!List prices on the heavier Texas Instrument laptops will be $4,999 for the TI Model 25, with a 20 megabyte disk drive, and $5,599 for the 40-megabyte Model 45._!) )
            ( Nucleus (leaf 47) (rel2par Comparison) (text _!The notebook, the TI Model 12, will be priced at $4,199._!) )
          )
        )
      )
    )
  )
)
