            )
            ( Satellite (span 65 71) (rel2par elaboration-additional)
              ( Satellite (span 65 67) (rel2par consequence-n)
                ( Nucleus (leaf 65) (rel2par span) (text _!The European defense industry is consolidating;_!) )
                ( Satellite (span 66 67) (rel2par example)
                  ( Nucleus (leaf 66) (rel2par List) (text _!for instance, West Germany's Siemens AG recently joined GEC in a takeover of Britain's Plessey Co.,_!) )
                  ( Nucleus (leaf 67) (rel2par List) (text _!and Daimler-Benz agreed to buy Messerschmitt-Boelkow Blohm G.m.b. H._!) )
                )
              )
              ( Nucleus (span 68 71) (rel2par span)
                ( Satellite (leaf 68) (rel2par circumstance) (text _!In missiles, Thomson is already overshadowed by British Aerospace and by its home rival, France's Aerospatiale S.A.;_!) )
                ( Nucleus (span 69 71) (rel2par span)
                  ( Nucleus (span 69 70) (rel2par span)
                    ( Nucleus (leaf 69) (rel2par span) (text _!to better compete,_!) )
                    ( Satellite (leaf 70) (rel2par attribution) (text _!Thomson officials say,_!) )
