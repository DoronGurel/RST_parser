          ( Nucleus (span 1 9) (rel2par span)
            ( Nucleus (span 1 4) (rel2par span)
              ( Satellite (leaf 1) (rel2par attribution) (text _!British Aerospace PLC and France's Thomson-CSF S.A. said_!) )
              ( Nucleus (span 2 4) (rel2par span)
                ( Nucleus (span 2 3) (rel2par span)
                  ( Nucleus (leaf 2) (rel2par span) (text _!they are nearing an agreement_!) )
                  ( Satellite (leaf 3) (rel2par elaboration-object-attribute-e) (text _!to merge their guided-missile divisions,_!) )
                )
                ( Satellite (leaf 4) (rel2par consequence-s) (text _!greatly expanding collaboration between the two defense contractors._!) )
              )
            )
            ( Satellite (span 5 9) (rel2par elaboration-additional)
              ( Nucleus (span 5 8) (rel2par List)
                ( Nucleus (span 5 6) (rel2par Same-Unit)
                  ( Nucleus (leaf 5) (rel2par span) (text _!The 50-50 joint venture,_!) )
                  ( Satellite (leaf 6) (rel2par elaboration-additional-e) (text _!which may be dubbed Eurodynamics,_!) )
                )
                ( Nucleus (span 7 8) (rel2par Same-Unit)
                  ( Nucleus (leaf 7) (rel2par span) (text _!would have combined annual sales of at least #1.4 billion_!) )
                  ( Satellite (leaf 8) (rel2par restatement-e) (text _!($2.17 billion)_!) )
                )
              )
              ( Nucleus (leaf 9) (rel2par List) (text _!and would be among the world's largest missile makers._!) )
            )
          )
          ( Satellite (span 10 12) (rel2par elaboration-additional)
            ( Nucleus (span 10 11) (rel2par span)
              ( Satellite (leaf 10) (rel2par enablement) (text _!After two years of talks, plans for the venture are sufficiently advanced_!) )
              ( Nucleus (leaf 11) (rel2par span) (text _!for the companies to seek French and British government clearance._!) )
            )
            ( Satellite (leaf 12) (rel2par elaboration-additional) (text _!The companies hope for a final agreement by year-end._!) )
          )
        )
        ( Satellite (span 13 56) (rel2par elaboration-additional)
          ( Nucleus (span 13 23) (rel2par span)
            ( Nucleus (span 13 15) (rel2par span)
              ( Nucleus (span 13 14) (rel2par span)
                ( Nucleus (leaf 13) (rel2par List) (text _!The venture would strengthen the rapidly growing ties between the two companies,_!) )
                ( Nucleus (leaf 14) (rel2par List) (text _!and help make them a leading force in European defense contracting._!) )
              )
              ( Satellite (leaf 15) (rel2par circumstance) (text _!In recent months, a string of cross-border mergers and joint ventures have reshaped the once-balkanized world of European arms manufacture._!) )
            )
            ( Satellite (span 16 23) (rel2par elaboration-additional)
              ( Nucleus (span 16 20) (rel2par span)
                ( Nucleus (leaf 16) (rel2par List) (text _!Already, British Aerospace and French government-controlled Thomson-CSF collaborate on a British missile contract and on an air-traffic control radar system._!) )
                ( Nucleus (span 17 20) (rel2par List)
                  ( Satellite (leaf 17) (rel2par attribution) (text _!Just last week they announced_!) )
                  ( Nucleus (span 18 20) (rel2par span)
                    ( Nucleus (leaf 18) (rel2par span) (text _!they may make a joint bid_!) )
                    ( Satellite (span 19 20) (rel2par elaboration-object-attribute-e)
                      ( Nucleus (leaf 19) (rel2par span) (text _!to buy Ferranti International Signal PLC, a smaller British defense contractor_!) )
                      ( Satellite (leaf 20) (rel2par elaboration-additional-e) (text _!rocked by alleged accounting fraud at a U.S. unit._!) )
                    )
                  )
                )
              )
              ( Satellite (span 21 23) (rel2par consequence-s)
                ( Nucleus (span 21 22) (rel2par Same-Unit)
                  ( Nucleus (leaf 21) (rel2par span) (text _!The sudden romance of British Aerospace and Thomson-CSF_!) )
                  ( Satellite (leaf 22) (rel2par elaboration-additional-e) (text _!-- traditionally bitter competitors for Middle East and Third World weapons contracts --_!) )
                )
                ( Nucleus (leaf 23) (rel2par Same-Unit) (text _!is stirring controversy in Western Europe's defense industry._!) )
              )
            )
          )
          ( Satellite (span 24 56) (rel2par elaboration-additional)
            ( Nucleus (span 24 29) (rel2par span)
              ( Satellite (span 24 25) (rel2par antithesis)
                ( Nucleus (leaf 24) (rel2par span) (text _!Most threatened by closer British Aerospace-Thomson ties would be their respective national rivals,_!) )
                ( Satellite (leaf 25) (rel2par elaboration-set-member-e) (text _!including Matra S.A. in France and Britain's General Electric Co. PLC._!) )
              )
              ( Nucleus (span 26 29) (rel2par span)
                ( Nucleus (span 26 27) (rel2par Same-Unit)
                  ( Nucleus (leaf 26) (rel2par span) (text _!But neither Matra nor GEC_!) )
                  ( Satellite (leaf 27) (rel2par elaboration-additional-e) (text _!-- unrelated to Stamford, Conn.-based General Electric Co. --_!) )
                )
                ( Nucleus (span 28 29) (rel2par Same-Unit)
                  ( Nucleus (leaf 28) (rel2par span) (text _!are sitting quietly by_!) )
                  ( Satellite (leaf 29) (rel2par circumstance) (text _!as their competitors join forces._!) )
                )
              )
            )
