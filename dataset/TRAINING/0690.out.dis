                ( Satellite (span 7 8) (rel2par purpose)
                  ( Satellite (leaf 7) (rel2par attribution) (text _!to see_!) )
                  ( Nucleus (leaf 8) (rel2par span) (text _!if similar deaths have occurred here._!) )
                )
              )
            )
          )
          ( Satellite (span 9 17) (rel2par elaboration-additional)
            ( Satellite (leaf 9) (rel2par background) (text _!The United Kingdom reports came from Dr. Patrick Toseland, head of clinical chemistry at Guy's Hospital in London._!) )
            ( Nucleus (span 10 17) (rel2par span)
              ( Nucleus (span 10 14) (rel2par span)
                ( Satellite (leaf 10) (rel2par attribution) (text _!In a telephone interview Friday, Dr. Toseland said_!) )
                ( Nucleus (span 11 14) (rel2par span)
                  ( Nucleus (span 11 12) (rel2par Same-Unit)
                    ( Nucleus (leaf 11) (rel2par span) (text _!the number of sudden, unexplained deaths of diabetics_!) )
                    ( Satellite (leaf 12) (rel2par elaboration-object-attribute-e) (text _!he had seen this year_!) )
                  )
                  ( Nucleus (span 13 14) (rel2par Same-Unit)
                    ( Nucleus (leaf 13) (rel2par span) (text _!was 17_!) )
                    ( Satellite (leaf 14) (rel2par comparison) (text _!compared with just two in 1985._!) )
                  )
                )
              )
              ( Satellite (span 15 17) (rel2par elaboration-additional)
                ( Nucleus (span 15 16) (rel2par span)
                  ( Nucleus (leaf 15) (rel2par span) (text _!At least six of the deaths occurred among relatively young diabetics_!) )
                  ( Satellite (leaf 16) (rel2par elaboration-object-attribute-e) (text _!who had switched from animal to human insulin within the past year,_!) )
                )
                ( Satellite (leaf 17) (rel2par attribution) (text _!he said._!) )
              )
            )
          )
        )
        ( Satellite (span 18 36) (rel2par elaboration-additional)
          ( Nucleus (span 18 19) (rel2par span)
            ( Satellite (leaf 18) (rel2par attribution) (text _!Dr. Solomon Sobel, director of metabolism and endrocrine drug products for the U.S. Food and Drug Administration, said_!) )
            ( Nucleus (leaf 19) (rel2par span) (text _!FDA officials have discussed Dr. Toseland's findings "fairly intensively."_!) )
          )
          ( Satellite (span 20 36) (rel2par elaboration-additional)
            ( Nucleus (span 20 23) (rel2par span)
              ( Satellite (leaf 20) (rel2par concession) (text _!While there have been no reports of similar sudden unexplained deaths among diabetics in the U.S.,_!) )
              ( Nucleus (span 21 23) (rel2par span)
                ( Satellite (leaf 21) (rel2par attribution) (text _!Dr. Sobel said_!) )
                ( Nucleus (span 22 23) (rel2par span)
                  ( Nucleus (leaf 22) (rel2par List) (text _!the FDA plans to examine Dr. Toseland's evidence_!) )
                  ( Nucleus (leaf 23) (rel2par List) (text _!and is considering its own study here._!) )
                )
              )
            )
            ( Satellite (span 24 36) (rel2par elaboration-additional)
              ( Nucleus (span 24 30) (rel2par span)
                ( Satellite (leaf 24) (rel2par attribution) (text _!Dr. Toseland, a toxicologist, said_!) )
                ( Nucleus (span 25 30) (rel2par span)
                  ( Nucleus (leaf 25) (rel2par span) (text _!he was preparing an article for a British forensic medical journal_!) )
                  ( Satellite (span 26 30) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 26) (rel2par span) (text _!raising the possibility_!) )
                    ( Satellite (span 27 30) (rel2par elaboration-object-attribute-e)
                      ( Nucleus (leaf 27) (rel2par span) (text _!that the deaths may have occurred_!) )
                      ( Satellite (span 28 30) (rel2par consequence-n)
                        ( Nucleus (leaf 28) (rel2par span) (text _!after human insulin blunted critical warning signs_!) )
                        ( Satellite (span 29 30) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (leaf 29) (rel2par span) (text _!indicating hypoglycemia, or low blood sugar,_!) )
                          ( Satellite (leaf 30) (rel2par elaboration-object-attribute-e) (text _!which can kill diabetics._!) )
                        )
                      )
                    )
                  )
                )
              )
              ( Satellite (span 31 36) (rel2par elaboration-additional)
                ( Satellite (span 31 33) (rel2par antithesis)
                  ( Satellite (leaf 31) (rel2par background) (text _!The usual warning signs of hypoglycemia include sweating, anxiety and cramps._!) )
                  ( Nucleus (span 32 33) (rel2par span)
                    ( Nucleus (leaf 32) (rel2par span) (text _!With proper warning, diabetics can easily raise their blood sugar to safe levels_!) )
                    ( Satellite (leaf 33) (rel2par means) (text _!by eating sugar or sugary food._!) )
                  )
                )
                ( Nucleus (span 34 36) (rel2par span)
                  ( Nucleus (span 34 35) (rel2par span)
                    ( Satellite (leaf 34) (rel2par attribution) (text _!"The anecdotal data certainly shows_!) )
                    ( Nucleus (leaf 35) (rel2par span) (text _!that some of the people were not aware of the rapid onset of hypoglycemia,"_!) )
                  )
                  ( Satellite (leaf 36) (rel2par attribution) (text _!Dr. Toseland said._!) )
                )
              )
            )
          )
        )
      )
      ( Satellite (span 37 57) (rel2par elaboration-additional)
        ( Nucleus (span 37 46) (rel2par span)
          ( Satellite (span 37 44) (rel2par result)
            ( Satellite (span 37 39) (rel2par antithesis)
              ( Satellite (leaf 37) (rel2par attribution) (text _!At the U.S. National Institutes of Health, Dr. Robert E. Silverman, chief of the diabetes program branch, said_!) )
              ( Nucleus (span 38 39) (rel2par span)
                ( Nucleus (leaf 38) (rel2par span) (text _!no evidence of unexpected deaths from hypoglycemia had shown up in a study of 1,500 diabetics_!) )
                ( Satellite (leaf 39) (rel2par elaboration-object-attribute-e) (text _!that has been under way at NIH for five years._!) )
              )
            )
            ( Nucleus (span 40 44) (rel2par span)
              ( Satellite (leaf 40) (rel2par attribution) (text _!However, he said_!) )
              ( Nucleus (span 41 44) (rel2par span)
                ( Nucleus (span 41 42) (rel2par Same-Unit)
                  ( Nucleus (leaf 41) (rel2par span) (text _!officials_!) )
                  ( Satellite (leaf 42) (rel2par elaboration-object-attribute-e) (text _!conducting the study_!) )
                )
                ( Nucleus (span 43 44) (rel2par Same-Unit)
                  ( Nucleus (leaf 43) (rel2par span) (text _!hadn't been looking for signs of problems_!) )
                  ( Satellite (leaf 44) (rel2par elaboration-object-attribute-e) (text _!related to hypoglycemia unawareness._!) )
                )
              )
            )
          )
          ( Nucleus (span 45 46) (rel2par span)
            ( Nucleus (leaf 45) (rel2par span) (text _!"We are now monitoring for it much more closely,"_!) )
            ( Satellite (leaf 46) (rel2par attribution) (text _!he said._!) )
          )
        )
        ( Satellite (span 47 57) (rel2par elaboration-additional)
          ( Satellite (span 47 53) (rel2par antithesis)
            ( Nucleus (span 47 50) (rel2par span)
              ( Nucleus (span 47 49) (rel2par span)
                ( Satellite (leaf 47) (rel2par attribution) (text _!"We do know_!) )
                ( Nucleus (span 48 49) (rel2par span)
                  ( Nucleus (leaf 48) (rel2par span) (text _!there are slight differences in the way_!) )
                  ( Satellite (leaf 49) (rel2par elaboration-object-attribute-e) (text _!human and animal insulins drive down blood sugar,"_!) )
                )
              )
              ( Satellite (leaf 50) (rel2par attribution) (text _!Dr. Sobel said._!) )
            )
            ( Satellite (span 51 53) (rel2par elaboration-general-specific)
              ( Nucleus (span 51 52) (rel2par span)
                ( Nucleus (leaf 51) (rel2par List) (text _!The human-based drug starts the blood sugar dropping sooner_!) )
                ( Nucleus (leaf 52) (rel2par List) (text _!and drives it down faster,_!) )
              )
              ( Satellite (leaf 53) (rel2par attribution) (text _!he said._!) )
            )
          )
          ( Nucleus (span 54 57) (rel2par span)
            ( Nucleus (span 54 56) (rel2par span)
              ( Satellite (leaf 54) (rel2par attribution-n) (text _!"But we don't believe_!) )
              ( Nucleus (span 55 56) (rel2par span)
                ( Satellite (leaf 55) (rel2par consequence-n) (text _!there is enough of a difference_!) )
                ( Nucleus (leaf 56) (rel2par span) (text _!to be clinically significant,"_!) )
              )
            )
            ( Satellite (leaf 57) (rel2par attribution) (text _!Dr. Sobel said._!) )
          )
