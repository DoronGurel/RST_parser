    ( Satellite (span 58 68) (rel2par elaboration-additional)
      ( Satellite (leaf 58) (rel2par statement-response-n) (text _!Reports of Dr. Toseland's findings in the British press have triggered widespread concern among diabetics there._!) )
      ( Nucleus (span 59 68) (rel2par span)
        ( Nucleus (span 59 63) (rel2par Comparison)
          ( Nucleus (span 59 60) (rel2par Same-Unit)
            ( Nucleus (leaf 59) (rel2par span) (text _!Both the British Diabetic Association and the Committee on Safety in Medicines_!) )
