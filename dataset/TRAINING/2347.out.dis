              ( Nucleus (leaf 9) (rel2par Same-Unit) (text _!but in different ways._!) )
            )
          )
          ( Satellite (span 10 11) (rel2par elaboration-additional)
            ( Nucleus (leaf 10) (rel2par Contrast) (text _!Justices Brennan and Stevens appear philosophical about it;_!) )
            ( Nucleus (leaf 11) (rel2par Contrast) (text _!Justices Marshall and Blackmun appear fighting mad._!) )
          )
        )
