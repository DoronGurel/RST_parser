( Root (span 1 54)
  ( Nucleus (span 1 34) (rel2par Topic-Drift)
    ( Nucleus (span 1 27) (rel2par span)
      ( Nucleus (span 1 10) (rel2par span)
        ( Nucleus (span 1 4) (rel2par span)
          ( Nucleus (span 1 3) (rel2par span)
            ( Nucleus (span 1 2) (rel2par span)
              ( Nucleus (leaf 1) (rel2par span) (text _!Southern Co. 's Gulf Power Co. unit may plead guilty this week to charges_!) )
              ( Satellite (leaf 2) (rel2par elaboration-object-attribute-e) (text _!that it illegally steered company money to politicians through outside vendors,_!) )
            )
            ( Satellite (leaf 3) (rel2par attribution) (text _!according to individuals close to an investigation of the utility holding company._!) )
          )
          ( Satellite (leaf 4) (rel2par interpretation-s) (text _!The tentative settlement between Gulf Power, a Pensacola, Fla., electric company, and federal prosecutors would mark the end of one part of a wide-ranging inquiry of Southern Co. in the past year._!) )
        )
        ( Satellite (span 5 10) (rel2par circumstance)
          ( Nucleus (span 5 7) (rel2par List)
            ( Satellite (leaf 5) (rel2par attribution) (text _!A grand jury has been investigating_!) )
            ( Nucleus (span 6 7) (rel2par span)
              ( Nucleus (leaf 6) (rel2par span) (text _!whether officials at Southern Co. conspired to cover up their accounting for spare parts_!) )
              ( Satellite (leaf 7) (rel2par purpose) (text _!to evade federal income taxes._!) )
            )
          )
          ( Nucleus (span 8 10) (rel2par List)
            ( Satellite (leaf 8) (rel2par attribution) (text _!The grand jury has also been investigating_!) )
            ( Nucleus (span 9 10) (rel2par span)
              ( Nucleus (leaf 9) (rel2par span) (text _!whether Gulf Power executives violated the federal Utility Holding Company Act,_!) )
              ( Satellite (leaf 10) (rel2par definition-e) (text _!which prohibits certain utilities from making political contributions._!) )
            )
          )
        )
      )
      ( Satellite (span 11 27) (rel2par elaboration-additional)
        ( Nucleus (span 11 20) (rel2par Statement-Response)
          ( Nucleus (span 11 17) (rel2par span)
            ( Satellite (leaf 11) (rel2par attribution) (text _!The individuals said_!) )
            ( Nucleus (span 12 17) (rel2par span)
              ( Nucleus (leaf 12) (rel2par span) (text _!Gulf Power and federal prosecutors are considering a settlement_!) )
              ( Satellite (span 13 17) (rel2par elaboration-object-attribute-e)
                ( Nucleus (span 13 15) (rel2par span)
                  ( Nucleus (leaf 13) (rel2par Sequence) (text _!under which the company would plead guilty to two felony charges_!) )
                  ( Nucleus (span 14 15) (rel2par Sequence)
                    ( Nucleus (leaf 14) (rel2par span) (text _!and pay fines_!) )
                    ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!totaling between $500,000 and $1.5 million._!) )
                  )
                )
                ( Satellite (span 16 17) (rel2par elaboration-set-member)
                  ( Nucleus (leaf 16) (rel2par List) (text _!Under one count, Gulf Power would plead guilty to conspiring to violate the Utility Holding Company Act._!) )
                  ( Nucleus (leaf 17) (rel2par List) (text _!Under the second count, the company would plead guilty to conspiring to evade taxes._!) )
                )
              )
            )
          )
          ( Satellite (span 18 20) (rel2par elaboration-additional)
            ( Nucleus (span 18 19) (rel2par span)
              ( Nucleus (leaf 18) (rel2par span) (text _!The guilty pleas would be made solely by Gulf Power,_!) )
              ( Satellite (leaf 19) (rel2par attribution) (text _!the individuals said._!) )
            )
            ( Satellite (leaf 20) (rel2par restatement) (text _!No employee or vendor would be involved._!) )
          )
        )
        ( Nucleus (span 21 27) (rel2par Statement-Response)
          ( Nucleus (span 21 25) (rel2par List)
            ( Nucleus (span 21 22) (rel2par span)
              ( Satellite (leaf 21) (rel2par attribution) (text _!A spokesman for Southern Co. would say_!) )
              ( Nucleus (leaf 22) (rel2par span) (text _!only that discussions are continuing between Gulf Power and federal prosecutors._!) )
            )
            ( Satellite (span 23 25) (rel2par elaboration-additional)
              ( Nucleus (span 23 24) (rel2par span)
                ( Nucleus (leaf 23) (rel2par span) (text _!"We have no further developments_!) )
                ( Satellite (leaf 24) (rel2par elaboration-object-attribute-e) (text _!to report,"_!) )
              )
              ( Satellite (leaf 25) (rel2par attribution) (text _!he said._!) )
            )
          )
          ( Nucleus (leaf 26) (rel2par List) (text _!Officials at Gulf Power couldn't be reached for comment._!) )
          ( Nucleus (leaf 27) (rel2par List) (text _!And prosecutors declined to comment._!) )
        )
      )
    )
    ( Satellite (span 28 34) (rel2par elaboration-additional)
      ( Satellite (leaf 28) (rel2par concession) (text _!While Southern Co. has been reluctant to discuss the grand jury investigations,_!) )
      ( Nucleus (span 29 34) (rel2par span)
        ( Nucleus (span 29 33) (rel2par span)
          ( Nucleus (span 29 31) (rel2par List)
            ( Satellite (leaf 29) (rel2par attribution) (text _!Edward L. Addison, chief executive officer, has said_!) )
            ( Nucleus (span 30 31) (rel2par span)
              ( Nucleus (leaf 30) (rel2par span) (text _!the company is prepared to defend its tax and acccounting practices_!) )
              ( Satellite (leaf 31) (rel2par condition) (text _!if any charges are brought against it._!) )
            )
          )
          ( Nucleus (span 32 33) (rel2par List)
            ( Satellite (leaf 32) (rel2par attribution) (text _!Morever, Mr. Addison has said_!) )
            ( Nucleus (leaf 33) (rel2par span) (text _!Southern Co. and its units don't condone illegal political contributions._!) )
          )
        )
        ( Satellite (leaf 34) (rel2par elaboration-additional) (text _!Neither Mr. Addison nor any other Southern Co. official has been charged with any wrongdoing in connection with the current inquiries._!) )
      )
    )
  )
  ( Nucleus (span 35 54) (rel2par Topic-Drift)
    ( Nucleus (leaf 35) (rel2par span) (text _!The probe of Southern Co. has attracted considerable attention this year_!) )
    ( Satellite (span 36 54) (rel2par explanation-argumentative)
      ( Nucleus (span 36 37) (rel2par span)
        ( Nucleus (leaf 36) (rel2par span) (text _!because of several events_!) )
        ( Satellite (leaf 37) (rel2par elaboration-object-attribute-e) (text _!that have befallen the company,_!) )
      )
      ( Satellite (span 38 54) (rel2par example-e)
        ( Nucleus (span 38 39) (rel2par span)
          ( Nucleus (leaf 38) (rel2par span) (text _!including the death of a Gulf Power executive in a plane crash and the disappearance of a company vendor_!) )
          ( Satellite (leaf 39) (rel2par elaboration-object-attribute-e) (text _!who was to be a key grand jury witness._!) )
        )
        ( Satellite (span 40 54) (rel2par elaboration-general-specific)
          ( Nucleus (span 40 48) (rel2par List)
            ( Nucleus (span 40 43) (rel2par span)
              ( Nucleus (span 40 42) (rel2par span)
                ( Satellite (leaf 40) (rel2par attribution) (text _!Witnesses have said_!) )
                ( Nucleus (span 41 42) (rel2par span)
                  ( Nucleus (leaf 41) (rel2par span) (text _!the grand jury has asked numerous questions about Jacob F. "Jake" Horton, the senior vice president of Gulf Power_!) )
                  ( Satellite (leaf 42) (rel2par elaboration-object-attribute-e) (text _!who died in the plane crash in April._!) )
                )
              )
              ( Satellite (leaf 43) (rel2par elaboration-additional) (text _!Mr. Horton oversaw Gulf Power's governmental-affairs efforts._!) )
            )
            ( Satellite (span 44 48) (rel2par elaboration-additional)
              ( Satellite (span 44 46) (rel2par circumstance)
                ( Nucleus (leaf 44) (rel2par span) (text _!On the morning of the crash, he had been put on notice_!) )
                ( Satellite (span 45 46) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 45) (rel2par span) (text _!that an audit committee was recommending his dismissal_!) )
                  ( Satellite (leaf 46) (rel2par consequence-n) (text _!because of invoicing irregularities in a company audit._!) )
                )
              )
              ( Nucleus (span 47 48) (rel2par span)
                ( Satellite (leaf 47) (rel2par attribution) (text _!Investigators have been trying to determine_!) )
                ( Nucleus (leaf 48) (rel2par span) (text _!whether the crash was an accident, sabotage or suicide._!) )
              )
            )
          )
          ( Nucleus (span 49 54) (rel2par List)
            ( Nucleus (span 49 52) (rel2par Contrast)
              ( Satellite (leaf 49) (rel2par attribution) (text _!Gulf Power said in May_!) )
              ( Nucleus (span 50 52) (rel2par span)
                ( Satellite (leaf 50) (rel2par attribution) (text _!that an internal audit had disclosed_!) )
                ( Nucleus (span 51 52) (rel2par span)
                  ( Nucleus (leaf 51) (rel2par span) (text _!that at least one vendor had used false invoices_!) )
                  ( Satellite (leaf 52) (rel2par purpose) (text _!to fund political causes._!) )
                )
              )
            )
            ( Nucleus (span 53 54) (rel2par Contrast)
              ( Satellite (leaf 53) (rel2par attribution) (text _!But the company said_!) )
              ( Nucleus (leaf 54) (rel2par span) (text _!the political contributions had been made more than five years ago._!) )
            )
          )
        )
      )
    )
  )
)
