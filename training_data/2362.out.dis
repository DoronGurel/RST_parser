                          )
                          ( Nucleus (span 42 43) (rel2par Same-Unit)
                            ( Nucleus (leaf 42) (rel2par span) (text _!included #215 million in fictitious contracts at a U.S. unit, International Signal & Control Group,_!) )
                            ( Satellite (leaf 43) (rel2par elaboration-object-attribute-e) (text _!with which Ferranti merged last year._!) )
                          )
                        )
                      )
                    )
                  )
                  ( Satellite (span 44 48) (rel2par elaboration-additional)
                    ( Nucleus (span 44 46) (rel2par Problem-Solution)
                      ( Nucleus (leaf 44) (rel2par span) (text _!Also, any GEC bid might be blocked by British antitrust regulators;_!) )
                      ( Satellite (span 45 46) (rel2par reason)
                        ( Satellite (leaf 45) (rel2par circumstance) (text _!Ferranti is GEC's main competitor on several key defense-electronics contracts,_!) )
                        ( Nucleus (leaf 46) (rel2par span) (text _!and its purchase by GEC may heighten British Defense Ministry worries about concentration in the country's defense industry._!) )
                      )
                    )
                    ( Nucleus (span 47 48) (rel2par Problem-Solution)
                      ( Nucleus (leaf 47) (rel2par span) (text _!A consortium bid, however, would diminish GEC's direct role in Ferranti_!) )
                      ( Satellite (leaf 48) (rel2par consequence-s) (text _!and might consequently appease ministry officials._!) )
                    )
                  )
                )
                ( Satellite (span 49 51) (rel2par elaboration-additional)
                  ( Nucleus (leaf 49) (rel2par span) (text _!A British Aerospace spokeswoman appeared unperturbed by the prospect of a fight with GEC for Ferranti:_!) )
                  ( Satellite (span 50 51) (rel2par elaboration-additional)
                    ( Nucleus (leaf 50) (rel2par span) (text _!"Competition is the name of the game,"_!) )
                    ( Satellite (leaf 51) (rel2par attribution) (text _!she said._!) )
                  )
                )
              )
              ( Satellite (span 52 56) (rel2par antithesis)
                ( Nucleus (span 52 53) (rel2par span)
