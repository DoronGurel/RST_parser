( Root (span 1 103)
  ( Nucleus (span 1 45) (rel2par span)
    ( Nucleus (span 1 31) (rel2par span)
      ( Nucleus (span 1 18) (rel2par span)
        ( Nucleus (span 1 10) (rel2par span)
          ( Nucleus (span 1 3) (rel2par span)
            ( Nucleus (span 1 2) (rel2par span)
              ( Satellite (leaf 1) (rel2par cause) (text _!This hasn't been Kellogg Co. 's year._!) )
              ( Nucleus (leaf 2) (rel2par span) (text _!The oat-bran craze has cost the world's largest cereal maker market share._!) )
            )
            ( Satellite (leaf 3) (rel2par consequence-s) (text _!The company's president quit suddenly._!) )
          )
          ( Satellite (span 4 10) (rel2par consequence-s)
            ( Nucleus (span 4 5) (rel2par span)
              ( Nucleus (leaf 4) (rel2par span) (text _!And now Kellogg is indefinitely suspending work_!) )
              ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!on what was to be a $1 billion cereal plant._!) )
            )
            ( Satellite (span 6 10) (rel2par explanation-argumentative)
              ( Nucleus (span 6 8) (rel2par span)
                ( Satellite (leaf 6) (rel2par attribution) (text _!The company said_!) )
                ( Nucleus (span 7 8) (rel2par span)
                  ( Nucleus (leaf 7) (rel2par span) (text _!it was delaying construction_!) )
                  ( Satellite (leaf 8) (rel2par reason) (text _!because of current market conditions._!) )
                )
              )
              ( Satellite (span 9 10) (rel2par antithesis)
                ( Nucleus (leaf 9) (rel2par span) (text _!But the Memphis, Tenn., facility wasn't to begin turning out product until 1993,_!) )
                ( Satellite (leaf 10) (rel2par interpretation-s) (text _!so the decision may reveal a more pessimistic long-term outlook as well._!) )
              )
            )
          )
        )
        ( Satellite (span 11 18) (rel2par elaboration-additional)
          ( Nucleus (span 11 13) (rel2par span)
            ( Nucleus (span 11 12) (rel2par Same-Unit)
              ( Nucleus (leaf 11) (rel2par span) (text _!Kellogg,_!) )
              ( Satellite (leaf 12) (rel2par elaboration-object-attribute-e) (text _!which hasn't been as successful in capitalizing on the public's health-oriented desire for oat bran as rival General Mills Inc.,_!) )
            )
            ( Nucleus (leaf 13) (rel2par Same-Unit) (text _!has been losing share in the $6 billion ready-to-eat cereal market._!) )
          )
          ( Satellite (span 14 18) (rel2par evidence)
            ( Satellite (span 14 15) (rel2par circumstance)
              ( Nucleus (leaf 14) (rel2par span) (text _!Kellogg's current share is believed to be slightly under 40%_!) )
              ( Satellite (leaf 15) (rel2par comparison) (text _!while General Mills' share is about 27%._!) )
            )
            ( Nucleus (span 16 18) (rel2par span)
              ( Nucleus (span 16 17) (rel2par span)
                ( Satellite (leaf 16) (rel2par circumstance) (text _!Led by its oat-based Cheerios line,_!) )
                ( Nucleus (leaf 17) (rel2par span) (text _!General Mills has gained an estimated 2% share so far this year, mostly at the expense of Kellogg._!) )
              )
              ( Satellite (leaf 18) (rel2par elaboration-additional) (text _!Each share point is worth about $60 million in sales._!) )
            )
          )
        )
      )
      ( Satellite (span 19 31) (rel2par elaboration-additional)
        ( Nucleus (span 19 21) (rel2par span)
          ( Satellite (leaf 19) (rel2par attribution) (text _!Analysts say_!) )
          ( Nucleus (span 20 21) (rel2par span)
            ( Nucleus (leaf 20) (rel2par span) (text _!much of Kellogg's erosion has been in such core brands as Corn Flakes, Rice Krispies and Frosted Flakes,_!) )
            ( Satellite (leaf 21) (rel2par elaboration-object-attribute-e) (text _!which represent nearly one-third of its sales volume._!) )
          )
        )
        ( Satellite (span 22 31) (rel2par consequence-s)
          ( Nucleus (span 22 28) (rel2par span)
            ( Nucleus (span 22 24) (rel2par span)
              ( Nucleus (span 22 23) (rel2par span)
                ( Satellite (leaf 22) (rel2par reason) (text _!Kellogg is so anxious to turn around Corn Flakes sales_!) )
                ( Nucleus (leaf 23) (rel2par span) (text _!that it soon will begin selling boxes for as little as 99 cents,_!) )
              )
              ( Satellite (leaf 24) (rel2par attribution) (text _!trade sources say._!) )
            )
            ( Satellite (span 25 28) (rel2par circumstance)
              ( Nucleus (span 25 27) (rel2par span)
                ( Nucleus (span 25 26) (rel2par span)
                  ( Nucleus (leaf 25) (rel2par span) (text _!"Cheerios and Honey Nut Cheerios have eaten away sales_!) )
                  ( Satellite (leaf 26) (rel2par elaboration-object-attribute-e) (text _!normally going to Kellogg's corn-based lines_!) )
                )
                ( Satellite (leaf 27) (rel2par explanation-argumentative) (text _!simply because they are made of oats,"_!) )
              )
              ( Satellite (leaf 28) (rel2par attribution) (text _!says Merrill Lynch food analyst William Maguire._!) )
            )
          )
          ( Satellite (span 29 31) (rel2par elaboration-additional)
            ( Nucleus (leaf 29) (rel2par span) (text _!"They are not a happy group of people at Battle Creek right now."_!) )
            ( Satellite (span 30 31) (rel2par elaboration-additional)
              ( Nucleus (leaf 30) (rel2par span) (text _!Kellogg is based in Battle Creek, Mich., a city_!) )
              ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!that calls itself the breakfast capital of the world._!) )
            )
          )
        )
      )
    )
    ( Satellite (span 32 45) (rel2par elaboration-additional)
      ( Nucleus (span 32 33) (rel2par span)
        ( Nucleus (leaf 32) (rel2par span) (text _!Another analyst, John C. Maxwell Jr. of Wheat, First Securities in Richmond, Va., recently went to a "sell" recommendation on Kellogg stock,_!) )
        ( Satellite (leaf 33) (rel2par elaboration-additional-e) (text _!which closed Friday at $71.75, down 75 cents, in New York Stock Exchange composite trading._!) )
      )
      ( Satellite (span 34 45) (rel2par elaboration-additional)
        ( Nucleus (span 34 42) (rel2par span)
          ( Nucleus (span 34 36) (rel2par span)
            ( Nucleus (span 34 35) (rel2par span)
              ( Satellite (leaf 34) (rel2par attribution-n) (text _!"I don't think_!) )
              ( Nucleus (leaf 35) (rel2par span) (text _!Kellogg can get back to 40% this year,"_!) )
            )
            ( Satellite (leaf 36) (rel2par attribution) (text _!he said._!) )
          )
          ( Satellite (span 37 42) (rel2par explanation-argumentative)
            ( Nucleus (leaf 37) (rel2par span) (text _!"Kellogg's main problem is life style._!) )
            ( Satellite (span 38 42) (rel2par elaboration-general-specific)
              ( Nucleus (leaf 38) (rel2par List) (text _!People are reading the boxes_!) )
              ( Nucleus (span 39 42) (rel2par List)
                ( Satellite (leaf 39) (rel2par attribution) (text _!and deciding_!) )
                ( Nucleus (span 40 42) (rel2par span)
                  ( Nucleus (leaf 40) (rel2par span) (text _!they want something_!) )
                  ( Satellite (span 41 42) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 41) (rel2par span) (text _!that's `healthy' for you_!) )
                    ( Satellite (leaf 42) (rel2par elaboration-additional-e) (text _!-- oats, bran."_!) )
                  )
                )
              )
            )
          )
        )
        ( Satellite (span 43 45) (rel2par elaboration-additional)
          ( Satellite (leaf 43) (rel2par attribution) (text _!Mr. Maxwell said_!) )
          ( Nucleus (span 44 45) (rel2par span)
            ( Nucleus (leaf 44) (rel2par span) (text _!he wouldn't be surprised_!) )
            ( Satellite (leaf 45) (rel2par condition) (text _!if, over the next two years or so, General Mills' share increased to 30% or more._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 46 103) (rel2par elaboration-additional)
