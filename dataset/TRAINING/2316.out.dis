        ( Nucleus (span 15 20) (rel2par span)
          ( Nucleus (span 15 17) (rel2par List)
            ( Nucleus (leaf 15) (rel2par span) (text _!Of the remaining 20% needed, the steel-quota negotiations allocate about 15% to foreign suppliers, with the difference_!) )
            ( Satellite (span 16 17) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 16) (rel2par span) (text _!supplied mainly by Canada_!) )
              ( Satellite (leaf 17) (rel2par elaboration-additional) (text _!-- which isn't included in the quota program._!) )
