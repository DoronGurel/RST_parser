          ( Nucleus (span 62 71) (rel2par span)
            ( Nucleus (span 62 64) (rel2par span)
              ( Nucleus (leaf 62) (rel2par span) (text _!The venture's importance for Thomson is great._!) )
              ( Satellite (span 63 64) (rel2par elaboration-additional)
                ( Satellite (leaf 63) (rel2par attribution) (text _!Thomson feels_!) )
                ( Nucleus (leaf 64) (rel2par span) (text _!the future of its defense business depends on building cooperation with other Europeans._!) )
