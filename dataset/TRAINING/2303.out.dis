                  ( Nucleus (span 12 15) (rel2par span)
                    ( Nucleus (span 12 14) (rel2par span)
                      ( Satellite (leaf 12) (rel2par antithesis) (text _!"The assets are good,_!) )
                      ( Nucleus (span 13 14) (rel2par span)
                        ( Nucleus (leaf 13) (rel2par Comparison) (text _!but they require more money and management"_!) )
                        ( Nucleus (leaf 14) (rel2par Comparison) (text _!than can be provided in L.J. Hooker's current situation,_!) )
