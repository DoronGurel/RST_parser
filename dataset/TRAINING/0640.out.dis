( Root (span 1 50)
  ( Nucleus (leaf 1) (rel2par span) (text _!Retailers in the West and parts of the South are entering the critical Christmas shopping season with more momentum than those in other regions._!) )
  ( Satellite (span 2 50) (rel2par summary-n)
    ( Nucleus (span 2 38) (rel2par span)
      ( Nucleus (span 2 34) (rel2par span)
        ( Nucleus (span 2 27) (rel2par span)
          ( Nucleus (span 2 23) (rel2par span)
            ( Nucleus (span 2 16) (rel2par span)
              ( Nucleus (span 2 13) (rel2par span)
                ( Nucleus (span 2 9) (rel2par span)
                  ( Nucleus (span 2 7) (rel2par Contrast)
                    ( Nucleus (span 2 3) (rel2par Comparison)
                      ( Satellite (leaf 2) (rel2par attribution) (text _!In a new report, the International Council of Shopping Centers said_!) )
                      ( Nucleus (leaf 3) (rel2par span) (text _!sales of general merchandise in the West for the first seven months of 1989 rose 6.6% above year-earlier levels._!) )
                    )
                    ( Nucleus (span 4 7) (rel2par Comparison)
                      ( Nucleus (leaf 4) (rel2par span) (text _!Sales increased a more modest 4.8% in the South and 4.4% in the Midwest._!) )
                      ( Satellite (span 5 7) (rel2par elaboration-general-specific)
                        ( Nucleus (span 5 6) (rel2par span)
                          ( Nucleus (leaf 5) (rel2par List) (text _!But sales in the oil-patch state of Texas surged 12.9%_!) )
                          ( Nucleus (leaf 6) (rel2par List) (text _!and sales in South Carolina jumped 10.6% in the period,_!) )
                        )
                        ( Satellite (leaf 7) (rel2par attribution) (text _!the New York trade group said._!) )
                      )
                    )
                  )
                  ( Nucleus (span 8 9) (rel2par Contrast)
                    ( Nucleus (leaf 8) (rel2par span) (text _!In the Northeast, however, sales declined 0.4% in the period,_!) )
                    ( Satellite (leaf 9) (rel2par elaboration-general-specific) (text _!with sales in New England falling 2.6%._!) )
                  )
                )
                ( Satellite (span 10 13) (rel2par elaboration-additional)
                  ( Nucleus (span 10 12) (rel2par span)
                    ( Nucleus (span 10 11) (rel2par span)
                      ( Satellite (leaf 10) (rel2par attribution) (text _!The numbers show_!) )
                      ( Nucleus (leaf 11) (rel2par span) (text _!that "we don't have a monolithic economy,"_!) )
                    )
                    ( Satellite (leaf 12) (rel2par attribution) (text _!said Isaac Lagnado, council research director._!) )
                  )
                  ( Satellite (leaf 13) (rel2par elaboration-additional) (text _!"There are a lot of have and have-not markets."_!) )
                )
              )
              ( Satellite (span 14 16) (rel2par elaboration-additional)
                ( Nucleus (span 14 15) (rel2par span)
                  ( Nucleus (leaf 14) (rel2par span) (text _!Sales nationally rose 3.9% through July, the latest month_!) )
                  ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!for which the figures are available,_!) )
                )
                ( Satellite (leaf 16) (rel2par attribution) (text _!the council said._!) )
              )
            )
            ( Satellite (span 17 23) (rel2par elaboration-additional)
              ( Nucleus (span 17 21) (rel2par span)
                ( Nucleus (span 17 19) (rel2par span)
                  ( Satellite (leaf 17) (rel2par antithesis) (text _!The Northern California earthquake and Hurricane Hugo are likely to temporarily damp sales growth in the West and South Carolina._!) )
                  ( Nucleus (span 18 19) (rel2par span)
                    ( Satellite (leaf 18) (rel2par attribution) (text _!But Mr. Lagnado predicted_!) )
                    ( Nucleus (leaf 19) (rel2par span) (text _!the regional trends would continue through Christmas._!) )
                  )
                )
                ( Satellite (span 20 21) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 20) (rel2par span) (text _!"The big mo is as much of a factor in retailing as in politics,"_!) )
                  ( Satellite (leaf 21) (rel2par attribution) (text _!he said._!) )
                )
              )
              ( Satellite (span 22 23) (rel2par elaboration-additional)
                ( Nucleus (leaf 22) (rel2par span) (text _!The Christmas quarter is important to retailers_!) )
                ( Satellite (leaf 23) (rel2par explanation-argumentative) (text _!because it represents roughly a third of their sales and nearly half of their profits._!) )
              )
            )
          )
          ( Satellite (span 24 27) (rel2par elaboration-additional)
            ( Nucleus (span 24 26) (rel2par span)
              ( Nucleus (span 24 25) (rel2par span)
                ( Nucleus (leaf 24) (rel2par span) (text _!The council's report is based on data_!) )
                ( Satellite (leaf 25) (rel2par elaboration-object-attribute-e) (text _!the trade group buys from the U.S. Census Bureau._!) )
              )
              ( Satellite (leaf 26) (rel2par elaboration-additional) (text _!The information on 125 metropolitan markets is supplied by retailers such as Sears, Roebuck & Co. and K mart Corp. as well as closely held concerns such as R.H. Macy & Co._!) )
            )
            ( Satellite (leaf 27) (rel2par elaboration-additional) (text _!The council plans to release its regional reports monthly._!) )
          )
        )
        ( Satellite (span 28 34) (rel2par elaboration-additional)
          ( Nucleus (span 28 29) (rel2par span)
            ( Satellite (leaf 28) (rel2par attribution) (text _!Mr. Lagnado said_!) )
            ( Nucleus (leaf 29) (rel2par span) (text _!strength in employment appears to have the biggest impact on sales growth._!) )
          )
          ( Satellite (span 30 34) (rel2par evidence)
            ( Nucleus (span 30 31) (rel2par Contrast)
              ( Nucleus (leaf 30) (rel2par span) (text _!El Paso, Austin and Fort Worth, the three strongest retail markets in the nation, are all located in Texas,_!) )
              ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!where employment grew a relatively strong 2%._!) )
            )
            ( Nucleus (span 32 34) (rel2par Contrast)
              ( Nucleus (span 32 33) (rel2par Same-Unit)
                ( Nucleus (leaf 32) (rel2par span) (text _!Massachusetts,_!) )
                ( Satellite (leaf 33) (rel2par elaboration-object-attribute-e) (text _!which has lost jobs in the computer and defense-related industries,_!) )
              )
              ( Nucleus (leaf 34) (rel2par Same-Unit) (text _!was the weakest link in bleak New England._!) )
            )
          )
        )
      )
      ( Satellite (span 35 38) (rel2par elaboration-additional)
        ( Nucleus (leaf 35) (rel2par span) (text _!The results reflect a reversal in the fortunes of the regions during the past two years._!) )
        ( Satellite (span 36 38) (rel2par example)
          ( Nucleus (span 36 37) (rel2par span)
            ( Nucleus (leaf 36) (rel2par Comparison) (text _!In 1987, the West had the slowest sales growth,_!) )
            ( Nucleus (leaf 37) (rel2par Comparison) (text _!and the South and the Midwest were first and second respectively,_!) )
          )
          ( Satellite (leaf 38) (rel2par attribution) (text _!according to the council._!) )
        )
      )
    )
    ( Satellite (span 39 50) (rel2par elaboration-additional)
      ( Satellite (leaf 39) (rel2par attribution) (text _!Mr. Lagnado said_!) )
      ( Nucleus (span 40 50) (rel2par span)
        ( Nucleus (span 40 44) (rel2par span)
          ( Nucleus (leaf 40) (rel2par Same-Unit) (text _!that_!) )
          ( Nucleus (span 41 44) (rel2par Same-Unit)
            ( Satellite (span 41 43) (rel2par antithesis-e)
              ( Nucleus (leaf 41) (rel2par span) (text _!although retailers probably won't ever recover sales_!) )
              ( Satellite (span 42 43) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 42) (rel2par span) (text _!lost_!) )
                ( Satellite (leaf 43) (rel2par consequence-n) (text _!because of the California quake and Hurricane Hugo,_!) )
              )
            )
            ( Nucleus (leaf 44) (rel2par span) (text _!they could see some benefits later on._!) )
          )
        )
        ( Satellite (span 45 50) (rel2par interpretation-s)
          ( Nucleus (span 45 49) (rel2par span)
            ( Nucleus (span 45 47) (rel2par span)
              ( Nucleus (span 45 46) (rel2par Same-Unit)
                ( Nucleus (leaf 45) (rel2par span) (text _!Stores such as Sears_!) )
                ( Satellite (leaf 46) (rel2par elaboration-object-attribute-e) (text _!that sell big-ticket durable goods_!) )
              )
              ( Nucleus (leaf 47) (rel2par Same-Unit) (text _!might actually get a boost_!) )
            )
            ( Satellite (span 48 49) (rel2par consequence-n)
              ( Nucleus (leaf 48) (rel2par span) (text _!as consumers rush to replace items_!) )
              ( Satellite (leaf 49) (rel2par elaboration-object-attribute-e) (text _!lost in the disasters,_!) )
            )
          )
          ( Satellite (leaf 50) (rel2par attribution) (text _!he said._!) )
        )
      )
    )
  )
)
