                          ( Nucleus (leaf 33) (rel2par span) (text _!Companies_!) )
                          ( Satellite (leaf 34) (rel2par elaboration-object-attribute-e) (text _!with which GEC has had talks about a possible joint Ferranti bid_!) )
                        )
                        ( Nucleus (leaf 35) (rel2par Same-Unit) (text _!include Matra, Britain's Dowty Group PLC, West Germany's Daimler-Benz AG, and France's Dassault group._!) )
                      )
                    )
                    ( Satellite (span 36 43) (rel2par elaboration-additional)
                      ( Nucleus (span 36 38) (rel2par span)
                        ( Nucleus (span 36 37) (rel2par span)
                          ( Satellite (leaf 36) (rel2par temporal-after) (text _!But it may be weeks_!) )
                          ( Nucleus (leaf 37) (rel2par span) (text _!before GEC and its potential partners decide whether to bid,_!) )
