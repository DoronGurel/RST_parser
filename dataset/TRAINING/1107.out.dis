    ( Nucleus (span 4 33) (rel2par List)
      ( Nucleus (span 4 31) (rel2par TextualOrganization)
        ( Nucleus (span 4 12) (rel2par Problem-Solution)
          ( Nucleus (span 4 5) (rel2par span)
            ( Satellite (leaf 4) (rel2par attribution) (text _!In Houston, we have seen_!) )
            ( Nucleus (leaf 5) (rel2par span) (text _!how bad the housing problem can become._!) )
          )
          ( Satellite (span 6 12) (rel2par evidence)
            ( Nucleus (span 6 8) (rel2par span)
              ( Nucleus (span 6 7) (rel2par span)
                ( Nucleus (leaf 6) (rel2par span) (text _!Unused houses deteriorate rapidly,_!) )
                ( Satellite (leaf 7) (rel2par consequence-s) (text _!affecting the value of nearby homes;_!) )
              )
              ( Satellite (leaf 8) (rel2par hypothetical) (text _!in a domino effect, the entire neighborhood can fall victim._!) )
            )
            ( Satellite (span 9 12) (rel2par elaboration-additional)
              ( Nucleus (span 9 10) (rel2par span)
                ( Nucleus (leaf 9) (rel2par span) (text _!At this stage some people just "walk away" from homes_!) )
                ( Satellite (leaf 10) (rel2par elaboration-object-attribute-e) (text _!where the mortgage exceeds current market value._!) )
              )
              ( Satellite (span 11 12) (rel2par elaboration-additional)
                ( Nucleus (leaf 11) (rel2par Contrast) (text _!But most of them could have afforded to keep up their payments_!) )
                ( Nucleus (leaf 12) (rel2par Contrast) (text _!-- they chose not to do so._!) )
              )
            )
          )
        )
        ( Nucleus (span 13 31) (rel2par Problem-Solution)
          ( Nucleus (span 13 15) (rel2par span)
            ( Satellite (leaf 13) (rel2par reason) (text _!The problem is so vast_!) )
            ( Nucleus (span 14 15) (rel2par span)
              ( Nucleus (leaf 14) (rel2par span) (text _!that we need to try innovative solutions_!) )
              ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!-- in small-scale experiments._!) )
            )
          )
          ( Satellite (span 16 31) (rel2par elaboration-additional)
            ( Nucleus (leaf 16) (rel2par span) (text _!Here are some ideas:_!) )
            ( Satellite (span 17 31) (rel2par elaboration-set-member)
              ( Nucleus (span 17 20) (rel2par List)
                ( Nucleus (span 17 18) (rel2par Same-Unit)
                  ( Nucleus (leaf 17) (rel2par span) (text _!1) Foreclosed homes could be sold by the FHA for no down payment_!) )
                  ( Satellite (leaf 18) (rel2par elaboration-additional-e) (text _!(the biggest obstacle to young buyers),_!) )
                )
                ( Nucleus (span 19 20) (rel2par Same-Unit)
                  ( Nucleus (leaf 19) (rel2par span) (text _!but with personal liability for the mortgage_!) )
                  ( Satellite (leaf 20) (rel2par restatement-e) (text _!(no walking away by choice)._!) )
                )
              )
              ( Nucleus (span 21 28) (rel2par List)
                ( Nucleus (leaf 21) (rel2par span) (text _!2) Encourage long-term occupancy_!) )
                ( Satellite (span 22 28) (rel2par means)
                  ( Nucleus (span 22 27) (rel2par span)
                    ( Nucleus (span 22 24) (rel2par List)
                      ( Nucleus (span 22 23) (rel2par Same-Unit)
                        ( Nucleus (leaf 22) (rel2par span) (text _!by forgiving one month's payment_!) )
                        ( Satellite (leaf 23) (rel2par elaboration-additional-e) (text _!(off the tail end of the mortgage)_!) )
                      )
                      ( Nucleus (leaf 24) (rel2par Same-Unit) (text _!for every six months paid;_!) )
                    )
                    ( Nucleus (span 25 27) (rel2par List)
                      ( Nucleus (span 25 26) (rel2par List)
                        ( Nucleus (leaf 25) (rel2par span) (text _!or perhaps have the down payment deferred to the end of the mortgage_!) )
                        ( Satellite (leaf 26) (rel2par elaboration-additional-e) (text _!(balloon),_!) )
                      )
                      ( Nucleus (leaf 27) (rel2par List) (text _!but "forgiven" on a monthly pro-rata basis_!) )
                    )
                  )
                  ( Satellite (leaf 28) (rel2par condition) (text _!as long as the owner remains the occupant._!) )
                )
              )
              ( Nucleus (span 29 31) (rel2par List)
                ( Nucleus (span 29 30) (rel2par span)
                  ( Nucleus (leaf 29) (rel2par span) (text _!3) Develop rental agreements with exclusive purchase options for the renter._!) )
                  ( Satellite (leaf 30) (rel2par evaluation-s) (text _!An occupant will, in most every case, be better for the home and neighborhood than a vacant house._!) )
                )
                ( Satellite (leaf 31) (rel2par consequence-s) (text _!In this way, the house is not dumped on to a glutted market._!) )
              )
            )
          )
        )
      )
      ( Nucleus (span 32 33) (rel2par TextualOrganization)
        ( Nucleus (leaf 32) (rel2par List) (text _!John F. Merrill_!) )
        ( Nucleus (leaf 33) (rel2par List) (text _!Houston_!) )
      )
    )
    ( Nucleus (span 34 51) (rel2par List)
      ( Nucleus (span 34 49) (rel2par TextualOrganization)
        ( Nucleus (span 34 40) (rel2par span)
          ( Nucleus (leaf 34) (rel2par span) (text _!The Federal Housing Administration, Veterans Administration and the Department of Housing and Urban Development further aggravate the problem of affordable housing stock_!) )
