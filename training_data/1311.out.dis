        ( Satellite (span 4 5) (rel2par elaboration-additional)
          ( Nucleus (leaf 4) (rel2par span) (text _!All cleanup costs from last spring's Alaskan oil spill were reflected in earlier results,_!) )
          ( Satellite (leaf 5) (rel2par attribution) (text _!it said._!) )
        )
      )
      ( Nucleus (leaf 6) (rel2par List) (text _!Phillips Petroleum Co. and Atlantic Richfield Co. also reported declines in quarterly profit,_!) )
      ( Nucleus (leaf 7) (rel2par List) (text _!while Ashland Oil Inc. posted a loss for the latest quarter._!) )
      ( Nucleus (leaf 8) (rel2par List) (text _!Amerada Hess Corp. and Occidental Petroleum Corp. reported higher earnings._!) )
