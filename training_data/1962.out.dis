( Root (span 1 123)
  ( Nucleus (span 1 120) (rel2par Topic-Shift)
    ( Nucleus (span 1 98) (rel2par span)
      ( Nucleus (span 1 69) (rel2par span)
        ( Nucleus (span 1 4) (rel2par span)
          ( Nucleus (span 1 2) (rel2par span)
            ( Satellite (leaf 1) (rel2par attribution) (text _!The stock market's dizzying gyrations during the past few days have made a lot of individual investors wish_!) )
            ( Nucleus (leaf 2) (rel2par span) (text _!they could buy some sort of insurance._!) )
          )
          ( Satellite (span 3 4) (rel2par explanation-argumentative)
            ( Nucleus (leaf 3) (rel2par span) (text _!After all, they won't soon forget the stock bargains_!) )
            ( Satellite (leaf 4) (rel2par elaboration-object-attribute-e) (text _!that became available after the October 1987 crash._!) )
          )
        )
        ( Satellite (span 5 69) (rel2par elaboration-additional)
          ( Nucleus (span 5 28) (rel2par span)
            ( Nucleus (span 5 18) (rel2par span)
              ( Nucleus (span 5 13) (rel2par span)
                ( Satellite (span 5 7) (rel2par problem-solution-n)
                  ( Nucleus (span 5 6) (rel2par Same-Unit)
                    ( Nucleus (leaf 5) (rel2par span) (text _!But_!) )
                    ( Satellite (leaf 6) (rel2par antithesis-e) (text _!while they want to be on the alert for similar buying opportunities now,_!) )
                  )
                  ( Nucleus (leaf 7) (rel2par Same-Unit) (text _!they're afraid of being hammered by another terrifying plunge._!) )
                )
                ( Nucleus (span 8 13) (rel2par span)
                  ( Nucleus (span 8 9) (rel2par span)
                    ( Nucleus (leaf 8) (rel2par span) (text _!The solution, at least for some investors, may be a hedging technique_!) )
                    ( Satellite (leaf 9) (rel2par elaboration-object-attribute-e) (text _!that's well known to players in the stock-options market._!) )
                  )
                  ( Satellite (span 10 13) (rel2par elaboration-general-specific)
                    ( Satellite (leaf 10) (rel2par circumstance) (text _!Called a "married put,"_!) )
                    ( Nucleus (span 11 13) (rel2par span)
                      ( Nucleus (leaf 11) (rel2par span) (text _!the technique is carried out_!) )
                      ( Satellite (span 12 13) (rel2par means)
                        ( Nucleus (leaf 12) (rel2par Temporal-Same-Time) (text _!by purchasing a stock_!) )
                        ( Nucleus (leaf 13) (rel2par Temporal-Same-Time) (text _!and simultaneously buying a put option on that stock._!) )
                      )
                    )
                  )
                )
              )
              ( Satellite (span 14 18) (rel2par analogy)
                ( Nucleus (span 14 15) (rel2par span)
                  ( Nucleus (leaf 14) (rel2par span) (text _!It's like "fire insurance,"_!) )
                  ( Satellite (leaf 15) (rel2par attribution) (text _!says Harrison Roth, the senior options strategist at Cowen & Co._!) )
                )
                ( Satellite (span 16 18) (rel2par elaboration-additional)
                  ( Satellite (span 16 17) (rel2par reason)
                    ( Nucleus (leaf 16) (rel2par span) (text _!Because a put option gives its owner the right, but not the obligation,_!) )
                    ( Satellite (leaf 17) (rel2par elaboration-object-attribute-e) (text _!to sell a fixed number of shares of the stock at a stated price on or before the option's expiration date,_!) )
                  )
                  ( Nucleus (leaf 18) (rel2par span) (text _!the investor is protected against a sudden drop in the stock's price._!) )
                )
              )
            )
            ( Satellite (span 19 28) (rel2par antithesis)
              ( Nucleus (leaf 19) (rel2par span) (text _!But most investment advisers don't recommend using married puts all the time._!) )
              ( Satellite (span 20 28) (rel2par explanation-argumentative)
                ( Nucleus (span 20 21) (rel2par span)
                  ( Nucleus (leaf 20) (rel2par span) (text _!That's because the cost of buying put options eats into an investor's profit_!) )
                  ( Satellite (leaf 21) (rel2par circumstance) (text _!when stock prices rise._!) )
                )
                ( Satellite (span 22 28) (rel2par elaboration-additional)
                  ( Nucleus (span 22 25) (rel2par span)
                    ( Nucleus (span 22 24) (rel2par span)
                      ( Nucleus (leaf 22) (rel2par span) (text _!"This is the type of fire insurance_!) )
                      ( Satellite (span 23 24) (rel2par elaboration-object-attribute-e)
                        ( Nucleus (leaf 23) (rel2par span) (text _!you only buy_!) )
                        ( Satellite (leaf 24) (rel2par circumstance) (text _!when the nearby woods are on fire,"_!) )
                      )
                    )
                    ( Satellite (leaf 25) (rel2par attribution) (text _!says Mr. Roth._!) )
                  )
                  ( Satellite (span 26 28) (rel2par interpretation-s)
                    ( Satellite (leaf 26) (rel2par antithesis) (text _!"You always want your house insured,_!) )
                    ( Nucleus (span 27 28) (rel2par span)
                      ( Nucleus (leaf 27) (rel2par span) (text _!but you don't always feel the need_!) )
                      ( Satellite (leaf 28) (rel2par elaboration-object-attribute-e) (text _!for your investments to be insured."_!) )
                    )
                  )
                )
              )
            )
          )
          ( Satellite (span 29 69) (rel2par elaboration-additional)
            ( Nucleus (span 29 31) (rel2par span)
              ( Nucleus (leaf 29) (rel2par span) (text _!In addition to hedging new stock purchases,_!) )
              ( Satellite (span 30 31) (rel2par elaboration-additional)
                ( Nucleus (leaf 30) (rel2par span) (text _!the married-put technique can be used to protect stocks_!) )
                ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!that an investor already owns._!) )
              )
            )
            ( Satellite (span 32 69) (rel2par elaboration-additional)
              ( Nucleus (leaf 32) (rel2par span) (text _!In either case, the investor faces three possible outcomes:_!) )
              ( Satellite (span 33 69) (rel2par elaboration-set-member)
                ( Nucleus (span 33 37) (rel2par List)
                  ( Nucleus (span 33 34) (rel2par span)
                    ( Satellite (leaf 33) (rel2par condition) (text _!-- If the stock goes up in price between now and the put's expiration date,_!) )
                    ( Nucleus (leaf 34) (rel2par span) (text _!the put will probably expire worthless._!) )
                  )
                  ( Satellite (span 35 37) (rel2par elaboration-additional)
                    ( Nucleus (span 35 36) (rel2par span)
                      ( Nucleus (leaf 35) (rel2par span) (text _!The investor will be out the cost of the put,_!) )
                      ( Satellite (leaf 36) (rel2par elaboration-additional-e) (text _!which is called the "premium,"_!) )
                    )
                    ( Satellite (leaf 37) (rel2par consequence-s) (text _!and this loss will reduce the stock-market profit._!) )
                  )
                )
                ( Nucleus (span 38 41) (rel2par List)
                  ( Nucleus (span 38 40) (rel2par span)
                    ( Satellite (leaf 38) (rel2par condition) (text _!-- If the stock stays at the same price between now and the put's expiration date,_!) )
                    ( Nucleus (span 39 40) (rel2par span)
                      ( Nucleus (leaf 39) (rel2par span) (text _!the investor's loss will be limited to the cost of the put, less any amount_!) )
                      ( Satellite (leaf 40) (rel2par elaboration-object-attribute-e) (text _!realized from a closing sale of the put._!) )
                    )
                  )
                  ( Satellite (leaf 41) (rel2par elaboration-additional) (text _!The worst-case scenario would be if the put expires worthless._!) )
                )
                ( Nucleus (span 42 69) (rel2par List)
                  ( Nucleus (span 42 45) (rel2par span)
                    ( Nucleus (span 42 43) (rel2par span)
                      ( Satellite (leaf 42) (rel2par condition) (text _!-- If the price of the stock declines,_!) )
                      ( Nucleus (leaf 43) (rel2par span) (text _!the put will increase in value._!) )
                    )
                    ( Satellite (span 44 45) (rel2par elaboration-additional)
                      ( Satellite (leaf 44) (rel2par circumstance) (text _!Once the stock price is less than the exercise price, or "strike price," of the put,_!) )
                      ( Nucleus (leaf 45) (rel2par span) (text _!the gain will match the loss on the stock dollar for dollar._!) )
                    )
                  )
                  ( Satellite (span 46 69) (rel2par elaboration-additional)
                    ( Nucleus (leaf 46) (rel2par span) (text _!The put establishes a minimum selling price for the stock during its life._!) )
                    ( Satellite (span 47 69) (rel2par elaboration-additional)
                      ( Nucleus (span 47 54) (rel2par span)
                        ( Nucleus (span 47 49) (rel2par Disjunction)
                          ( Satellite (leaf 47) (rel2par circumstance) (text _!When a stock falls below the put's strike price,_!) )
                          ( Nucleus (span 48 49) (rel2par span)
                            ( Nucleus (leaf 48) (rel2par List) (text _!the investor simply sells the stock at a loss_!) )
                            ( Nucleus (leaf 49) (rel2par List) (text _!and simultaneously sells the put at a profit._!) )
                          )
                        )
                        ( Nucleus (span 50 54) (rel2par Disjunction)
                          ( Nucleus (span 50 52) (rel2par span)
                            ( Nucleus (leaf 50) (rel2par span) (text _!Or, the investor can exercise the put,_!) )
                            ( Satellite (span 51 52) (rel2par means)
                              ( Nucleus (leaf 51) (rel2par span) (text _!by tendering the stock to his or her broker in return for payment from another investor_!) )
                              ( Satellite (leaf 52) (rel2par elaboration-object-attribute-e) (text _!who has sold a put on the same stock._!) )
                            )
                          )
                          ( Satellite (span 53 54) (rel2par elaboration-additional)
                            ( Nucleus (leaf 53) (rel2par span) (text _!Brokers handle such transactions through the Options Clearing Corp.,_!) )
                            ( Satellite (leaf 54) (rel2par elaboration-additional-e) (text _!which guarantees all option trades._!) )
                          )
                        )
                      )
                      ( Satellite (span 55 69) (rel2par elaboration-additional)
                        ( Nucleus (span 55 67) (rel2par span)
                          ( Nucleus (span 55 56) (rel2par span)
                            ( Satellite (leaf 55) (rel2par attribution) (text _!The accompanying table shows_!) )
                            ( Nucleus (leaf 56) (rel2par span) (text _!how this strategy would work for three stocks._!) )
                          )
                          ( Satellite (span 57 67) (rel2par elaboration-additional)
                            ( Nucleus (span 57 60) (rel2par span)
                              ( Satellite (leaf 57) (rel2par concession) (text _!Though not reflected in the table,_!) )
                              ( Nucleus (span 58 60) (rel2par span)
                                ( Satellite (leaf 58) (rel2par attribution) (text _!an investor should know_!) )
                                ( Nucleus (span 59 60) (rel2par span)
