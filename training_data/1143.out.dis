( Root (span 1 84)
  ( Nucleus (span 1 46) (rel2par span)
    ( Nucleus (span 1 39) (rel2par span)
      ( Satellite (span 1 36) (rel2par problem-solution-n)
        ( Nucleus (span 1 34) (rel2par span)
          ( Satellite (span 1 33) (rel2par concession)
            ( Satellite (span 1 26) (rel2par circumstance)
              ( Satellite (span 1 21) (rel2par circumstance)
                ( Nucleus (span 1 4) (rel2par List)
                  ( Nucleus (leaf 1) (rel2par TextualOrganization) (text _!Probably the most clear-cut Soviet violation, for example, is the Krasnoyarsk radar._!) )
                  ( Nucleus (span 2 4) (rel2par TextualOrganization)
                    ( Nucleus (leaf 2) (rel2par span) (text _!-- "Arms Control Reality," Nov. 20, 1984, the first of some 20 Journal editorials_!) )
                    ( Satellite (span 3 4) (rel2par elaboration-object-attribute-e)
                      ( Satellite (leaf 3) (rel2par attribution) (text _!saying_!) )
                      ( Nucleus (leaf 4) (rel2par span) (text _!that Krasnoyarsk violated the ABM treaty._!) )
                    )
                  )
                )
                ( Nucleus (span 5 10) (rel2par List)
                  ( Nucleus (span 5 9) (rel2par TextualOrganization)
                    ( Nucleus (span 5 8) (rel2par span)
                      ( Satellite (span 5 7) (rel2par interpretation-n)
                        ( Nucleus (span 5 6) (rel2par span)
                          ( Satellite (leaf 5) (rel2par circumstance) (text _!-- "Whether the installation is for early warning or space track,_!) )
                          ( Nucleus (leaf 6) (rel2par span) (text _!it clearly is not deployed,"_!) )
                        )
                        ( Satellite (leaf 7) (rel2par attribution) (text _!the lawmakers said._!) )
                      )
                      ( Nucleus (leaf 8) (rel2par span) (text _!"Thus we judge it to be not a violation of the ABM treaty at this time."_!) )
                    )
                    ( Satellite (leaf 9) (rel2par elaboration-additional) (text _!The delegation included a reporter from the New York Times, aides to Sen. Edward M. Kennedy and Rep. Les AuCoin, and Natural Resources Defense Council staff members._!) )
                  )
                  ( Nucleus (leaf 10) (rel2par TextualOrganization) (text _!-- The Washington Post, Sept. 9, 1987._!) )
                )
                ( Nucleus (span 11 17) (rel2par List)
                  ( Nucleus (span 11 15) (rel2par TextualOrganization)
                    ( Nucleus (leaf 11) (rel2par span) (text _!-- The U.S.S.R. has taken unprecedented unilateral measures of openness,_!) )
                    ( Satellite (span 12 15) (rel2par means)
                      ( Nucleus (leaf 12) (rel2par span) (text _!by giving American representatives a possibility_!) )
                      ( Satellite (span 13 15) (rel2par elaboration-object-attribute-e)
                        ( Nucleus (leaf 13) (rel2par span) (text _!to inspect the building site of the Krasnoyarsk radar as well as radar vans in the areas of Gomel and Moscow,_!) )
                        ( Satellite (span 14 15) (rel2par purpose)
                          ( Satellite (leaf 14) (rel2par attribution) (text _!so as to see for themselves_!) )
                          ( Nucleus (leaf 15) (rel2par span) (text _!that there are no violations of the ABM treaty of 1972 on the part of the Soviet Union._!) )
                        )
                      )
                    )
                  )
                  ( Nucleus (span 16 17) (rel2par TextualOrganization)
                    ( Nucleus (leaf 16) (rel2par span) (text _!-- Letter from Eduard Shevardnadze to U.N. Secretary-General Perez de Cuellar,_!) )
                    ( Satellite (leaf 17) (rel2par elaboration-additional-e) (text _!reported in Tass, June 10, 1988._!) )
                  )
                )
                ( Nucleus (span 18 21) (rel2par List)
                  ( Nucleus (span 18 20) (rel2par TextualOrganization)
                    ( Nucleus (span 18 19) (rel2par Same-Unit)
                      ( Nucleus (leaf 18) (rel2par span) (text _!-- The construction of this station equal in size to the Egyptian pyramids constituted,_!) )
                      ( Satellite (leaf 19) (rel2par attribution-e) (text _!I say it directly,_!) )
                    )
                    ( Nucleus (leaf 20) (rel2par Same-Unit) (text _!a clear violation of ABM._!) )
                  )
                  ( Nucleus (leaf 21) (rel2par TextualOrganization) (text _!-- Eduard Shevardnadze, Oct. 23, 1989._!) )
                )
              )
              ( Nucleus (span 22 26) (rel2par span)
                ( Nucleus (span 22 25) (rel2par span)
                  ( Nucleus (span 22 23) (rel2par Same-Unit)
                    ( Nucleus (leaf 22) (rel2par span) (text _!We're happy,_!) )
                    ( Satellite (leaf 23) (rel2par attribution-e) (text _!we guess,_!) )
                  )
                  ( Nucleus (span 24 25) (rel2par Same-Unit)
                    ( Nucleus (leaf 24) (rel2par span) (text _!to receive confirmation of the Krasnoyarsk violation from the Soviets,_!) )
                    ( Satellite (leaf 25) (rel2par temporal-after) (text _!five years after we started writing about it._!) )
                  )
                )
                ( Satellite (leaf 26) (rel2par hypothetical) (text _!Perhaps even the American apologists will now accede._!) )
              )
            )
            ( Nucleus (span 27 33) (rel2par span)
              ( Nucleus (span 27 28) (rel2par span)
                ( Satellite (leaf 27) (rel2par comment) (text _!Without question,_!) )
                ( Nucleus (leaf 28) (rel2par span) (text _!something intriguing is going on in the policy chambers of the Politburo._!) )
              )
              ( Satellite (span 29 33) (rel2par elaboration-additional)
                ( Satellite (span 29 31) (rel2par explanation-argumentative)
                  ( Satellite (leaf 29) (rel2par circumstance) (text _!As it bids for new agreements, new loans and indeed admission to the civilized world,_!) )
                  ( Nucleus (span 30 31) (rel2par span)
                    ( Satellite (leaf 30) (rel2par attribution) (text _!the Soviet government has recognized_!) )
                    ( Nucleus (leaf 31) (rel2par span) (text _!it has a credibility problem._!) )
                  )
                )
                ( Nucleus (span 32 33) (rel2par span)
                  ( Nucleus (leaf 32) (rel2par span) (text _!So after 70 years, it is confessing the obvious,_!) )
                  ( Satellite (leaf 33) (rel2par elaboration-additional) (text _!hoping to be believed about other things._!) )
                )
              )
            )
          )
          ( Nucleus (leaf 34) (rel2par span) (text _!It's not enough._!) )
        )
        ( Satellite (span 35 36) (rel2par explanation-argumentative)
          ( Satellite (leaf 35) (rel2par condition) (text _!If the Soviets want to be believed,_!) )
          ( Nucleus (leaf 36) (rel2par span) (text _!they need to start telling the truth about more than the totally obvious._!) )
        )
      )
      ( Nucleus (span 37 39) (rel2par span)
        ( Nucleus (leaf 37) (rel2par span) (text _!Our own test of "glasnost's" authenticity would be a Soviet decision_!) )
        ( Satellite (span 38 39) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 38) (rel2par span) (text _!to open itself to a complete international examination of one of the most troubling mysteries in U.S.-Soviet relations_!) )
          ( Satellite (leaf 39) (rel2par elaboration-object-attribute-e) (text _!-- the reported 1979 anthrax outbreak at a Soviet military facility in Sverdlovsk._!) )
        )
      )
    )
    ( Satellite (span 40 46) (rel2par background)
      ( Nucleus (leaf 40) (rel2par span) (text _!The U.S. government has never waivered in its assessment of this incident as an accident at a biological weapons facility there, and hence a violation of the 1972 Biological Weapons Convention._!) )
      ( Satellite (span 41 46) (rel2par evidence)
        ( Nucleus (span 41 43) (rel2par List)
          ( Nucleus (leaf 41) (rel2par Same-Unit) (text _!The Pentagon's recently issued "Soviet Military Power,"_!) )
          ( Nucleus (span 42 43) (rel2par Same-Unit)
            ( Satellite (leaf 42) (rel2par antithesis-e) (text _!though in general adopting a softer line,_!) )
            ( Nucleus (leaf 43) (rel2par span) (text _!repeated the Sverdlovsk assessment._!) )
          )
        )
        ( Nucleus (span 44 46) (rel2par List)
          ( Satellite (leaf 44) (rel2par attribution) (text _!It also was detailed in Congressional testimony this past February:_!) )
          ( Nucleus (span 45 46) (rel2par span)
            ( Nucleus (leaf 45) (rel2par span) (text _!An explosion at the Microbiology and Virology Institute in Sverdlovsk released anthrax germs_!) )
            ( Satellite (leaf 46) (rel2par elaboration-object-attribute-e) (text _!that caused a significant number of deaths._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 47 84) (rel2par elaboration-additional)
    ( Satellite (span 47 71) (rel2par circumstance)
      ( Satellite (span 47 70) (rel2par interpretation-n)
        ( Nucleus (span 47 56) (rel2par span)
          ( Satellite (span 47 49) (rel2par antithesis)
            ( Satellite (leaf 47) (rel2par conclusion) (text _!Since Mr. Shevardnadze did not address this topic before the Supreme Soviet,_!) )
            ( Nucleus (span 48 49) (rel2par span)
              ( Satellite (leaf 48) (rel2par attribution) (text _!the Soviet Union's official position remains_!) )
              ( Nucleus (leaf 49) (rel2par span) (text _!that the anthrax deaths were caused by tainted meat._!) )
            )
          )
          ( Nucleus (span 50 56) (rel2par span)
            ( Nucleus (span 50 52) (rel2par span)
              ( Nucleus (leaf 50) (rel2par span) (text _!We doubt this claim_!) )
              ( Satellite (span 51 52) (rel2par analogy)
                ( Nucleus (leaf 51) (rel2par span) (text _!just as we doubted Mr. Shevardnadze's assurance last year_!) )
                ( Satellite (leaf 52) (rel2par elaboration-object-attribute-e) (text _!that Krasnoyarsk didn't violate the ABM treaty._!) )
              )
            )
            ( Satellite (span 53 56) (rel2par elaboration-additional)
              ( Satellite (span 53 54) (rel2par analogy)
                ( Nucleus (leaf 53) (rel2par span) (text _!And just as we did not believe the tendentious claims of the Congressmen and arms-control advocates_!) )
                ( Satellite (leaf 54) (rel2par elaboration-object-attribute-e) (text _!who visited Krasnoyarsk,_!) )
              )
              ( Nucleus (span 55 56) (rel2par span)
                ( Nucleus (leaf 55) (rel2par span) (text _!we are in no way persuaded by the assent to the tainted-meat theory by a U.S. team of scientists_!) )
                ( Satellite (leaf 56) (rel2par elaboration-object-attribute-e) (text _!who met with Soviet counterparts in Washington last year._!) )
              )
            )
          )
        )
        ( Satellite (span 57 70) (rel2par elaboration-additional)
          ( Satellite (span 57 67) (rel2par antithesis)
            ( Nucleus (span 57 60) (rel2par span)
              ( Nucleus (leaf 57) (rel2par span) (text _!The Soviets' explanation is that the anthrax came from one lot of animal feed_!) )
              ( Satellite (span 58 60) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 58) (rel2par span) (text _!made from the bones of cattle_!) )
                ( Satellite (span 59 60) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 59) (rel2par span) (text _!that grazed on soil_!) )
                  ( Satellite (leaf 60) (rel2par elaboration-object-attribute-e) (text _!that was naturally infected with anthrax spores._!) )
                )
              )
            )
            ( Satellite (span 61 67) (rel2par elaboration-additional)
              ( Nucleus (span 61 66) (rel2par Same-Unit)
                ( Nucleus (leaf 61) (rel2par span) (text _!Harvard's Matthew Meselson_!) )
                ( Satellite (span 62 66) (rel2par elaboration-additional-e)
                  ( Nucleus (span 62 63) (rel2par Same-Unit)
                    ( Nucleus (leaf 62) (rel2par span) (text _!-- who_!) )
                    ( Satellite (leaf 63) (rel2par attribution-e) (text _!we read_!) )
                  )
                  ( Nucleus (span 64 66) (rel2par Same-Unit)
                    ( Nucleus (leaf 64) (rel2par span) (text _!has sold something_!) )
                    ( Satellite (span 65 66) (rel2par elaboration-object-attribute-e)
                      ( Nucleus (leaf 65) (rel2par span) (text _!called the "scientific community" on the notion_!) )
                      ( Satellite (leaf 66) (rel2par elaboration-object-attribute-e) (text _!that "yellow rain" attacks on the Laotian Hmong were in fact the result of fecal showers by giant bees --_!) )
                    )
                  )
                )
              )
              ( Nucleus (leaf 67) (rel2par Same-Unit) (text _!found the Soviet anthrax scenario "completely plausible."_!) )
            )
          )
          ( Nucleus (span 68 70) (rel2par span)
            ( Nucleus (leaf 68) (rel2par span) (text _!We don't believe it._!) )
            ( Satellite (span 69 70) (rel2par elaboration-additional)
              ( Satellite (leaf 69) (rel2par attribution-n) (text _!And we certainly do not believe_!) )
              ( Nucleus (leaf 70) (rel2par span) (text _!that Mr. Gorbachev or any of his emissaries yet deserve to have the West take their word for it._!) )
            )
          )
        )
      )
      ( Nucleus (leaf 71) (rel2par span) (text _!Sverdlovsk is a large gray cloud over glasnost and indeed over the legitimacy of the arms-control process itself._!) )
    )
    ( Nucleus (span 72 84) (rel2par span)
      ( Nucleus (span 72 76) (rel2par Contrast)
        ( Nucleus (span 72 74) (rel2par span)
          ( Nucleus (leaf 72) (rel2par span) (text _!The U.S. government's Sverdlovsk complaint, as with Krasnoyarsk, is no mere political posturing._!) )
          ( Satellite (span 73 74) (rel2par background)
            ( Nucleus (leaf 73) (rel2par List) (text _!Biological weapons violations have figured little in political debate,_!) )
            ( Nucleus (leaf 74) (rel2par List) (text _!and indeed have not been pressed vigorously enough by the U.S. government._!) )
          )
        )
        ( Satellite (span 75 76) (rel2par elaboration-additional)
          ( Nucleus (leaf 75) (rel2par List) (text _!But the stated U.S. position is detailed and specific,_!) )
          ( Nucleus (leaf 76) (rel2par List) (text _!and the prospect of biological warfare is profoundly chilling._!) )
        )
      )
      ( Nucleus (span 77 84) (rel2par Contrast)
        ( Nucleus (span 77 81) (rel2par span)
          ( Nucleus (leaf 77) (rel2par span) (text _!The Soviets should be willing to set in motion a process_!) )
          ( Satellite (span 78 81) (rel2par elaboration-object-attribute-e)
            ( Nucleus (span 78 79) (rel2par Disjunction)
              ( Satellite (leaf 78) (rel2par attribution) (text _!that would allow them to acknowledge_!) )
              ( Nucleus (leaf 79) (rel2par span) (text _!that Sverdlovsk violated the 1972 agreement_!) )
            )
            ( Nucleus (span 80 81) (rel2par Disjunction)
              ( Nucleus (leaf 80) (rel2par span) (text _!or, alternatively, that would give U.S. specialists reasonable confidence_!) )
              ( Satellite (leaf 81) (rel2par elaboration-object-attribute-e) (text _!that this was a wholly civilian accident._!) )
            )
          )
        )
        ( Satellite (span 82 84) (rel2par evaluation-s)
          ( Satellite (leaf 82) (rel2par condition) (text _!Until that happens,_!) )
          ( Nucleus (span 83 84) (rel2par span)
            ( Nucleus (leaf 83) (rel2par span) (text _!glasnost cannot begin to deserve the kind of credibility_!) )
            ( Satellite (leaf 84) (rel2par elaboration-object-attribute-e) (text _!Mr. Shevardnadze was bidding for with his confessions on Monday._!) )
          )
        )
      )
    )
  )
