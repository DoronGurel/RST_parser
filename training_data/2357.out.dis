( Root (span 1 29)
  ( Nucleus (span 1 25) (rel2par span)
    ( Nucleus (span 1 13) (rel2par span)
      ( Nucleus (span 1 3) (rel2par span)
        ( Satellite (leaf 1) (rel2par attribution) (text _!A group of Arby's franchisees said_!) )
        ( Nucleus (span 2 3) (rel2par span)
          ( Nucleus (leaf 2) (rel2par span) (text _!they formed an association_!) )
          ( Satellite (leaf 3) (rel2par purpose) (text _!to oppose Miami Beach financier Victor Posner's control of the restaurant chain._!) )
        )
      )
      ( Satellite (span 4 13) (rel2par elaboration-additional)
        ( Nucleus (span 4 5) (rel2par span)
          ( Nucleus (leaf 4) (rel2par span) (text _!The decision is the latest move in an escalating battle between the franchisees and Mr. Posner_!) )
          ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!that began in August._!) )
        )
        ( Satellite (span 6 13) (rel2par elaboration-additional)
          ( Nucleus (span 6 12) (rel2par Sequence)
            ( Nucleus (span 6 8) (rel2par Same-Unit)
              ( Nucleus (span 6 7) (rel2par span)
                ( Nucleus (leaf 6) (rel2par span) (text _!At the time, a group_!) )
                ( Satellite (leaf 7) (rel2par elaboration-additional-e) (text _!called R.B. Partners Ltd.,_!) )
              )
              ( Satellite (leaf 8) (rel2par elaboration-object-attribute-e) (text _!consisting of eight of Arby's largest franchisees,_!) )
            )
            ( Nucleus (span 9 12) (rel2par Same-Unit)
              ( Nucleus (leaf 9) (rel2par span) (text _!offered more than $200 million_!) )
              ( Satellite (span 10 12) (rel2par purpose)
                ( Nucleus (leaf 10) (rel2par span) (text _!to buy Arby's Inc.,_!) )
                ( Satellite (span 11 12) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 11) (rel2par span) (text _!which is part of DWG Corp._!) )
                  ( Satellite (leaf 12) (rel2par elaboration-additional) (text _!DWG is a holding company controlled by Mr. Posner._!) )
                )
              )
            )
          )
          ( Nucleus (leaf 13) (rel2par Sequence) (text _!One week later, Leonard H. Roberts, president and chief executive officer of Arby's, was fired in a dispute with Mr. Posner._!) )
        )
      )
    )
    ( Satellite (span 14 25) (rel2par elaboration-additional)
      ( Nucleus (span 14 16) (rel2par span)
        ( Nucleus (span 14 15) (rel2par span)
          ( Nucleus (leaf 14) (rel2par span) (text _!Friday, 42 franchisees announced the formation of an association_!) )
          ( Satellite (leaf 15) (rel2par elaboration-additional-e) (text _!-- called A.P. Association Inc. --_!) )
        )
        ( Satellite (leaf 16) (rel2par purpose) (text _!to "preserve the integrity of the Arby's system."_!) )
      )
      ( Satellite (span 17 25) (rel2par elaboration-additional)
        ( Nucleus (span 17 22) (rel2par span)
          ( Nucleus (span 17 20) (rel2par span)
            ( Satellite (leaf 17) (rel2par attribution) (text _!The franchisees, owners or operators of 1,000 of the 1,900 franchised Arby's in the U.S., said:_!) )
            ( Nucleus (span 18 20) (rel2par span)
              ( Satellite (leaf 18) (rel2par attribution) (text _!"We have concluded_!) )
              ( Nucleus (span 19 20) (rel2par span)
                ( Nucleus (leaf 19) (rel2par span) (text _!that continued control of Arby's by Victor Posner is totally unacceptable to us,_!) )
                ( Satellite (leaf 20) (rel2par reason) (text _!because it is extremely likely to cause irreparable damage to the Arby's system._!) )
              )
            )
          )
          ( Satellite (span 21 22) (rel2par elaboration-additional)
            ( Nucleus (leaf 21) (rel2par span) (text _!We support all efforts_!) )
            ( Satellite (leaf 22) (rel2par elaboration-object-attribute-e) (text _!to remove Victor Posner from control of Arby's Inc. and the Arby's system."_!) )
          )
        )
        ( Satellite (span 23 25) (rel2par elaboration-additional)
          ( Satellite (leaf 23) (rel2par attribution) (text _!The group said_!) )
          ( Nucleus (span 24 25) (rel2par span)
            ( Nucleus (leaf 24) (rel2par span) (text _!it would consider, among other things, withholding royalty payments and initiating a class-action lawsuit_!) )
            ( Satellite (leaf 25) (rel2par elaboration-object-attribute-e) (text _!seeking court approval for the withholdings._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 26 29) (rel2par statement-response-s)
    ( Satellite (leaf 26) (rel2par attribution) (text _!In Florida, Renee Mottram, a senior vice president at DWG, responded:_!) )
    ( Nucleus (span 27 29) (rel2par span)
