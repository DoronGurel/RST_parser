( Root (span 1 59)
  ( Nucleus (span 1 22) (rel2par Topic-Drift)
    ( Nucleus (span 1 12) (rel2par span)
      ( Nucleus (span 1 11) (rel2par span)
        ( Nucleus (span 1 5) (rel2par span)
          ( Satellite (span 1 3) (rel2par attribution)
            ( Nucleus (leaf 1) (rel2par Same-Unit) (text _!Tandem Computers Inc.,_!) )
            ( Nucleus (span 2 3) (rel2par Same-Unit)
              ( Satellite (leaf 2) (rel2par circumstance-e) (text _!preparing to fight with International Business Machines Corp. for a piece of the mainframe business,_!) )
              ( Nucleus (leaf 3) (rel2par span) (text _!said_!) )
            )
          )
          ( Nucleus (span 4 5) (rel2par span)
            ( Nucleus (leaf 4) (rel2par span) (text _!it expects to post higher revenue and earnings for its fiscal fourth quarter_!) )
            ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!ended Sept. 30._!) )
          )
        )
        ( Satellite (span 6 11) (rel2par elaboration-additional)
          ( Nucleus (span 6 7) (rel2par span)
            ( Satellite (leaf 6) (rel2par attribution) (text _!Tandem said_!) )
            ( Nucleus (leaf 7) (rel2par span) (text _!it expects to report revenue of about $450 million and earnings of 35 cents to 40 cents a share._!) )
          )
          ( Satellite (span 8 11) (rel2par elaboration-additional)
            ( Nucleus (span 8 10) (rel2par span)
              ( Nucleus (span 8 9) (rel2par Same-Unit)
                ( Nucleus (leaf 8) (rel2par span) (text _!The results,_!) )
                ( Satellite (leaf 9) (rel2par elaboration-additional-e) (text _!which are in line with analysts' estimates,_!) )
              )
              ( Nucleus (leaf 10) (rel2par Same-Unit) (text _!reflect "a continued improvement in our U.S. business,"_!) )
            )
            ( Satellite (leaf 11) (rel2par attribution) (text _!said James Treybig, Tandem's chief executive officer._!) )
          )
        )
      )
      ( Satellite (leaf 12) (rel2par circumstance) (text _!In the year-earlier period, Tandem reported net income of $30.1 million, or 31 cents a share, on revenue of $383.9 million._!) )
    )
    ( Satellite (span 13 22) (rel2par elaboration-additional)
      ( Nucleus (span 13 15) (rel2par span)
        ( Nucleus (leaf 13) (rel2par span) (text _!Tandem expects to report the full results for the quarter next week._!) )
        ( Satellite (span 14 15) (rel2par elaboration-additional)
          ( Satellite (leaf 14) (rel2par attribution) (text _!Analysts have predicted_!) )
          ( Nucleus (leaf 15) (rel2par span) (text _!that the Cupertino, Calif., company will report revenue of $430 million to $460 million and earnings of 35 cents to 40 cents a share._!) )
        )
      )
      ( Satellite (span 16 22) (rel2par comment)
        ( Nucleus (span 16 20) (rel2par span)
          ( Nucleus (span 16 18) (rel2par span)
            ( Satellite (leaf 16) (rel2par circumstance) (text _!Commenting on the results for the quarter,_!) )
            ( Nucleus (span 17 18) (rel2par span)
              ( Satellite (leaf 17) (rel2par attribution) (text _!Mr. Treybig said_!) )
              ( Nucleus (leaf 18) (rel2par span) (text _!the strength of the company's domestic business came as "a surprise" to him,_!) )
            )
          )
          ( Satellite (span 19 20) (rel2par elaboration-additional)
            ( Satellite (leaf 19) (rel2par attribution) (text _!noting_!) )
            ( Nucleus (leaf 20) (rel2par span) (text _!that sales "in every region of the U.S. exceeded our plan."_!) )
          )
        )
        ( Satellite (span 21 22) (rel2par explanation-argumentative)
          ( Nucleus (leaf 21) (rel2par span) (text _!The company's U.S. performance was helped by "a record quarter for new customers,"_!) )
          ( Satellite (leaf 22) (rel2par attribution) (text _!he said._!) )
        )
      )
    )
  )
  ( Nucleus (span 23 59) (rel2par Topic-Drift)
    ( Nucleus (span 23 25) (rel2par span)
      ( Nucleus (span 23 24) (rel2par span)
        ( Nucleus (leaf 23) (rel2par span) (text _!Tandem makes "fault-tolerant" computers_!) )
        ( Satellite (leaf 24) (rel2par elaboration-general-specific-e) (text _!-- machines with built-in backup systems --_!) )
      )
      ( Satellite (leaf 25) (rel2par elaboration-object-attribute-e) (text _!that run stock exchanges, networks of automatic tellers and other complex computer systems._!) )
    )
    ( Satellite (span 26 59) (rel2par elaboration-additional)
      ( Nucleus (span 26 39) (rel2par Statement-Response)
        ( Nucleus (span 26 31) (rel2par span)
          ( Nucleus (span 26 27) (rel2par span)
            ( Nucleus (leaf 26) (rel2par span) (text _!Tomorrow the company is scheduled to announce its most powerful computer ever,_!) )
            ( Satellite (leaf 27) (rel2par elaboration-object-attribute-e) (text _!which for the first time will bring it into direct competition with makers of mainframe computers._!) )
          )
          ( Satellite (span 28 31) (rel2par elaboration-additional)
            ( Nucleus (leaf 28) (rel2par span) (text _!Tandem's new high-end computer is called Cyclone._!) )
            ( Satellite (span 29 31) (rel2par elaboration-additional)
              ( Nucleus (span 29 30) (rel2par Same-Unit)
                ( Nucleus (leaf 29) (rel2par span) (text _!Prices for the machine,_!) )
                ( Satellite (leaf 30) (rel2par elaboration-additional-e) (text _!which can come in various configurations,_!) )
              )
              ( Nucleus (leaf 31) (rel2par Same-Unit) (text _!are $2 million to $10 million._!) )
            )
          )
        )
        ( Satellite (span 32 39) (rel2par interpretation-s)
          ( Nucleus (leaf 32) (rel2par span) (text _!Analysts expect the new computer to wrest a hefty slice of business away from IBM, the longtime leader in mainframes._!) )
          ( Satellite (span 33 39) (rel2par explanation-argumentative)
            ( Nucleus (span 33 36) (rel2par span)
              ( Nucleus (span 33 35) (rel2par span)
                ( Nucleus (span 33 34) (rel2par span)
                  ( Satellite (leaf 33) (rel2par attribution) (text _!"We believe_!) )
                  ( Nucleus (leaf 34) (rel2par span) (text _!they could siphon perhaps two to three billion dollars from IBM" over the next few years,_!) )
                )
                ( Satellite (leaf 35) (rel2par attribution) (text _!said George Weiss, an analyst at the Gartner group._!) )
              )
              ( Satellite (leaf 36) (rel2par elaboration-additional) (text _!That will spur Tandem's growth._!) )
            )
            ( Satellite (span 37 39) (rel2par elaboration-additional)
              ( Nucleus (span 37 38) (rel2par span)
                ( Nucleus (leaf 37) (rel2par span) (text _!"I'd be disappointed_!) )
                ( Satellite (leaf 38) (rel2par condition) (text _!if the company grew by less than 20% next year,"_!) )
              )
              ( Satellite (leaf 39) (rel2par attribution) (text _!said John Levinson, an analyst at Goldman, Sachs & Co._!) )
            )
          )
        )
      )
      ( Nucleus (span 40 59) (rel2par Statement-Response)
        ( Nucleus (span 40 44) (rel2par span)
          ( Nucleus (leaf 40) (rel2par span) (text _!IBM is expected to respond to Tandem's Cyclone_!) )
          ( Satellite (span 41 44) (rel2par means)
            ( Nucleus (leaf 41) (rel2par span) (text _!by discounting its own mainframes,_!) )
            ( Satellite (span 42 44) (rel2par elaboration-additional-e)
              ( Nucleus (span 42 43) (rel2par Same-Unit)
                ( Nucleus (leaf 42) (rel2par span) (text _!which_!) )
                ( Satellite (leaf 43) (rel2par attribution-e) (text _!analysts say_!) )
              )
              ( Nucleus (leaf 44) (rel2par Same-Unit) (text _!are roughly three times the price of a comparable system from Tandem._!) )
            )
          )
        )
        ( Satellite (span 45 59) (rel2par elaboration-additional)
          ( Satellite (span 45 46) (rel2par antithesis)
            ( Nucleus (leaf 45) (rel2par span) (text _!"Obviously IBM can give bigger discounts to users immediately,"_!) )
            ( Satellite (leaf 46) (rel2par attribution) (text _!said Mr. Weiss._!) )
          )
          ( Nucleus (span 47 59) (rel2par span)
            ( Nucleus (span 47 50) (rel2par span)
              ( Satellite (leaf 47) (rel2par attribution) (text _!But Mr. Treybig questions_!) )
              ( Nucleus (span 48 50) (rel2par span)
                ( Nucleus (leaf 48) (rel2par span) (text _!whether that will be enough_!) )
                ( Satellite (span 49 50) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 49) (rel2par span) (text _!to stop Tandem's first mainframe from taking on some of the functions_!) )
                  ( Satellite (leaf 50) (rel2par elaboration-object-attribute-e) (text _!that large organizations previously sought from Big Blue's machines._!) )
                )
              )
            )
            ( Satellite (span 51 59) (rel2par explanation-argumentative)
              ( Nucleus (span 51 54) (rel2par span)
                ( Satellite (span 51 52) (rel2par concession)
                  ( Nucleus (leaf 51) (rel2par span) (text _!"The answer isn't price reductions, but new systems,"_!) )
                  ( Satellite (leaf 52) (rel2par attribution) (text _!he said._!) )
                )
                ( Nucleus (span 53 54) (rel2par span)
                  ( Nucleus (leaf 53) (rel2par span) (text _!Nevertheless, Tandem faces a variety of challenges, the biggest_!) )
                  ( Satellite (leaf 54) (rel2par elaboration-object-attribute-e) (text _!being that customers generally view the company's computers as complementary to IBM's mainframes._!) )
                )
              )
              ( Satellite (span 55 59) (rel2par elaboration-additional)
                ( Nucleus (span 55 57) (rel2par span)
                  ( Nucleus (leaf 55) (rel2par span) (text _!Even Mr. Treybig is reluctant to abandon this notion,_!) )
                  ( Satellite (span 56 57) (rel2par elaboration-additional)
                    ( Satellite (leaf 56) (rel2par attribution) (text _!insisting_!) )
                    ( Nucleus (leaf 57) (rel2par span) (text _!that Tandem's new machines aren't replacements for IBM's mainframes._!) )
                  )
                )
                ( Satellite (span 58 59) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 58) (rel2par span) (text _!"We're after a little bigger niche,"_!) )
                  ( Satellite (leaf 59) (rel2par attribution) (text _!he said._!) )
                )
              )
            )
          )
        )
      )
    )
  )
)
