          ( Nucleus (span 1 2) (rel2par span)
            ( Nucleus (leaf 1) (rel2par span) (text _!When Justice William Brennan marks the start of his 34th year on the Supreme Court today,_!) )
            ( Satellite (leaf 2) (rel2par temporal-same-time) (text _!the occasion will differ sharply from previous anniversaries of his tenure._!) )
          )
          ( Satellite (span 3 4) (rel2par elaboration-additional)
            ( Nucleus (leaf 3) (rel2par span) (text _!For the first time, the 83-year-old justice finds his influence almost exclusively in dissent, rather than as a force in the high court's majority._!) )
            ( Satellite (leaf 4) (rel2par elaboration-set-member) (text _!This role reversal holds true, as well, for his three liberal and moderate allies, Justices Thurgood Marshall, Harry Blackmun and John Stevens._!) )
          )
        )
        ( Satellite (span 5 11) (rel2par explanation-argumentative)
          ( Nucleus (span 5 9) (rel2par span)
            ( Nucleus (leaf 5) (rel2par Question-Answer) (text _!But are these four players, three of them in their 80s, ready to assume a different role after 88 years, collectively, of service on the high court?_!) )
            ( Nucleus (span 6 9) (rel2par Question-Answer)
              ( Nucleus (leaf 6) (rel2par Same-Unit) (text _!Every indication is that the four are prepared to accept this new role,_!) )
              ( Nucleus (span 7 8) (rel2par Same-Unit)
                ( Nucleus (leaf 7) (rel2par span) (text _!and the frustrations_!) )
                ( Satellite (leaf 8) (rel2par elaboration-object-attribute-e) (text _!that go with it,_!) )
