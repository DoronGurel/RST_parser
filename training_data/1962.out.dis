                )
              )
            )
            ( Satellite (span 79 82) (rel2par example)
              ( Nucleus (span 79 80) (rel2par span)
                ( Nucleus (leaf 79) (rel2par span) (text _!For instance, the most popular index option is the S&P 100 option,_!) )
                ( Satellite (leaf 80) (rel2par elaboration-object-attribute-e) (text _!commonly called the OEX._!) )
              )
              ( Satellite (span 81 82) (rel2par elaboration-additional)
                ( Nucleus (leaf 81) (rel2par span) (text _!It is based on the stocks_!) )
                ( Satellite (leaf 82) (rel2par elaboration-object-attribute-e) (text _!that make up Standard & Poor's 100-stock index._!) )
              )
            )
          )
          ( Satellite (span 83 84) (rel2par elaboration-additional)
            ( Nucleus (leaf 83) (rel2par List) (text _!Unlike options on individual issues, index options are settled only in cash,_!) )
            ( Nucleus (leaf 84) (rel2par List) (text _!and no stock is ever tendered._!) )
