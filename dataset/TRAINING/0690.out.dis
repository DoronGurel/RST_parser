          ( Nucleus (span 1 8) (rel2par span)
            ( Nucleus (span 1 4) (rel2par Same-Unit)
              ( Nucleus (leaf 1) (rel2par span) (text _!Federal drug regulators,_!) )
              ( Satellite (span 2 4) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 2) (rel2par span) (text _!concerned over British reports_!) )
                ( Satellite (span 3 4) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 3) (rel2par span) (text _!that diabetics have died_!) )
                  ( Satellite (leaf 4) (rel2par circumstance) (text _!after shifting from animal to human-based insulin,_!) )
                )
              )
            )
            ( Nucleus (span 5 8) (rel2par Same-Unit)
              ( Satellite (leaf 5) (rel2par attribution) (text _!say_!) )
