        ( Satellite (span 71 78) (rel2par elaboration-additional)
          ( Nucleus (span 71 72) (rel2par span)
            ( Satellite (leaf 71) (rel2par attribution) (text _!A company spokesman said_!) )
            ( Nucleus (leaf 72) (rel2par span) (text _!it was "possible, but highly unlikely," that the plant might never be built._!) )
          )
          ( Satellite (span 73 78) (rel2par elaboration-additional)
            ( Nucleus (span 73 77) (rel2par span)
              ( Satellite (span 73 76) (rel2par circumstance)
                ( Nucleus (span 73 74) (rel2par List)
                  ( Nucleus (leaf 73) (rel2par span) (text _!"As we regain our leadership level_!) )
                  ( Satellite (leaf 74) (rel2par elaboration-object-attribute-e) (text _!where we have been,_!) )
                )
                ( Nucleus (span 75 76) (rel2par List)
                  ( Nucleus (leaf 75) (rel2par Consequence) (text _!and as we continue to put new products into the marketplace_!) )
                  ( Nucleus (leaf 76) (rel2par Consequence) (text _!and need additional capacity,_!) )
                )
              )
              ( Nucleus (leaf 77) (rel2par span) (text _!we will look at resuming our involvement with our plan,"_!) )
            )
            ( Satellite (leaf 78) (rel2par attribution) (text _!he said._!) )
          )
        )
      )
      ( Satellite (span 79 86) (rel2par elaboration-additional)
        ( Nucleus (leaf 79) (rel2par span) (text _!The new facility was to have been the world's most advanced cereal manufacturing plant, and Kellogg's largest construction project._!) )
        ( Satellite (span 80 86) (rel2par elaboration-additional)
          ( Nucleus (leaf 80) (rel2par span) (text _!The company had retained the Fluor Daniel unit of Fluor Corp. as general contractor._!) )
          ( Satellite (span 81 86) (rel2par antithesis)
            ( Nucleus (span 81 82) (rel2par span)
              ( Satellite (leaf 81) (rel2par attribution) (text _!But in recent weeks, construction-industry sources reported_!) )
              ( Nucleus (leaf 82) (rel2par span) (text _!that early preparation work was slowing at the 185-acre site._!) )
            )
            ( Satellite (span 83 86) (rel2par evidence)
              ( Nucleus (span 83 84) (rel2par List)
                ( Satellite (leaf 83) (rel2par attribution) (text _!Subcontractors said_!) )
                ( Nucleus (leaf 84) (rel2par span) (text _!they were told that equipment orders would be delayed._!) )
              )
              ( Nucleus (span 85 86) (rel2par List)
                ( Nucleus (leaf 85) (rel2par span) (text _!Fluor Daniel already has reassigned most of its work crew,_!) )
                ( Satellite (leaf 86) (rel2par attribution) (text _!the sources said._!) )
              )
            )
          )
        )
      )
