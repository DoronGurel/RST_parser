            ( Nucleus (span 35 38) (rel2par span)
              ( Nucleus (span 35 36) (rel2par Same-Unit)
                ( Nucleus (leaf 35) (rel2par span) (text _!by "buying in" to their foreclosed properties_!) )
                ( Satellite (leaf 36) (rel2par elaboration-additional-e) (text _!(of which there are, alas, many)_!) )
              )
              ( Nucleus (span 37 38) (rel2par Same-Unit)
                ( Nucleus (leaf 37) (rel2par span) (text _!at an inflated "balance due"_!) )
                ( Satellite (leaf 38) (rel2par elaboration-additional-e) (text _!-- say $80,000 on a house worth $55,000 --_!) )
              )
            )
            ( Satellite (span 39 40) (rel2par antithesis)
              ( Nucleus (leaf 39) (rel2par span) (text _!instead of allowing a free market to price the house_!) )
              ( Satellite (leaf 40) (rel2par elaboration-object-attribute-e) (text _!for what it's really worth._!) )
            )
          )
        )
        ( Satellite (span 41 49) (rel2par elaboration-additional)
          ( Nucleus (span 41 42) (rel2par Sequence)
            ( Nucleus (leaf 41) (rel2par span) (text _!Worse, the properties then sit around_!) )
            ( Satellite (leaf 42) (rel2par elaboration-object-attribute-e) (text _!deteriorating for maybe a year or so,_!) )
          )
          ( Nucleus (span 43 46) (rel2par Sequence)
            ( Nucleus (span 43 44) (rel2par Same-Unit)
              ( Nucleus (leaf 43) (rel2par span) (text _!but are resold eventually_!) )
              ( Satellite (leaf 44) (rel2par explanation-argumentative-e) (text _!(because of the attractiveness of the low down payment, etc.)_!) )
            )
            ( Nucleus (span 45 46) (rel2par Same-Unit)
              ( Nucleus (leaf 45) (rel2par span) (text _!to a marginal buyer_!) )
              ( Satellite (leaf 46) (rel2par elaboration-object-attribute-e) (text _!who can't afford both the mortgage and needed repairs;_!) )
            )
          )
          ( Nucleus (span 47 49) (rel2par Sequence)
            ( Satellite (span 47 48) (rel2par result)
              ( Satellite (leaf 47) (rel2par circumstance) (text _!and having little vested interest_!) )
              ( Nucleus (leaf 48) (rel2par span) (text _!that buyer will walk away_!) )
            )
            ( Nucleus (leaf 49) (rel2par span) (text _!and the vicious cycle repeats itself all over again._!) )
          )
