                  ( Satellite (span 56 59) (rel2par example)
                    ( Nucleus (leaf 56) (rel2par span) (text _!The company has five other U.S. plants,_!) )
                    ( Satellite (span 57 59) (rel2par elaboration-set-member)
                      ( Nucleus (span 57 58) (rel2par span)
                        ( Nucleus (leaf 57) (rel2par span) (text _!including a modern facility at its Battle Creek headquarters_!) )
                        ( Satellite (leaf 58) (rel2par elaboration-additional-e) (text _!known as Building 100,_!) )
                      )
                      ( Satellite (leaf 59) (rel2par elaboration-object-attribute-e) (text _!which is to add bran-processing and rice-processing capacity next year._!) )
                    )
                  )
                )
              )
            )
            ( Satellite (span 60 62) (rel2par comparison)
              ( Nucleus (span 60 61) (rel2par span)
                ( Nucleus (leaf 60) (rel2par span) (text _!General Mills, meanwhile, finds itself constrained from boosting sales further_!) )
                ( Satellite (leaf 61) (rel2par reason) (text _!because its plants are operating at capacity._!) )
              )
              ( Satellite (leaf 62) (rel2par consequence-s) (text _!A large plant in Covington, Ga., is to come on line next year._!) )
            )
