                      ( Nucleus (leaf 18) (rel2par span) (text _!L.J. Hooker,_!) )
                      ( Satellite (leaf 19) (rel2par elaboration-additional-e) (text _!based in Atlanta,_!) )
                    )
                    ( Nucleus (leaf 20) (rel2par Same-Unit) (text _!is operating with protection from its creditors under Chapter 11 of the U.S. Bankruptcy Code._!) )
                  )
                  ( Satellite (leaf 21) (rel2par elaboration-additional) (text _!Its parent company, Hooker Corp. of Sydney, Australia, is currently being managed by a court-appointed provisional liquidator._!) )
