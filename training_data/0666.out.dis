              ( Nucleus (span 66 68) (rel2par span)
                ( Nucleus (span 66 67) (rel2par span)
                  ( Nucleus (leaf 66) (rel2par List) (text _!A company such as Federal Paper Board may be overly discounted_!) )
                  ( Nucleus (leaf 67) (rel2par List) (text _!and looks "tempting" to him,_!) )
                )
                ( Satellite (leaf 68) (rel2par attribution) (text _!he says,_!) )
