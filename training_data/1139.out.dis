( Root (span 1 53)
  ( Nucleus (span 1 47) (rel2par TextualOrganization)
    ( Nucleus (span 1 5) (rel2par span)
      ( Nucleus (span 1 2) (rel2par span)
        ( Nucleus (leaf 1) (rel2par span) (text _!Sales of North American-built cars and trucks plunged 20.5% in mid-October from a year earlier,_!) )
        ( Satellite (leaf 2) (rel2par circumstance) (text _!as domestic manufacturers paid the price for heavy incentives earlier this year._!) )
      )
      ( Satellite (span 3 5) (rel2par elaboration-additional)
        ( Nucleus (leaf 3) (rel2par span) (text _!"People are waiting for {new} factory giveaways,"_!) )
        ( Satellite (span 4 5) (rel2par attribution)
          ( Nucleus (leaf 4) (rel2par span) (text _!said Ben Kaye, sales manager of Bob Brest Auto World in Lynn, Mass.,_!) )
          ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!whose sales are slow._!) )
        )
      )
    )
    ( Satellite (span 6 47) (rel2par elaboration-general-specific)
      ( Nucleus (span 6 13) (rel2par List)
        ( Nucleus (span 6 9) (rel2par span)
          ( Nucleus (leaf 6) (rel2par span) (text _!This trend appears to be especially true at General Motors Corp.,_!) )
          ( Satellite (span 7 9) (rel2par elaboration-object-attribute-e)
            ( Nucleus (span 7 8) (rel2par span)
              ( Nucleus (leaf 7) (rel2par span) (text _!which used both dealer and consumer incentives_!) )
              ( Satellite (leaf 8) (rel2par purpose) (text _!to ignite sales in August and September._!) )
            )
            ( Satellite (leaf 9) (rel2par result) (text _!Since then, deliveries have slumped._!) )
          )
        )
        ( Satellite (span 10 13) (rel2par elaboration-general-specific)
          ( Nucleus (span 10 11) (rel2par span)
            ( Nucleus (leaf 10) (rel2par Temporal-Same-Time) (text _!GM's car sales dropped 24.8% in mid-October to 69,980,_!) )
            ( Nucleus (leaf 11) (rel2par Temporal-Same-Time) (text _!while truck sales fell 26% to 37,860._!) )
          )
          ( Satellite (span 12 13) (rel2par elaboration-additional)
            ( Nucleus (leaf 12) (rel2par Temporal-Same-Time) (text _!GM also had dismal results in the first 10 days of the month,_!) )
            ( Nucleus (leaf 13) (rel2par Temporal-Same-Time) (text _!while other auto makers reported mixed results._!) )
          )
        )
      )
      ( Nucleus (span 14 36) (rel2par List)
        ( Nucleus (span 14 20) (rel2par span)
          ( Nucleus (span 14 18) (rel2par span)
            ( Nucleus (span 14 17) (rel2par span)
              ( Nucleus (leaf 14) (rel2par span) (text _!All of the Big Three suffered in the just-ended period, however,_!) )
              ( Satellite (span 15 17) (rel2par circumstance)
                ( Nucleus (span 15 16) (rel2par Same-Unit)
                  ( Nucleus (leaf 15) (rel2par span) (text _!with sales of all domestically made cars,_!) )
                  ( Satellite (leaf 16) (rel2par elaboration-object-attribute-e) (text _!including those built at Japanese-managed plants,_!) )
                )
                ( Nucleus (leaf 17) (rel2par Same-Unit) (text _!falling 19% to 158,863 from a year earlier._!) )
              )
            )
            ( Satellite (leaf 18) (rel2par background) (text _!The seasonal adjusted annual selling rate was six million vehicles, a small improvement from the 5.8 million rate of early October, but a big drop from the 7.1 million rate a year ago._!) )
          )
          ( Satellite (span 19 20) (rel2par elaboration-additional)
            ( Nucleus (leaf 19) (rel2par span) (text _!Sales of domestically made trucks also continued to be sluggish in mid-October,_!) )
            ( Satellite (leaf 20) (rel2par elaboration-object-attribute) (text _!dropping 22.8% to 94,543 from a year ago._!) )
          )
        )
        ( Satellite (span 21 36) (rel2par explanation-argumentative)
          ( Nucleus (span 21 27) (rel2par span)
            ( Nucleus (span 21 24) (rel2par span)
              ( Nucleus (leaf 21) (rel2par span) (text _!The Big Three auto makers already have slashed fourth-quarter production plans 10.4% below year-ago levels,_!) )
              ( Satellite (span 22 24) (rel2par concession)
                ( Nucleus (span 22 23) (rel2par span)
                  ( Nucleus (leaf 22) (rel2par span) (text _!but that may not be enough_!) )
                  ( Satellite (leaf 23) (rel2par purpose) (text _!to prevent inventories from ballooning_!) )
                )
                ( Satellite (leaf 24) (rel2par condition) (text _!if sales don't improve._!) )
              )
            )
            ( Satellite (span 25 27) (rel2par evaluation-s)
              ( Satellite (leaf 25) (rel2par attribution) (text _!Industry analyst John H. Qualls, a vice president with Hill & Knowlton in St. Louis, forecasts_!) )
              ( Nucleus (span 26 27) (rel2par span)
                ( Nucleus (leaf 26) (rel2par span) (text _!that domestic auto makers will have a 93-day supply of cars at the end of the year,_!) )
                ( Satellite (leaf 27) (rel2par condition) (text _!even if car sales improve to a 6.5 million vehicle rate for the quarter._!) )
              )
            )
          )
          ( Satellite (span 28 36) (rel2par elaboration-additional)
            ( Nucleus (span 28 32) (rel2par span)
              ( Nucleus (leaf 28) (rel2par span) (text _!Ford Motor Co. reported a 21.2% drop in sales of domestic-made cars to 46,995 and a 24.2% drop in domestic trucks to 31,143._!) )
              ( Satellite (span 29 32) (rel2par reason)
                ( Nucleus (span 29 30) (rel2par span)
                  ( Nucleus (leaf 29) (rel2par span) (text _!The sales are being dragged down by a glut of 1989 vehicles,_!) )
                  ( Satellite (leaf 30) (rel2par attribution) (text _!said Joel Pitcoff, a Ford analyst._!) )
                )
                ( Satellite (span 31 32) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 31) (rel2par span) (text _!The earlier use of incentives depleted the market of "scavengers" for bargain-basement 1989 cars,_!) )
                  ( Satellite (leaf 32) (rel2par attribution) (text _!he said._!) )
                )
              )
            )
            ( Satellite (span 33 36) (rel2par elaboration-additional)
              ( Nucleus (leaf 33) (rel2par span) (text _!Town & Country Ford in Charlotte, N.C., still needs to move about 850 1989 cars and trucks._!) )
              ( Satellite (span 34 36) (rel2par explanation-argumentative)
                ( Nucleus (leaf 34) (rel2par span) (text _!Business had been fairly strong_!) )
                ( Satellite (span 35 36) (rel2par circumstance)
                  ( Nucleus (leaf 35) (rel2par span) (text _!until Hurricane Hugo hit the area,_!) )
                  ( Satellite (leaf 36) (rel2par result) (text _!but has been down since._!) )
                )
              )
            )
          )
        )
      )
      ( Nucleus (span 37 39) (rel2par List)
        ( Nucleus (leaf 37) (rel2par span) (text _!Chrysler Corp. also hit the rocks in mid-October._!) )
        ( Satellite (span 38 39) (rel2par elaboration-additional)
          ( Nucleus (leaf 38) (rel2par span) (text _!The No. 3 U.S. auto maker had a 23.7% plunge in car sales to 22,336 and a 17.5% drop in truck sales to 22,925,_!) )
          ( Satellite (leaf 39) (rel2par elaboration-object-attribute-e) (text _!which include its minivans and Jeeps._!) )
        )
      )
      ( Nucleus (span 40 47) (rel2par List)
        ( Nucleus (span 40 42) (rel2par Contrast)
          ( Nucleus (span 40 41) (rel2par Same-Unit)
            ( Nucleus (leaf 40) (rel2par span) (text _!Honda Motor Co.,_!) )
            ( Satellite (leaf 41) (rel2par elaboration-object-attribute-e) (text _!which continues to have short supplies of domestically made Accords,_!) )
          )
          ( Nucleus (leaf 42) (rel2par Same-Unit) (text _!saw its sales of North American-built cars fall 14.1% to 8,355._!) )
        )
        ( Nucleus (span 43 47) (rel2par Contrast)
          ( Nucleus (span 43 44) (rel2par List)
            ( Nucleus (leaf 43) (rel2par span) (text _!But sales of domestic cars and trucks at Nissan Motor Corp. rose 26.1% to 5,651._!) )
            ( Satellite (leaf 44) (rel2par reason) (text _!A Nissan spokesman attributed the increase to the use of incentives this year and not a year ago and to higher fleet sales._!) )
          )
          ( Nucleus (span 45 47) (rel2par List)
            ( Nucleus (span 45 46) (rel2par Same-Unit)
              ( Nucleus (leaf 45) (rel2par span) (text _!Toyota Motor Corp.,_!) )
              ( Satellite (leaf 46) (rel2par elaboration-object-attribute-e) (text _!which opened a plant in Georgetown, Ky., last year,_!) )
            )
            ( Nucleus (leaf 47) (rel2par Same-Unit) (text _!saw sales triple to 6,256 vehicles._!) )
          )
        )
      )
    )
