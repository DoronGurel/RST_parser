                  ( Nucleus (span 32 35) (rel2par List)
                    ( Satellite (leaf 32) (rel2par attribution) (text _!Great Northern Nekoosa said,_!) )
                    ( Nucleus (span 33 35) (rel2par span)
                      ( Nucleus (span 33 34) (rel2par Same-Unit)
                        ( Nucleus (leaf 33) (rel2par span) (text _!"The odds of the dire predictions_!) )
                        ( Satellite (leaf 34) (rel2par elaboration-object-attribute-e) (text _!about us being right_!) )
