            )
          )
        )
      )
      ( Nucleus (span 29 43) (rel2par span)
        ( Nucleus (span 29 35) (rel2par span)
          ( Nucleus (span 29 33) (rel2par span)
            ( Satellite (leaf 29) (rel2par attribution) (text _!The FAA already has ordered_!) )
            ( Nucleus (span 30 33) (rel2par span)
              ( Nucleus (span 30 31) (rel2par Same-Unit)
                ( Nucleus (leaf 30) (rel2par span) (text _!that all 232 disks_!) )
                ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!made by the old process_!) )
              )
              ( Nucleus (span 32 33) (rel2par Same-Unit)
                ( Nucleus (leaf 32) (rel2par List) (text _!be removed from the planes_!) )
                ( Nucleus (leaf 33) (rel2par List) (text _!and subjected to an ultrasonic test in a water-submersion chamber._!) )
              )
            )
          )
          ( Satellite (span 34 35) (rel2par elaboration-additional)
            ( Nucleus (leaf 34) (rel2par span) (text _!Such tests make the FAA confident that a Sioux City-type accident "won't happen again,"_!) )
