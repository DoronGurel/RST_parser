( Root (span 1 125)
  ( Nucleus (span 1 124) (rel2par TextualOrganization)
    ( Nucleus (span 1 103) (rel2par Topic-Shift)
      ( Nucleus (span 1 60) (rel2par Topic-Drift)
        ( Nucleus (span 1 57) (rel2par span)
          ( Nucleus (span 1 35) (rel2par Contrast)
            ( Nucleus (span 1 23) (rel2par span)
              ( Nucleus (span 1 5) (rel2par span)
                ( Nucleus (leaf 1) (rel2par span) (text _!Ripples from the strike by 55,000 Machinists union members against Boeing Co. reached air carriers Friday_!) )
                ( Satellite (span 2 5) (rel2par consequence-n)
                  ( Nucleus (span 2 3) (rel2par span)
                    ( Satellite (leaf 2) (rel2par attribution) (text _!as America West Airlines announced_!) )
                    ( Nucleus (leaf 3) (rel2par span) (text _!it will postpone its new service out of Houston_!) )
                  )
                  ( Satellite (span 4 5) (rel2par consequence-n)
                    ( Nucleus (leaf 4) (rel2par span) (text _!because of delays_!) )
                    ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!in receiving aircraft from the Seattle jet maker._!) )
                  )
                )
              )
              ( Satellite (span 6 23) (rel2par summary-n)
                ( Nucleus (span 6 13) (rel2par span)
                  ( Satellite (leaf 6) (rel2par attribution) (text _!Peter Otradovec, vice president for planning at the Phoenix, Ariz., carrier, said in an interview_!) )
                  ( Nucleus (span 7 13) (rel2par span)
                    ( Nucleus (span 7 9) (rel2par span)
                      ( Nucleus (span 7 8) (rel2par Same-Unit)
                        ( Nucleus (leaf 7) (rel2par span) (text _!that the work stoppage at Boeing,_!) )
                        ( Satellite (leaf 8) (rel2par elaboration-additional-e) (text _!now entering its 13th day,_!) )
                      )
                      ( Nucleus (leaf 9) (rel2par Same-Unit) (text _!"has caused some turmoil in our scheduling"_!) )
                    )
                    ( Satellite (span 10 13) (rel2par elaboration-general-specific)
                      ( Nucleus (span 10 12) (rel2par Same-Unit)
                        ( Nucleus (leaf 10) (rel2par span) (text _!and that more than 500 passengers_!) )
                        ( Satellite (span 11 12) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (leaf 11) (rel2par span) (text _!who were booked_!) )
                          ( Satellite (leaf 12) (rel2par purpose) (text _!to fly out of Houston on America West_!) )
                        )
                      )
                      ( Nucleus (leaf 13) (rel2par Same-Unit) (text _!would now be put on other airlines._!) )
                    )
                  )
                )
                ( Satellite (span 14 23) (rel2par consequence-n)
                  ( Nucleus (span 14 21) (rel2par span)
                    ( Nucleus (span 14 20) (rel2par span)
                      ( Satellite (leaf 14) (rel2par attribution) (text _!Mr. Otradovec said_!) )
                      ( Nucleus (span 15 20) (rel2par span)
                        ( Satellite (leaf 15) (rel2par attribution) (text _!Boeing told America West_!) )
                        ( Nucleus (span 16 20) (rel2par span)
                          ( Nucleus (span 16 17) (rel2par Same-Unit)
                            ( Nucleus (leaf 16) (rel2par span) (text _!that the 757_!) )
                            ( Satellite (leaf 17) (rel2par elaboration-object-attribute-e) (text _!it was supposed to get this Thursday_!) )
                          )
                          ( Nucleus (span 18 20) (rel2par Same-Unit)
                            ( Nucleus (leaf 18) (rel2par span) (text _!wouldn't be delivered until Nov. 7_!) )
                            ( Satellite (span 19 20) (rel2par elaboration-additional-e)
                              ( Nucleus (leaf 19) (rel2par span) (text _!-- the day after the airline had been planning to initiate service at Houston with four daily flights,_!) )
                              ( Satellite (leaf 20) (rel2par elaboration-set-member-e) (text _!including three nonstops to Phoenix and one nonstop to Las Vegas._!) )
                            )
                          )
                        )
                      )
                    )
                    ( Satellite (leaf 21) (rel2par consequence-s) (text _!Now, those routes aren't expected to begin until Jan._!) )
                  )
                  ( Satellite (span 22 23) (rel2par elaboration-additional)
                    ( Nucleus (leaf 22) (rel2par span) (text _!Boeing is also supposed to send to America West another 757 twin-engine aircraft as well as a 737 by year's end._!) )
                    ( Satellite (leaf 23) (rel2par interpretation-s) (text _!Those, too, are almost certain to arrive late._!) )
                  )
                )
              )
            )
            ( Satellite (span 24 35) (rel2par interpretation-s)
              ( Satellite (span 24 26) (rel2par antithesis)
                ( Nucleus (span 24 25) (rel2par Same-Unit)
                  ( Nucleus (leaf 24) (rel2par span) (text _!At this point, no other America West flights_!) )
                  ( Satellite (leaf 25) (rel2par example-e) (text _!-- including its new service at San Antonio, Texas; Newark, N.J.; and Palmdale, Calif. --_!) )
                )
                ( Nucleus (leaf 26) (rel2par Same-Unit) (text _!have been affected by the delays in Boeing deliveries._!) )
              )
              ( Nucleus (span 27 35) (rel2par span)
                ( Nucleus (span 27 28) (rel2par span)
                  ( Nucleus (leaf 27) (rel2par span) (text _!Nevertheless, the company's reaction underscores the domino effect_!) )
                  ( Satellite (leaf 28) (rel2par elaboration-object-attribute-e) (text _!that a huge manufacturer such as Boeing can have on other parts of the economy._!) )
                )
                ( Satellite (span 29 35) (rel2par elaboration-additional)
                  ( Nucleus (leaf 29) (rel2par span) (text _!It also is sure to help the machinists put added pressure on the company._!) )
                  ( Satellite (span 30 35) (rel2par evidence)
                    ( Nucleus (span 30 33) (rel2par span)
                      ( Nucleus (span 30 32) (rel2par span)
                        ( Satellite (leaf 30) (rel2par attribution-n) (text _!"I just don't feel_!) )
                        ( Nucleus (span 31 32) (rel2par span)
                          ( Nucleus (leaf 31) (rel2par Disjunction) (text _!that the company can really stand_!) )
                          ( Nucleus (leaf 32) (rel2par Disjunction) (text _!or would want a prolonged walkout,"_!) )
                        )
                      )
                      ( Satellite (leaf 33) (rel2par attribution) (text _!Tom Baker, president of Machinists' District 751, said in an interview yesterday._!) )
                    )
                    ( Satellite (span 34 35) (rel2par explanation-argumentative)
                      ( Satellite (leaf 34) (rel2par attribution-n) (text _!"I don't think_!) )
                      ( Nucleus (leaf 35) (rel2par span) (text _!their customers would like it very much."_!) )
                    )
                  )
                )
              )
            )
          )
          ( Nucleus (span 36 57) (rel2par Contrast)
            ( Nucleus (span 36 43) (rel2par span)
              ( Nucleus (span 36 38) (rel2par span)
                ( Satellite (leaf 36) (rel2par consequence-n) (text _!America West, though, is a smaller airline_!) )
                ( Nucleus (span 37 38) (rel2par span)
                  ( Nucleus (leaf 37) (rel2par span) (text _!and therefore more affected by the delayed delivery of a single plane_!) )
                  ( Satellite (leaf 38) (rel2par comparison) (text _!than many of its competitors would be._!) )
                )
              )
              ( Satellite (span 39 43) (rel2par interpretation-s)
                ( Nucleus (span 39 42) (rel2par span)
                  ( Satellite (leaf 39) (rel2par attribution) (text _!"I figure_!) )
                  ( Nucleus (span 40 42) (rel2par span)
                    ( Satellite (span 40 41) (rel2par hypothetical)
                      ( Nucleus (leaf 40) (rel2par span) (text _!that American and United probably have such a hard time_!) )
                      ( Satellite (leaf 41) (rel2par manner) (text _!counting all the planes in their fleets,_!) )
                    )
                    ( Nucleus (leaf 42) (rel2par span) (text _!they might not miss one at all,"_!) )
                  )
                )
                ( Satellite (leaf 43) (rel2par attribution) (text _!Mr. Otradovec said._!) )
              )
            )
            ( Satellite (span 44 57) (rel2par evidence)
              ( Nucleus (span 44 45) (rel2par span)
                ( Satellite (leaf 44) (rel2par attribution-n) (text _!Indeed, a random check Friday didn't seem to indicate_!) )
                ( Nucleus (leaf 45) (rel2par span) (text _!that the strike was having much of an effect on other airline operations._!) )
              )
              ( Satellite (span 46 57) (rel2par example)
                ( Nucleus (span 46 52) (rel2par List)
                  ( Nucleus (span 46 48) (rel2par span)
                    ( Nucleus (span 46 47) (rel2par List)
                      ( Nucleus (leaf 46) (rel2par span) (text _!Southwest Airlines has a Boeing 737-300_!) )
                      ( Satellite (leaf 47) (rel2par elaboration-object-attribute-e) (text _!set for delivery at the end of this month_!) )
                    )
                    ( Nucleus (leaf 48) (rel2par List) (text _!and expects to have the plane on time._!) )
                  )
                  ( Satellite (span 49 52) (rel2par elaboration-additional)
                    ( Nucleus (span 49 51) (rel2par span)
                      ( Satellite (leaf 49) (rel2par consequence-n) (text _!"It's so close to completion,_!) )
                      ( Nucleus (span 50 51) (rel2par span)
                        ( Satellite (leaf 50) (rel2par attribution) (text _!Boeing's told us_!) )
                        ( Nucleus (leaf 51) (rel2par span) (text _!there won't be a problem,"_!) )
                      )
                    )
                    ( Satellite (leaf 52) (rel2par attribution) (text _!said a Southwest spokesman._!) )
                  )
                )
                ( Nucleus (span 53 57) (rel2par List)
                  ( Nucleus (span 53 55) (rel2par span)
                    ( Satellite (leaf 53) (rel2par attribution) (text _!A spokesman for AMR Corp. said_!) )
                    ( Nucleus (span 54 55) (rel2par span)
                      ( Satellite (leaf 54) (rel2par attribution) (text _!Boeing has assured American Airlines_!) )
                      ( Nucleus (leaf 55) (rel2par span) (text _!it will deliver a 757 on time later this month._!) )
                    )
                  )
                  ( Satellite (span 56 57) (rel2par elaboration-additional)
                    ( Nucleus (leaf 56) (rel2par Temporal-Same-Time) (text _!American is preparing to take delivery of another 757 in early December and 20 more next year_!) )
                    ( Nucleus (leaf 57) (rel2par Temporal-Same-Time) (text _!and isn't anticipating any changes in that timetable._!) )
                  )
                )
              )
            )
          )
        )
        ( Satellite (span 58 60) (rel2par explanation-argumentative)
          ( Satellite (leaf 58) (rel2par attribution) (text _!In Seattle, a Boeing spokesman explained_!) )
          ( Nucleus (span 59 60) (rel2par span)
            ( Nucleus (leaf 59) (rel2par List) (text _!that the company has been in constant communication with all of its customers_!) )
            ( Nucleus (leaf 60) (rel2par List) (text _!and that it was impossible to predict what further disruptions might be triggered by the strike._!) )
          )
        )
      )
      ( Nucleus (span 61 65) (rel2par Topic-Drift)
        ( Satellite (span 61 63) (rel2par result)
          ( Nucleus (span 61 62) (rel2par span)
            ( Nucleus (leaf 61) (rel2par span) (text _!Meanwhile, supervisors and non-striking employees have been trying to finish some 40 aircraft_!) )
            ( Satellite (leaf 62) (rel2par elaboration-general-specific-e) (text _!-- mostly 747 and 767 jumbo jets at the company's Everett, Wash., plant --_!) )
          )
          ( Satellite (leaf 63) (rel2par elaboration-object-attribute-e) (text _!that were all but completed before the walkout._!) )
        )
        ( Nucleus (span 64 65) (rel2par span)
          ( Nucleus (leaf 64) (rel2par span) (text _!As of Friday, four had been delivered_!) )
          ( Satellite (leaf 65) (rel2par elaboration-additional) (text _!and a fifth plane, a 747-400, was supposed to be flown out over the weekend to Air China._!) )
        )
      )
      ( Nucleus (span 66 103) (rel2par Topic-Drift)
        ( Nucleus (span 66 94) (rel2par span)
          ( Nucleus (span 66 74) (rel2par span)
            ( Nucleus (span 66 67) (rel2par span)
              ( Nucleus (leaf 66) (rel2par span) (text _!No date has yet been set_!) )
              ( Satellite (leaf 67) (rel2par purpose) (text _!to get back to the bargaining table._!) )
            )
            ( Satellite (span 68 74) (rel2par explanation-argumentative)
              ( Nucleus (span 68 71) (rel2par span)
                ( Satellite (leaf 68) (rel2par attribution) (text _!"We want to make sure_!) )
                ( Nucleus (span 69 71) (rel2par span)
                  ( Nucleus (span 69 70) (rel2par span)
                    ( Satellite (leaf 69) (rel2par attribution) (text _!they know_!) )
                    ( Nucleus (leaf 70) (rel2par span) (text _!what they want_!) )
                  )
                  ( Satellite (leaf 71) (rel2par temporal-before) (text _!before they come back,"_!) )
                )
              )
              ( Satellite (span 72 74) (rel2par attribution)
                ( Nucleus (leaf 72) (rel2par span) (text _!said Doug Hammond, the federal mediator_!) )
                ( Satellite (span 73 74) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 73) (rel2par span) (text _!who has been in contact with both sides_!) )
                  ( Satellite (leaf 74) (rel2par temporal-after) (text _!since the strike began._!) )
                )
              )
            )
          )
          ( Satellite (span 75 94) (rel2par interpretation-s)
            ( Nucleus (span 75 83) (rel2par Contrast)
              ( Nucleus (span 75 81) (rel2par span)
                ( Nucleus (span 75 77) (rel2par span)
                  ( Nucleus (leaf 75) (rel2par span) (text _!The investment community, for one, has been anticipating a speedy resolution._!) )
                  ( Satellite (span 76 77) (rel2par elaboration-additional)
                    ( Satellite (leaf 76) (rel2par concession) (text _!Though Boeing's stock price was battered along with the rest of the market Friday,_!) )
                    ( Nucleus (leaf 77) (rel2par span) (text _!it actually has risen over the last two weeks on the strength of new orders._!) )
                  )
                )
                ( Satellite (span 78 81) (rel2par elaboration-additional)
                  ( Nucleus (span 78 80) (rel2par span)
                    ( Nucleus (leaf 78) (rel2par span) (text _!"The market has taken two views:_!) )
                    ( Satellite (span 79 80) (rel2par elaboration-set-member)
                      ( Nucleus (leaf 79) (rel2par List) (text _!that the labor situation will get settled in the short term_!) )
                      ( Nucleus (leaf 80) (rel2par List) (text _!and that things look very rosy for Boeing in the long term,"_!) )
                    )
                  )
                  ( Satellite (leaf 81) (rel2par attribution) (text _!said Howard Rubel, an analyst at Cyrus J. Lawrence Inc._!) )
                )
              )
              ( Satellite (span 82 83) (rel2par elaboration-additional)
                ( Nucleus (leaf 82) (rel2par span) (text _!Boeing's shares fell $4 Friday_!) )
                ( Satellite (leaf 83) (rel2par consequence-s) (text _!to close at $57.375 in composite trading on the New York Stock Exchange._!) )
              )
            )
            ( Nucleus (span 84 94) (rel2par Contrast)
              ( Nucleus (span 84 93) (rel2par span)
                ( Satellite (span 84 90) (rel2par concession)
                  ( Nucleus (span 84 88) (rel2par span)
                    ( Satellite (leaf 84) (rel2par attribution) (text _!But Mr. Baker said_!) )
                    ( Nucleus (span 85 88) (rel2par span)
                      ( Satellite (leaf 85) (rel2par attribution) (text _!he thinks_!) )
                      ( Nucleus (span 86 88) (rel2par span)
                        ( Nucleus (span 86 87) (rel2par Same-Unit)
                          ( Nucleus (leaf 86) (rel2par span) (text _!the earliest_!) )
                          ( Satellite (leaf 87) (rel2par elaboration-object-attribute-e) (text _!a pact could be struck_!) )
                        )
                        ( Nucleus (leaf 88) (rel2par Same-Unit) (text _!would be the end of this month,_!) )
                      )
                    )
                  )
                  ( Satellite (span 89 90) (rel2par manner)
                    ( Satellite (leaf 89) (rel2par attribution) (text _!hinting_!) )
                    ( Nucleus (leaf 90) (rel2par span) (text _!that the company and union may resume negotiations as early as this week._!) )
                  )
                )
                ( Nucleus (span 91 93) (rel2par span)
                  ( Nucleus (span 91 92) (rel2par Same-Unit)
                    ( Nucleus (leaf 91) (rel2par span) (text _!Still,_!) )
                    ( Satellite (leaf 92) (rel2par attribution-e) (text _!he said,_!) )
                  )
                  ( Nucleus (leaf 93) (rel2par Same-Unit) (text _!it's possible that the strike could last considerably longer._!) )
                )
              )
              ( Satellite (leaf 94) (rel2par comment) (text _!"I wouldn't expect an immediate resolution to anything."_!) )
            )
          )
        )
        ( Satellite (span 95 103) (rel2par problem-solution-s)
          ( Nucleus (span 95 97) (rel2par Statement-Response)
            ( Nucleus (leaf 95) (rel2par span) (text _!Last week, Boeing Chairman Frank Shrontz sent striking workers a letter,_!) )
            ( Satellite (span 96 97) (rel2par elaboration-additional-e)
              ( Satellite (leaf 96) (rel2par attribution) (text _!saying_!) )
              ( Nucleus (leaf 97) (rel2par span) (text _!that "to my knowledge, Boeing's offer represents the best overall three-year contract of any major U.S. industrial firm in recent history."_!) )
            )
          )
          ( Nucleus (span 98 103) (rel2par Statement-Response)
            ( Nucleus (span 98 100) (rel2par span)
              ( Nucleus (span 98 99) (rel2par Same-Unit)
                ( Nucleus (leaf 98) (rel2par span) (text _!But Mr. Baker called the letter_!) )
                ( Satellite (leaf 99) (rel2par elaboration-additional-e) (text _!-- and the company's offer of a 10% wage increase over the life of the pact, plus bonuses --_!) )
              )
              ( Nucleus (leaf 100) (rel2par Same-Unit) (text _!"very weak."_!) )
            )
            ( Satellite (span 101 103) (rel2par elaboration-additional)
              ( Satellite (leaf 101) (rel2par attribution) (text _!He added_!) )
              ( Nucleus (span 102 103) (rel2par span)
                ( Nucleus (leaf 102) (rel2par span) (text _!that the company miscalculated the union's resolve and the workers' disgust_!) )
                ( Satellite (leaf 103) (rel2par analogy-e) (text _!with being forced to work many hours overtime._!) )
              )
            )
          )
        )
      )
    )
    ( Nucleus (span 104 124) (rel2par Topic-Shift)
      ( Nucleus (leaf 104) (rel2par TextualOrganization) (text _!In separate developments:_!) )
      ( Nucleus (span 105 124) (rel2par TextualOrganization)
        ( Nucleus (span 105 108) (rel2par List)
          ( Satellite (leaf 105) (rel2par concession) (text _!-- Talks have broken off between Machinists representatives at Lockheed Corp. and the Calabasas, Calif., aerospace company._!) )
          ( Nucleus (span 106 108) (rel2par span)
            ( Nucleus (leaf 106) (rel2par List) (text _!The union is continuing to work through its expired contract, however._!) )
            ( Nucleus (span 107 108) (rel2par List)
              ( Nucleus (leaf 107) (rel2par Contrast) (text _!It had planned a strike vote for next Sunday,_!) )
              ( Nucleus (leaf 108) (rel2par Contrast) (text _!but that has been pushed back indefinitely._!) )
            )
          )
        )
        ( Nucleus (span 109 115) (rel2par List)
          ( Nucleus (span 109 114) (rel2par span)
            ( Satellite (span 109 111) (rel2par attribution)
              ( Nucleus (span 109 110) (rel2par Same-Unit)
                ( Nucleus (leaf 109) (rel2par span) (text _!-- United Auto Workers Local 1069,_!) )
                ( Satellite (leaf 110) (rel2par elaboration-additional-e) (text _!which represents 3,000 workers at Boeing's helicopter unit in Delaware County, Pa.,_!) )
              )
              ( Nucleus (leaf 111) (rel2par Same-Unit) (text _!said_!) )
            )
            ( Nucleus (span 112 114) (rel2par span)
              ( Nucleus (span 112 113) (rel2par span)
                ( Nucleus (leaf 112) (rel2par span) (text _!it agreed to extend its contract on a day-by-day basis, with a 10-day notification_!) )
                ( Satellite (leaf 113) (rel2par elaboration-object-attribute-e) (text _!to cancel,_!) )
              )
              ( Satellite (leaf 114) (rel2par temporal-same-time) (text _!while it continues bargaining._!) )
            )
          )
          ( Satellite (leaf 115) (rel2par elaboration-additional) (text _!The accord expired yesterday._!) )
        )
        ( Nucleus (span 116 124) (rel2par List)
          ( Nucleus (span 116 121) (rel2par span)
            ( Nucleus (span 116 119) (rel2par span)
              ( Nucleus (span 116 118) (rel2par span)
                ( Satellite (leaf 116) (rel2par attribution) (text _!-- And Boeing on Friday said_!) )
                ( Nucleus (span 117 118) (rel2par span)
                  ( Nucleus (leaf 117) (rel2par span) (text _!it received an order from Martinair Holland for four model 767-300 wide-body jetliners_!) )
                  ( Satellite (leaf 118) (rel2par evaluation-s-e) (text _!valued at a total of about $326 million._!) )
                )
              )
              ( Satellite (leaf 119) (rel2par elaboration-additional) (text _!The planes, long range versions of the medium-haul twin-jet, will be delivered with Pratt & Whitney PW4060 engines._!) )
            )
            ( Satellite (span 120 121) (rel2par explanation-argumentative)
              ( Nucleus (leaf 120) (rel2par List) (text _!Pratt & Whitney is a unit of United Technologies Inc._!) )
              ( Nucleus (leaf 121) (rel2par List) (text _!Martinair Holland is based in Amsterdam._!) )
            )
          )
          ( Satellite (span 122 124) (rel2par elaboration-additional)
            ( Satellite (leaf 122) (rel2par attribution) (text _!A Boeing spokeswoman said_!) )
            ( Nucleus (span 123 124) (rel2par span)
              ( Nucleus (leaf 123) (rel2par span) (text _!a delivery date for the planes is still being worked out "for a variety of reasons,_!) )
              ( Satellite (leaf 124) (rel2par circumstance) (text _!but not because of the strike."_!) )
            )
          )
        )
      )
    )
  )
  ( Nucleus (leaf 125) (rel2par TextualOrganization) (text _!Bridget O'Brian contributed to this article._!) )
