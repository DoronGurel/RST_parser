                  )
                  ( Satellite (span 16 17) (rel2par elaboration-additional)
                    ( Nucleus (leaf 16) (rel2par Contrast) (text _!"Hooker's philosophy was to build and sell._!) )
                    ( Nucleus (leaf 17) (rel2par Contrast) (text _!We want to build and hold."_!) )
                  )
                )
