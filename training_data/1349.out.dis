( Root (span 1 83)
  ( Nucleus (span 1 28) (rel2par span)
    ( Nucleus (span 1 18) (rel2par span)
      ( Nucleus (span 1 7) (rel2par span)
        ( Nucleus (span 1 4) (rel2par span)
          ( Nucleus (span 1 3) (rel2par span)
            ( Nucleus (leaf 1) (rel2par span) (text _!The House Appropriations Committee approved an estimated $2.85 billion in emergency funding_!) )
            ( Satellite (span 2 3) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 2) (rel2par List) (text _!to assist California's recovery from last week's earthquake_!) )
              ( Nucleus (leaf 3) (rel2par List) (text _!and to extend further aid to East Coast victims of Hurricane Hugo._!) )
            )
          )
          ( Satellite (leaf 4) (rel2par evaluation-s) (text _!The package was termed excessive by the Bush administration,_!) )
        )
        ( Satellite (span 5 7) (rel2par elaboration-additional)
          ( Nucleus (leaf 5) (rel2par span) (text _!but it also provoked a struggle with influential California lawmakers_!) )
          ( Satellite (span 6 7) (rel2par elaboration-additional-e)
            ( Nucleus (leaf 6) (rel2par span) (text _!who sought unsuccessfully to add nearly $1 billion more and waive current restrictions_!) )
            ( Satellite (leaf 7) (rel2par elaboration-object-attribute-e) (text _!to expedite the distribution of funds._!) )
          )
        )
      )
      ( Satellite (span 8 18) (rel2par elaboration-additional)
        ( Nucleus (span 8 11) (rel2par span)
          ( Nucleus (leaf 8) (rel2par List) (text _!By a 26-7 margin, the committee scuttled the more expensive alternative,_!) )
          ( Nucleus (span 9 11) (rel2par List)
            ( Nucleus (span 9 10) (rel2par Same-Unit)
              ( Nucleus (leaf 9) (rel2par span) (text _!and the debate forced a strained confrontation between Appropriations Committee Chairman Jamie Whitten_!) )
              ( Satellite (leaf 10) (rel2par elaboration-additional-e) (text _!(D., Miss.)_!) )
            )
            ( Nucleus (leaf 11) (rel2par Same-Unit) (text _!and his party's largest state delegation in the House._!) )
          )
        )
        ( Satellite (span 12 18) (rel2par explanation-argumentative)
          ( Nucleus (span 12 15) (rel2par span)
            ( Nucleus (leaf 12) (rel2par span) (text _!"I have no regrets about going forward,"_!) )
            ( Satellite (span 13 15) (rel2par attribution)
              ( Nucleus (span 13 14) (rel2par span)
                ( Nucleus (leaf 13) (rel2par span) (text _!said Rep. Vic Fazio_!) )
                ( Satellite (leaf 14) (rel2par elaboration-additional-e) (text _!(D.,Calif.),_!) )
              )
              ( Satellite (leaf 15) (rel2par elaboration-additional-e) (text _!who sought later to play down the sometimes hostile tone of the long evening meeting._!) )
            )
          )
          ( Satellite (span 16 18) (rel2par elaboration-additional)
            ( Nucleus (span 16 17) (rel2par List)
              ( Nucleus (leaf 16) (rel2par span) (text _!"We are the Golden State,"_!) )
              ( Satellite (leaf 17) (rel2par attribution) (text _!Mr. Fazio said,_!) )
            )
            ( Nucleus (leaf 18) (rel2par List) (text _!"and there is a certain amount of jealousy."_!) )
          )
        )
      )
    )
    ( Satellite (span 19 28) (rel2par elaboration-additional)
      ( Nucleus (span 19 22) (rel2par span)
        ( Nucleus (leaf 19) (rel2par span) (text _!The $2.85 billion package incorporates $500 million for small-business loans, $1 billion in highway construction funds, and $1.35 billion_!) )
        ( Satellite (span 20 22) (rel2par elaboration-additional-e)
          ( Nucleus (leaf 20) (rel2par span) (text _!divided between general emergency assistance and a reserve_!) )
          ( Satellite (span 21 22) (rel2par elaboration-additional-e)
            ( Nucleus (leaf 21) (rel2par span) (text _!to be available to President Bush_!) )
            ( Satellite (leaf 22) (rel2par purpose) (text _!to meet unanticipated costs from the two disasters._!) )
          )
        )
      )
      ( Satellite (span 23 28) (rel2par elaboration-additional)
        ( Nucleus (span 23 24) (rel2par span)
          ( Nucleus (leaf 23) (rel2par span) (text _!The funding is attached to a stopgap bill_!) )
          ( Satellite (leaf 24) (rel2par purpose) (text _!to keep most of the government operating through Nov. 15._!) )
        )
        ( Satellite (span 25 28) (rel2par elaboration-additional)
          ( Nucleus (leaf 25) (rel2par List) (text _!The measure is expected to come before the House today,_!) )
          ( Nucleus (span 26 28) (rel2par List)
            ( Nucleus (leaf 26) (rel2par span) (text _!and Congress is under pressure_!) )
            ( Satellite (span 27 28) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 27) (rel2par span) (text _!to complete action before midnight EDT tomorrow,_!) )
              ( Satellite (leaf 28) (rel2par circumstance-e) (text _!when the current continuing resolution expires._!) )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 29 83) (rel2par elaboration-additional)
    ( Nucleus (span 29 65) (rel2par span)
      ( Satellite (span 29 30) (rel2par antithesis)
        ( Satellite (leaf 29) (rel2par circumstance) (text _!Given California's size and political importance,_!) )
        ( Nucleus (leaf 30) (rel2par span) (text _!the White House is eager to appear generous._!) )
      )
      ( Nucleus (span 31 65) (rel2par span)
        ( Nucleus (span 31 33) (rel2par span)
          ( Satellite (leaf 31) (rel2par attribution) (text _!But in private meetings yesterday, Budget Director Richard Darman argued_!) )
          ( Nucleus (span 32 33) (rel2par span)
            ( Nucleus (leaf 32) (rel2par span) (text _!that only $1.5 billion in new federal appropriations are needed_!) )
            ( Satellite (leaf 33) (rel2par purpose) (text _!to supplement existing resources._!) )
          )
        )
        ( Satellite (span 34 65) (rel2par explanation-argumentative)
          ( Nucleus (span 34 50) (rel2par span)
            ( Nucleus (span 34 38) (rel2par List)
              ( Satellite (leaf 34) (rel2par attribution) (text _!A White House budget office analysis estimates_!) )
              ( Nucleus (span 35 38) (rel2par span)
                ( Nucleus (span 35 37) (rel2par span)
                  ( Nucleus (span 35 36) (rel2par Same-Unit)
                    ( Nucleus (leaf 35) (rel2par span) (text _!that $500 million_!) )
                    ( Satellite (leaf 36) (rel2par interpretation-s-e) (text _!-- or half the level in the committee bill --_!) )
                  )
                  ( Nucleus (leaf 37) (rel2par Same-Unit) (text _!is needed for highway assistance_!) )
                )
                ( Satellite (leaf 38) (rel2par purpose) (text _!to meet California's needs,_!) )
              )
            )
            ( Nucleus (span 39 41) (rel2par List)
              ( Nucleus (leaf 39) (rel2par span) (text _!and the administration rejects the notion_!) )
              ( Satellite (span 40 41) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 40) (rel2par span) (text _!that new appropriations are needed_!) )
                ( Satellite (leaf 41) (rel2par purpose) (text _!to finance disaster loans by the Small Business Administration._!) )
              )
            )
            ( Nucleus (span 42 50) (rel2par List)
              ( Nucleus (span 42 47) (rel2par List)
                ( Nucleus (span 42 44) (rel2par span)
                  ( Satellite (leaf 42) (rel2par attribution) (text _!"Everybody appreciates_!) )
                  ( Nucleus (span 43 44) (rel2par span)
                    ( Nucleus (leaf 43) (rel2par List) (text _!that it is a national disaster_!) )
                    ( Nucleus (leaf 44) (rel2par List) (text _!and that we've got to address it,"_!) )
                  )
                )
                ( Satellite (span 45 47) (rel2par attribution)
                  ( Nucleus (leaf 45) (rel2par span) (text _!said Mr. Darman,_!) )
                  ( Satellite (span 46 47) (rel2par elaboration-additional-e)
                    ( Nucleus (leaf 46) (rel2par span) (text _!who came to the Capitol_!) )
                    ( Satellite (leaf 47) (rel2par purpose) (text _!to meet with Mr. Whitten and California lawmakers before the committee session._!) )
                  )
                )
              )
              ( Nucleus (span 48 50) (rel2par List)
                ( Satellite (leaf 48) (rel2par attribution) (text _!"I would hope very much_!) )
                ( Nucleus (span 49 50) (rel2par span)
                  ( Nucleus (leaf 49) (rel2par span) (text _!that we wouldn't end up in a kind of situation_!) )
                  ( Satellite (leaf 50) (rel2par elaboration-object-attribute-e) (text _!where you have a bidding war and then a veto threat."_!) )
                )
              )
            )
          )
          ( Satellite (span 51 65) (rel2par elaboration-additional)
            ( Nucleus (span 51 56) (rel2par span)
              ( Nucleus (span 51 52) (rel2par span)
                ( Satellite (leaf 51) (rel2par antithesis) (text _!Although this White House pressure was clearly a factor among committee Republicans,_!) )
                ( Nucleus (leaf 52) (rel2par span) (text _!no single influence was greater than Mr. Whitten._!) )
              )
              ( Satellite (span 53 56) (rel2par elaboration-additional)
                ( Nucleus (leaf 53) (rel2par List) (text _!A master of pork-barrel politics, he had crafted the $2.85 billion package in vintage style_!) )
                ( Nucleus (span 54 56) (rel2par List)
                  ( Nucleus (leaf 54) (rel2par span) (text _!and used the full force of his chairmanship_!) )
                  ( Satellite (span 55 56) (rel2par purpose)
                    ( Nucleus (leaf 55) (rel2par List) (text _!to keep the proposal intact_!) )
                    ( Nucleus (leaf 56) (rel2par List) (text _!and dismiss any alternative._!) )
                  )
                )
              )
            )
            ( Satellite (span 57 65) (rel2par elaboration-additional)
              ( Nucleus (span 57 61) (rel2par span)
                ( Satellite (leaf 57) (rel2par circumstance) (text _!When Mr. Fazio offered the California-backed $3.84 billion plan,_!) )
                ( Nucleus (span 58 61) (rel2par span)
                  ( Satellite (leaf 58) (rel2par attribution) (text _!Mr. Whitten insisted_!) )
                  ( Nucleus (span 59 61) (rel2par span)
                    ( Nucleus (leaf 59) (rel2par span) (text _!that the full 14 pages be read aloud by the panel's clerk_!) )
                    ( Satellite (span 60 61) (rel2par purpose)
                      ( Nucleus (leaf 60) (rel2par span) (text _!to underscore the range of legislative changes_!) )
                      ( Satellite (leaf 61) (rel2par elaboration-object-attribute-e) (text _!also sought by the delegation._!) )
                    )
                  )
                )
              )
              ( Satellite (span 62 65) (rel2par elaboration-additional)
                ( Nucleus (leaf 62) (rel2par List) (text _!On the chairman's motion, the California package was subsequently reduced to less-binding report language,_!) )
                ( Nucleus (span 63 65) (rel2par List)
                  ( Nucleus (leaf 63) (rel2par Same-Unit) (text _!and_!) )
                  ( Nucleus (span 64 65) (rel2par Same-Unit)
                    ( Satellite (leaf 64) (rel2par circumstance-e) (text _!even when this was accepted as such on a voice vote,_!) )
                    ( Nucleus (leaf 65) (rel2par span) (text _!Mr. Whitten pointedly opposed it._!) )
                  )
                )
              )
            )
          )
        )
      )
    )
    ( Satellite (span 66 83) (rel2par circumstance)
      ( Nucleus (span 66 69) (rel2par span)
        ( Nucleus (leaf 66) (rel2par span) (text _!More important than money in many cases are waivers_!) )
        ( Satellite (span 67 69) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 67) (rel2par span) (text _!California is seeking on current restrictions_!) )
          ( Satellite (span 68 69) (rel2par elaboration-object-attribute-e)
            ( Nucleus (leaf 68) (rel2par span) (text _!covering federal highway funds, such as a $100 million cap_!) )
            ( Satellite (leaf 69) (rel2par elaboration-object-attribute-e) (text _!on how much any single state can receive in emergency funds in a year._!) )
          )
        )
      )
      ( Satellite (span 70 83) (rel2par elaboration-additional)
        ( Nucleus (span 70 73) (rel2par span)
          ( Nucleus (leaf 70) (rel2par Contrast) (text _!Mr. Whitten's package appears to accomplish this purpose,_!) )
          ( Nucleus (span 71 73) (rel2par Contrast)
            ( Nucleus (leaf 71) (rel2par span) (text _!but the state faces more resistance in its bid for an extended waiver_!) )
            ( Satellite (span 72 73) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 72) (rel2par span) (text _!on having to put up any matching funds on repairs_!) )
              ( Satellite (leaf 73) (rel2par elaboration-object-attribute-e) (text _!completed in the next six months._!) )
            )
          )
        )
        ( Satellite (span 74 83) (rel2par elaboration-additional)
          ( Nucleus (span 74 76) (rel2par span)
            ( Nucleus (leaf 74) (rel2par span) (text _!A member in the House leadership and skilled legislator, Mr. Fazio nonetheless found himself burdened not only by California's needs but by Hurricane Hugo amendments_!) )
            ( Satellite (span 75 76) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 75) (rel2par span) (text _!he accepted in a vain effort_!) )
              ( Satellite (leaf 76) (rel2par purpose) (text _!to build support in the panel._!) )
            )
          )
          ( Satellite (span 77 83) (rel2par elaboration-additional)
            ( Nucleus (span 77 80) (rel2par span)
              ( Nucleus (span 77 78) (rel2par List)
                ( Nucleus (leaf 77) (rel2par span) (text _!The California Democrat appeared embarrassed by provisions_!) )
                ( Satellite (leaf 78) (rel2par elaboration-object-attribute-e) (text _!inserted on behalf of owners of private beaches in the Virgin Islands,_!) )
              )
              ( Nucleus (span 79 80) (rel2par List)
                ( Nucleus (leaf 79) (rel2par span) (text _!and lumber interests sought to add another $100 million in federal aid_!) )
                ( Satellite (leaf 80) (rel2par elaboration-object-attribute-e) (text _!to plant timber on private land in North and South Carolina._!) )
              )
            )
            ( Satellite (span 81 83) (rel2par elaboration-additional)
              ( Nucleus (leaf 81) (rel2par span) (text _!California's high-priced real estate puts it in an awkward position, too._!) )
              ( Satellite (span 82 83) (rel2par elaboration-additional)
                ( Nucleus (leaf 82) (rel2par span) (text _!One provision last night would have raised the cap on disaster loans to $500,000 from $100,000 per household_!) )
                ( Satellite (leaf 83) (rel2par purpose) (text _!to accommodate San Francisco losses._!) )
              )
            )
          )
        )
      )
    )
  )
)
