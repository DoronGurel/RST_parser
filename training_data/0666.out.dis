                  ( Nucleus (span 36 38) (rel2par List)
                    ( Satellite (leaf 36) (rel2par attribution) (text _!International Paper emphasizes_!) )
                    ( Nucleus (span 37 38) (rel2par span)
                      ( Nucleus (leaf 37) (rel2par span) (text _!that it is better positioned than most companies for the coming overcapacity_!) )
                      ( Satellite (leaf 38) (rel2par result) (text _!because its individual mills can make more than one grade of paper._!) )
                    )
                  )
                  ( Nucleus (span 39 42) (rel2par List)
                    ( Nucleus (leaf 39) (rel2par span) (text _!A Boise-Cascade spokesman referred to a speech by Chairman John Fery,_!) )
                    ( Satellite (span 40 42) (rel2par elaboration-object-attribute-e)
                      ( Satellite (leaf 40) (rel2par attribution) (text _!in which he said_!) )
                      ( Nucleus (span 41 42) (rel2par span)
                        ( Nucleus (leaf 41) (rel2par span) (text _!that markets generally are stable,_!) )
                        ( Satellite (leaf 42) (rel2par antithesis) (text _!although some risk of further price deterioration exists._!) )
                      )
                    )
