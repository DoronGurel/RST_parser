  ( Nucleus (span 78 104) (rel2par TextualOrganization)
    ( Nucleus (span 78 102) (rel2par TextualOrganization)
      ( Nucleus (span 78 95) (rel2par span)
        ( Nucleus (leaf 78) (rel2par span) (text _!Your story is an insult to the citizens of the Mississippi Delta._!) )
        ( Satellite (span 79 95) (rel2par explanation-argumentative)
          ( Nucleus (span 79 86) (rel2par span)
            ( Nucleus (span 79 82) (rel2par span)
              ( Nucleus (span 79 81) (rel2par span)
                ( Nucleus (span 79 80) (rel2par Same-Unit)
                  ( Nucleus (leaf 79) (rel2par span) (text _!Many of the problems_!) )
                  ( Satellite (leaf 80) (rel2par elaboration-object-attribute-e) (text _!you presented_!) )
                )
                ( Nucleus (leaf 81) (rel2par Same-Unit) (text _!exist in every part of this country._!) )
              )
              ( Satellite (leaf 82) (rel2par evidence) (text _!Poverty is only two blocks from President Bush's residence._!) )
            )
            ( Satellite (span 83 86) (rel2par elaboration-additional)
              ( Nucleus (leaf 83) (rel2par List) (text _!For years, we tried to ignore the problem of poverty,_!) )
              ( Nucleus (span 84 86) (rel2par List)
                ( Nucleus (leaf 84) (rel2par Same-Unit) (text _!and_!) )
                ( Nucleus (span 85 86) (rel2par Same-Unit)
                  ( Satellite (leaf 85) (rel2par result) (text _!now that it has gotten out of hand_!) )
                  ( Nucleus (leaf 86) (rel2par span) (text _!it's a new crusade for the media and our Democratic Congress._!) )
                )
              )
            )
          )
          ( Satellite (span 87 95) (rel2par elaboration-additional)
            ( Nucleus (leaf 87) (rel2par List) (text _!Nobody should have to live in such poor conditions as in "Sugar Ditch,"_!) )
            ( Nucleus (span 88 95) (rel2par List)
              ( Nucleus (span 88 90) (rel2par span)
                ( Nucleus (leaf 88) (rel2par Same-Unit) (text _!but_!) )
                ( Nucleus (span 89 90) (rel2par Same-Unit)
                  ( Satellite (leaf 89) (rel2par circumstance) (text _!when you travel to Washington, Boston, Chicago or New York,_!) )
                  ( Nucleus (leaf 90) (rel2par span) (text _!the same problems exist._!) )
                )
              )
              ( Satellite (span 91 95) (rel2par evidence)
                ( Nucleus (leaf 91) (rel2par span) (text _!The only difference is, in those cities the poor are housed in high-rise-project apartments_!) )
                ( Satellite (span 92 95) (rel2par elaboration-additional-e)
                  ( Nucleus (span 92 93) (rel2par List)
                    ( Nucleus (leaf 92) (rel2par span) (text _!each consisting of one room, with rusty pipes_!) )
                    ( Satellite (leaf 93) (rel2par definition) (text _!called plumbing,_!) )
                  )
                  ( Nucleus (leaf 94) (rel2par List) (text _!rodents and cockroaches everywhere and nonworking elevators_!) )
                  ( Nucleus (leaf 95) (rel2par List) (text _!-- and with the building patrolled by gangs and drug dealers._!) )
                )
              )
            )
          )
        )
      )
      ( Satellite (span 96 102) (rel2par evaluation-s)
        ( Nucleus (span 96 99) (rel2par Contrast)
          ( Satellite (leaf 96) (rel2par circumstance) (text _!Many middle-class people would love free food, Medicaid insurance, utilities and rent._!) )
          ( Nucleus (span 97 99) (rel2par span)
            ( Nucleus (leaf 97) (rel2par List) (text _!Then maybe I could stay home_!) )
            ( Nucleus (leaf 98) (rel2par List) (text _!and have seven children_!) )
            ( Nucleus (leaf 99) (rel2par List) (text _!and watch Oprah Winfrey, like Beulah in the article,_!) )
          )
        )
        ( Nucleus (span 100 102) (rel2par Contrast)
          ( Nucleus (leaf 100) (rel2par List) (text _!instead of having one child_!) )
          ( Nucleus (span 101 102) (rel2par List)
            ( Nucleus (leaf 101) (rel2par span) (text _!and working constantly_!) )
            ( Satellite (leaf 102) (rel2par purpose) (text _!just to stay above water, like so many families in this country._!) )
          )
        )
      )
