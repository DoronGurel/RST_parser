( Root (span 1 62)
  ( Nucleus (span 1 53) (rel2par span)
    ( Nucleus (span 1 33) (rel2par span)
      ( Nucleus (span 1 25) (rel2par span)
        ( Nucleus (span 1 18) (rel2par span)
          ( Nucleus (span 1 5) (rel2par span)
            ( Nucleus (span 1 2) (rel2par Same-Unit)
              ( Nucleus (leaf 1) (rel2par span) (text _!Time magazine, in a move_!) )
              ( Satellite (leaf 2) (rel2par elaboration-object-attribute-e) (text _!to reduce the costs of wooing new subscribers,_!) )
            )
            ( Nucleus (span 3 5) (rel2par Same-Unit)
              ( Nucleus (leaf 3) (rel2par List) (text _!is lowering its circulation guarantee to advertisers for the second consecutive year,_!) )
              ( Nucleus (leaf 4) (rel2par List) (text _!increasing its subscription rates_!) )
              ( Nucleus (leaf 5) (rel2par List) (text _!and cutting back on merchandise giveaways._!) )
            )
          )
          ( Satellite (span 6 18) (rel2par elaboration-additional)
            ( Nucleus (span 6 11) (rel2par List)
              ( Satellite (leaf 6) (rel2par attribution) (text _!In an announcement to its staff last week, executives at Time Warner Inc. 's weekly magazine said_!) )
              ( Nucleus (span 7 11) (rel2par span)
                ( Nucleus (leaf 7) (rel2par List) (text _!Time will "dramatically de-emphasize" its use of electronic giveaways such as telephones in television subscription drives;_!) )
                ( Nucleus (span 8 10) (rel2par List)
                  ( Nucleus (span 8 9) (rel2par Same-Unit)
                    ( Nucleus (leaf 8) (rel2par span) (text _!cut the circulation_!) )
                    ( Satellite (leaf 9) (rel2par elaboration-object-attribute-e) (text _!it guarantees advertisers_!) )
                  )
                  ( Nucleus (leaf 10) (rel2par Same-Unit) (text _!by 300,000, to four million;_!) )
                )
                ( Nucleus (leaf 11) (rel2par List) (text _!and increase the cost of its annual subscription rate by about $4 to $55._!) )
              )
            )
            ( Nucleus (span 12 18) (rel2par List)
              ( Satellite (span 12 14) (rel2par antithesis)
                ( Nucleus (span 12 13) (rel2par span)
                  ( Satellite (leaf 12) (rel2par attribution) (text _!In a related development, the news-weekly, for the fourth year in a row, said_!) )
                  ( Nucleus (leaf 13) (rel2par span) (text _!it won't increase its advertising rates in 1990;_!) )
                )
                ( Satellite (leaf 14) (rel2par elaboration-additional) (text _!a full, four-color page in the magazine costs about $120,000._!) )
              )
              ( Nucleus (span 15 18) (rel2par span)
                ( Nucleus (span 15 17) (rel2par span)
                  ( Nucleus (leaf 15) (rel2par Same-Unit) (text _!However,_!) )
                  ( Nucleus (span 16 17) (rel2par Same-Unit)
                    ( Satellite (leaf 16) (rel2par result-e) (text _!because the guaranteed circulation base is being lowered,_!) )
                    ( Nucleus (leaf 17) (rel2par span) (text _!ad rates will be effectively 7.5% higher per subscriber,_!) )
                  )
                )
                ( Satellite (leaf 18) (rel2par attribution) (text _!according to Richard Heinemann, Time associate publisher._!) )
              )
            )
          )
        )
        ( Satellite (span 19 25) (rel2par background)
          ( Nucleus (span 19 22) (rel2par span)
            ( Nucleus (leaf 19) (rel2par span) (text _!Time is following the course of some other mass-circulation magazines_!) )
            ( Satellite (span 20 22) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 20) (rel2par span) (text _!that in recent years have challenged the publishing myth_!) )
              ( Satellite (span 21 22) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 21) (rel2par span) (text _!that maintaining artificially high, and expensive, circulations is the way_!) )
                ( Satellite (leaf 22) (rel2par elaboration-object-attribute-e) (text _!to draw advertisers._!) )
              )
            )
          )
          ( Satellite (span 23 25) (rel2par example)
            ( Nucleus (leaf 23) (rel2par span) (text _!In recent years, Reader's Digest, New York Times Co. 's McCall's, and most recently News Corp. 's TV Guide, have cut their massive circulation rate bases_!) )
            ( Satellite (span 24 25) (rel2par purpose)
              ( Nucleus (leaf 24) (rel2par List) (text _!to eliminate marginal circulation_!) )
              ( Nucleus (leaf 25) (rel2par List) (text _!and hold down rates for advertisers._!) )
            )
          )
        )
      )
      ( Satellite (span 26 33) (rel2par circumstance)
        ( Nucleus (leaf 26) (rel2par span) (text _!Deep discounts in subscriptions and offers of free clock radios and watches have become accepted forms_!) )
        ( Satellite (span 27 33) (rel2par elaboration-object-attribute-e)
          ( Nucleus (span 27 28) (rel2par span)
            ( Satellite (leaf 27) (rel2par antithesis) (text _!of attracting new subscribers in the hyper-competitive world of magazine news-weeklies._!) )
            ( Nucleus (leaf 28) (rel2par span) (text _!But Time, as part of the more cost-conscious Time Warner, wants to wean itself away from expensive gimmicks._!) )
          )
          ( Satellite (span 29 33) (rel2par elaboration-additional)
            ( Nucleus (span 29 30) (rel2par span)
              ( Satellite (leaf 29) (rel2par attribution) (text _!Besides, Time executives think_!) )
              ( Nucleus (leaf 30) (rel2par span) (text _!selling a news magazine with a clock radio is tacky._!) )
            )
            ( Satellite (span 31 33) (rel2par elaboration-additional)
              ( Nucleus (span 31 32) (rel2par span)
                ( Nucleus (leaf 31) (rel2par span) (text _!"Giveaways just give people the wrong image,"_!) )
                ( Satellite (leaf 32) (rel2par attribution) (text _!said Mr. Heinemann._!) )
              )
              ( Satellite (leaf 33) (rel2par elaboration-additional) (text _!"That perception takes the focus off the magazine."_!) )
            )
          )
        )
      )
    )
    ( Satellite (span 34 53) (rel2par evaluation-s)
      ( Nucleus (span 34 45) (rel2par span)
        ( Satellite (span 34 39) (rel2par concession)
          ( Nucleus (leaf 34) (rel2par span) (text _!Time magazine executives predictably paint the circulation cut as a show of strength and actually a benefit to advertisers._!) )
          ( Satellite (span 35 39) (rel2par explanation-argumentative)
            ( Nucleus (span 35 38) (rel2par span)
              ( Nucleus (span 35 37) (rel2par span)
                ( Nucleus (leaf 35) (rel2par span) (text _!"What we are doing is screening out the readers_!) )
                ( Satellite (span 36 37) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 36) (rel2par List) (text _!who are only casually related to the magazine_!) )
                  ( Nucleus (leaf 37) (rel2par List) (text _!and don't really read it,"_!) )
                )
              )
              ( Satellite (leaf 38) (rel2par attribution) (text _!said Mr. Heinemann._!) )
            )
            ( Satellite (leaf 39) (rel2par elaboration-additional) (text _!"We are trying to create quality and involvement."_!) )
          )
        )
        ( Nucleus (span 40 45) (rel2par span)
          ( Nucleus (span 40 41) (rel2par span)
            ( Nucleus (leaf 40) (rel2par span) (text _!However, Time executives used the same explanation_!) )
            ( Satellite (leaf 41) (rel2par circumstance) (text _!when in October 1988 the magazine cut its guaranteed circulation from 4.6 million to 4.3 million._!) )
          )
          ( Satellite (span 42 45) (rel2par elaboration-additional)
            ( Nucleus (span 42 43) (rel2par Same-Unit)
              ( Nucleus (leaf 42) (rel2par span) (text _!And Time's paid circulation,_!) )
              ( Satellite (leaf 43) (rel2par attribution-e) (text _!according to Audit Bureau of Circulations,_!) )
            )
            ( Nucleus (span 44 45) (rel2par Same-Unit)
              ( Nucleus (leaf 44) (rel2par span) (text _!dropped 7.3% to 4,393,237 in the six months_!) )
              ( Satellite (leaf 45) (rel2par elaboration-object-attribute-e) (text _!ended June 30, 1989._!) )
            )
          )
        )
      )
      ( Satellite (span 46 53) (rel2par elaboration-additional)
        ( Nucleus (leaf 46) (rel2par span) (text _!Still, Time's move is being received well, once again._!) )
        ( Satellite (span 47 53) (rel2par elaboration-additional)
          ( Nucleus (span 47 49) (rel2par span)
            ( Nucleus (span 47 48) (rel2par span)
              ( Satellite (leaf 47) (rel2par attribution) (text _!"It's terrific for advertisers to know_!) )
              ( Nucleus (leaf 48) (rel2par span) (text _!the reader will be paying more,"_!) )
            )
            ( Satellite (leaf 49) (rel2par attribution) (text _!said Michael Drexler, national media director at Bozell Inc. ad agency._!) )
          )
          ( Satellite (span 50 53) (rel2par elaboration-additional)
            ( Nucleus (leaf 50) (rel2par span) (text _!"A few drops in circulation are of no consequence._!) )
            ( Satellite (span 51 53) (rel2par elaboration-additional)
              ( Nucleus (leaf 51) (rel2par List) (text _!It's not a show of weakness;_!) )
              ( Nucleus (span 52 53) (rel2par List)
                ( Nucleus (leaf 52) (rel2par span) (text _!they are improving the quality of circulation_!) )
                ( Satellite (leaf 53) (rel2par circumstance) (text _!while insuring their profits."_!) )
              )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 54 62) (rel2par elaboration-additional)
    ( Nucleus (span 54 59) (rel2par Contrast)
      ( Nucleus (span 54 57) (rel2par span)
        ( Satellite (leaf 54) (rel2par attribution) (text _!Mr. Heinemann said_!) )
        ( Nucleus (span 55 57) (rel2par span)
          ( Nucleus (leaf 55) (rel2par span) (text _!the changes represent a new focus in the magazine industry:_!) )
          ( Satellite (span 56 57) (rel2par elaboration-general-specific-e)
            ( Nucleus (leaf 56) (rel2par span) (text _!a magazine's net revenue per subscriber, or the actual revenue from subscribers_!) )
            ( Satellite (leaf 57) (rel2par circumstance) (text _!after discounts and the cost of premiums have been stripped away._!) )
          )
        )
      )
      ( Satellite (span 58 59) (rel2par elaboration-additional)
        ( Nucleus (leaf 58) (rel2par span) (text _!"The question is how much are we getting from each reader,"_!) )
        ( Satellite (leaf 59) (rel2par attribution) (text _!said Mr. Heinemann._!) )
      )
    )
    ( Nucleus (span 60 62) (rel2par Contrast)
      ( Nucleus (span 60 61) (rel2par span)
        ( Nucleus (leaf 60) (rel2par span) (text _!Time's rivals news-weeklies, Washington Post Co. 's Newsweek and U.S. News & World Report, are less reliant on electronic giveaways,_!) )
        ( Satellite (leaf 61) (rel2par elaboration-additional) (text _!and in recent years both have been increasing their circulation rate bases._!) )
      )
      ( Satellite (leaf 62) (rel2par elaboration-additional) (text _!Both magazines are expected to announce their ad rates and circulation levels for 1990 within a month._!) )
    )
  )
)
