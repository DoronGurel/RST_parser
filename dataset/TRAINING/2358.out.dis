              ( Nucleus (span 20 24) (rel2par span)
                ( Satellite (span 20 22) (rel2par circumstance)
                  ( Nucleus (leaf 20) (rel2par span) (text _!Further fueling the belief_!) )
                  ( Satellite (span 21 22) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 21) (rel2par span) (text _!that pressures in the economy were sufficient_!) )
