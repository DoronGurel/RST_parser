        ( Nucleus (span 64 68) (rel2par Comparison)
          ( Nucleus (span 64 65) (rel2par span)
            ( Nucleus (leaf 64) (rel2par span) (text _!On Friday, the American Diabetes Association issued a similar statement_!) )
            ( Satellite (leaf 65) (rel2par elaboration-object-attribute-e) (text _!urging the six million U.S. diabetics not to overreact to the British report._!) )
          )
          ( Satellite (span 66 68) (rel2par elaboration-additional)
            ( Nucleus (span 66 67) (rel2par span)
              ( Nucleus (leaf 66) (rel2par span) (text _!"A loss of the warning symptoms of hypoglycemia is a complex problem_!) )
              ( Satellite (leaf 67) (rel2par elaboration-object-attribute-e) (text _!that is very unlikely to be due simply to the type of insulin used,"_!) )
            )
            ( Satellite (leaf 68) (rel2par attribution) (text _!the American association said._!) )
