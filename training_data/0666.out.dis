                  ( Nucleus (span 43 45) (rel2par List)
                    ( Nucleus (span 43 44) (rel2par span)
                      ( Satellite (leaf 43) (rel2par attribution) (text _!Stone Container Chairman Roger Stone said_!) )
                      ( Nucleus (leaf 44) (rel2par span) (text _!that, unlike for some other paper products, demand for Stone's principal commodity, unbleached containerboard, remains strong._!) )
                    )
                    ( Satellite (leaf 45) (rel2par elaboration-additional) (text _!He expects the price for that product to rise even more next year._!) )
                  )
                  ( Nucleus (span 46 48) (rel2par List)
                    ( Satellite (leaf 46) (rel2par attribution) (text _!Gaylord Container said_!) )
                    ( Nucleus (span 47 48) (rel2par span)
                      ( Nucleus (leaf 47) (rel2par span) (text _!analysts are skeptical of it_!) )
                      ( Satellite (leaf 48) (rel2par reason) (text _!because it's carrying a lot of debt._!) )
                    )
                  )
                  ( Nucleus (span 49 52) (rel2par List)
                    ( Satellite (leaf 49) (rel2par attribution) (text _!Champion International said,_!) )
                    ( Nucleus (span 50 52) (rel2par span)
                      ( Satellite (leaf 50) (rel2par circumstance) (text _!"We've gotten our costs down_!) )
                      ( Nucleus (span 51 52) (rel2par span)
                        ( Nucleus (leaf 51) (rel2par Comparison) (text _!and we're better positioned for any cyclical downturn_!) )
                        ( Nucleus (leaf 52) (rel2par Comparison) (text _!than we've ever been."_!) )
                      )
                    )
                  )
                  ( Nucleus (span 53 55) (rel2par List)
                    ( Satellite (leaf 53) (rel2par attribution) (text _!Louisiana Pacific and Georgia Pacific said_!) )
                    ( Nucleus (span 54 55) (rel2par span)
                      ( Nucleus (leaf 54) (rel2par span) (text _!a number of other analysts are recommending them_!) )
                      ( Satellite (leaf 55) (rel2par reason) (text _!because of their strong wood-products business._!) )
                    )
                  )
                  ( Nucleus (span 56 61) (rel2par List)
                    ( Nucleus (span 56 57) (rel2par span)
                      ( Satellite (leaf 56) (rel2par attribution) (text _!Federal Paper Board said,_!) )
                      ( Nucleus (leaf 57) (rel2par span) (text _!"We're not as exposed as the popular perception of us."_!) )
                    )
                    ( Satellite (span 58 61) (rel2par explanation-argumentative)
                      ( Satellite (leaf 58) (rel2par attribution) (text _!The company explained_!) )
                      ( Nucleus (span 59 61) (rel2par span)
                        ( Nucleus (span 59 60) (rel2par Same-Unit)
                          ( Nucleus (leaf 59) (rel2par span) (text _!that its main product, bleached paperboard,_!) )
                          ( Satellite (leaf 60) (rel2par elaboration-additional-e) (text _!which goes into some advertising materials and white boxes,_!) )
                        )
                        ( Nucleus (leaf 61) (rel2par Same-Unit) (text _!historically doesn't have sharp price swings._!) )
                      )
                    )
                  )
