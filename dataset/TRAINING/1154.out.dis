      ( Nucleus (leaf 146) (rel2par Contrast) (text _!The Tokyo International Film Festival was no match for the Cannes Film Festival in terms of prestige,_!) )
      ( Nucleus (span 147 156) (rel2par Contrast)
        ( Nucleus (leaf 147) (rel2par span) (text _!but it made its mark:_!) )
        ( Satellite (span 148 156) (rel2par explanation-argumentative)
          ( Nucleus (leaf 148) (rel2par span) (text _!It awarded the largest cash prize of any film festival to young and first-time film makers._!) )
          ( Satellite (span 149 156) (rel2par explanation-argumentative)
            ( Nucleus (span 149 151) (rel2par span)
              ( Nucleus (span 149 150) (rel2par Comparison)
                ( Nucleus (leaf 149) (rel2par span) (text _!At this year's event, the third since the festival got under way in 1985, Idrissa Ouedraogo of Burkina Faso won the Sakura Gold prize of $143,000 for "Yaaba"_!) )
                ( Satellite (leaf 150) (rel2par interpretation-s-e) (text _!("Old Woman")._!) )
              )
              ( Nucleus (leaf 151) (rel2par Comparison) (text _!By comparison, Cannes now gives $39,000 to the winner of its young director's award._!) )
            )
            ( Satellite (span 152 156) (rel2par elaboration-additional)
              ( Satellite (span 152 153) (rel2par attribution)
                ( Nucleus (leaf 152) (rel2par span) (text _!Says director George Miller_!) )
                ( Satellite (leaf 153) (rel2par elaboration-additional-e) (text _!("Mad Max"):_!) )
              )
              ( Nucleus (span 154 156) (rel2par span)
                ( Satellite (leaf 154) (rel2par attribution) (text _!"I think_!) )
                ( Nucleus (span 155 156) (rel2par span)
                  ( Nucleus (leaf 155) (rel2par Cause-Result) (text _!the Tokyo festival may become known as a major attraction for young directors_!) )
                  ( Nucleus (leaf 156) (rel2par Cause-Result) (text _!because of the money as well as the recognition."_!) )
                )
              )
            )
          )
        )
      )
    )
    ( Satellite (span 157 160) (rel2par evaluation-s)
      ( Nucleus (leaf 157) (rel2par span) (text _!There are drawbacks._!) )
      ( Satellite (span 158 160) (rel2par explanation-argumentative)
        ( Satellite (leaf 158) (rel2par attribution) (text _!Vincent Tolentino, a correspondent for the French magazine Telerama, says of the recently ended Tokyo festival:_!) )
        ( Nucleus (span 159 160) (rel2par span)
          ( Nucleus (leaf 159) (rel2par List) (text _!"No one makes deals . . ._!) )
          ( Nucleus (leaf 160) (rel2par List) (text _!and most of the films have been seen before at other festivals."_!) )
        )
      )
    )
  )
