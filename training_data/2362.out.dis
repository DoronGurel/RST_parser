                        ( Satellite (leaf 38) (rel2par attribution) (text _!the source indicated._!) )
                      )
                      ( Satellite (span 39 43) (rel2par reason)
                        ( Nucleus (leaf 39) (rel2par span) (text _!GEC plans first to study Ferranti's financial accounts,_!) )
                        ( Satellite (span 40 43) (rel2par elaboration-object-attribute-e)
                          ( Nucleus (span 40 41) (rel2par Same-Unit)
