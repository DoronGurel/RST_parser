        ( Nucleus (span 3 12) (rel2par span)
          ( Nucleus (span 3 10) (rel2par span)
            ( Satellite (span 3 6) (rel2par concession)
              ( Satellite (leaf 3) (rel2par circumstance) (text _!As the good times rolled_!) )
              ( Nucleus (span 4 6) (rel2par span)
                ( Nucleus (span 4 5) (rel2par Same-Unit)
                  ( Nucleus (leaf 4) (rel2par span) (text _!they more than doubled their prices for pulp, a raw material_!) )
                  ( Satellite (leaf 5) (rel2par elaboration-object-attribute-e) (text _!used in all sorts of paper,_!) )
                )
                ( Nucleus (leaf 6) (rel2par Same-Unit) (text _!to $830 a metric ton this past spring from $380 a ton at the start of 1986._!) )
              )
            )
            ( Nucleus (span 7 10) (rel2par span)
              ( Nucleus (span 7 9) (rel2par Cause-Result)
                ( Nucleus (leaf 7) (rel2par Cause-Result) (text _!But now the companies are getting into trouble_!) )
                ( Nucleus (span 8 9) (rel2par Cause-Result)
                  ( Nucleus (leaf 8) (rel2par Temporal-Same-Time) (text _!because they undertook a record expansion program_!) )
                  ( Nucleus (leaf 9) (rel2par Temporal-Same-Time) (text _!while they were raising prices sharply._!) )
                )
              )
              ( Nucleus (leaf 10) (rel2par Cause-Result) (text _!Third-quarter profits fell at several companies._!) )
            )
          )
          ( Satellite (span 11 12) (rel2par comment)
            ( Nucleus (leaf 11) (rel2par span) (text _!"Put your money in a good utility or bank stock, not a paper company,"_!) )
            ( Satellite (leaf 12) (rel2par attribution) (text _!advises George Adler of Smith Barney._!) )
          )
        )
        ( Satellite (span 13 69) (rel2par evaluation-s)
          ( Nucleus (span 13 61) (rel2par span)
            ( Nucleus (span 13 25) (rel2par List)
              ( Nucleus (span 13 17) (rel2par Statement-Response)
                ( Nucleus (leaf 13) (rel2par span) (text _!Other analysts are nearly as pessimistic._!) )
                ( Satellite (span 14 17) (rel2par example)
                  ( Nucleus (span 14 15) (rel2par List)
                    ( Nucleus (leaf 14) (rel2par span) (text _!Gary Palmero of Oppenheimer & Co. expects a 30% decline in earnings between now and 1991 for "commodity-oriented" paper companies,_!) )
                    ( Satellite (leaf 15) (rel2par elaboration-additional-e) (text _!which account for the majority of the industry._!) )
                  )
                  ( Nucleus (span 16 17) (rel2par List)
                    ( Nucleus (leaf 16) (rel2par span) (text _!Robert Schneider of Duff & Phelps sees paper-company stock prices falling 10% to 15% in 1990, perhaps 25%_!) )
                    ( Satellite (leaf 17) (rel2par condition) (text _!if there's a recession._!) )
                  )
                )
              )
              ( Nucleus (span 18 25) (rel2par Statement-Response)
                ( Nucleus (leaf 18) (rel2par span) (text _!Paper companies concede that business has been off recently._!) )
                ( Satellite (span 19 25) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 19) (rel2par List) (text _!But they attribute much of the weakness to customer inventory reductions._!) )
                  ( Nucleus (span 20 25) (rel2par List)
                    ( Nucleus (span 20 23) (rel2par span)
                      ( Satellite (leaf 20) (rel2par attribution) (text _!Generally they maintain_!) )
                      ( Nucleus (span 21 23) (rel2par span)
                        ( Nucleus (leaf 21) (rel2par Same-Unit) (text _!that,_!) )
                        ( Nucleus (span 22 23) (rel2par Same-Unit)
                          ( Satellite (leaf 22) (rel2par circumstance) (text _!barring a recession and a further strengthening of the dollar against foreign currencies,_!) )
                          ( Nucleus (leaf 23) (rel2par span) (text _!the industry isn't headed for a prolonged slump._!) )
                        )
                      )
                    )
                    ( Satellite (span 24 25) (rel2par elaboration-additional)
                      ( Nucleus (leaf 24) (rel2par span) (text _!"It won't be an earthshaking drop,"_!) )
                      ( Satellite (leaf 25) (rel2par attribution) (text _!a Weyerhaeuser spokesman says._!) )
                    )
                  )
                )
              )
            )
            ( Nucleus (span 26 61) (rel2par List)
              ( Nucleus (span 26 29) (rel2par span)
                ( Nucleus (leaf 26) (rel2par List) (text _!Last week Mr. Adler lowered his rating from hold to "avoid" on Boise Cascade, Champion International, Great Northern Nekoosa, International Paper, Louisiana Pacific and Weyerhaeuser._!) )
                ( Nucleus (leaf 27) (rel2par List) (text _!Oppenheimer's Mr. Palmero, meanwhile, is steering clear of Gaylord Container, Stone Container and Federal Paper Board._!) )
                ( Nucleus (leaf 28) (rel2par List) (text _!Mr. Schneider is cool to Georgia Pacific and Abitibi-Price._!) )
                ( Nucleus (leaf 29) (rel2par List) (text _!Lawrence Ross of PaineWebber would avoid Union Camp._!) )
              )
              ( Satellite (span 30 61) (rel2par evaluation-s)
                ( Nucleus (span 30 31) (rel2par span)
                  ( Satellite (leaf 30) (rel2par attribution) (text _!The companies in question believe_!) )
                  ( Nucleus (leaf 31) (rel2par span) (text _!the analysts are too pessimistic._!) )
