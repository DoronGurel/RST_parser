          ( Satellite (span 3 6) (rel2par background)
            ( Nucleus (span 3 4) (rel2par span)
              ( Nucleus (leaf 3) (rel2par span) (text _!Mr. Johnson has been a managing director at Shearson Lehman Hutton since 1985,_!) )
              ( Satellite (leaf 4) (rel2par temporal-before) (text _!and before that was president of Public Strategies, a Washington consulting firm._!) )
            )
            ( Satellite (span 5 6) (rel2par elaboration-additional)
              ( Nucleus (leaf 5) (rel2par span) (text _!He is well-known in Democratic circles,_!) )
              ( Satellite (leaf 6) (rel2par reason) (text _!having been executive assistant to Vice President Walter Mondale and chairman of Mr. Mondale's 1984 presidential campaign._!) )
            )
          )
          ( Nucleus (span 7 14) (rel2par span)
            ( Nucleus (span 7 8) (rel2par span)
              ( Nucleus (leaf 7) (rel2par List) (text _!At Fannie Mae, he will take responsibility for the corporation's financial and legal areas_!) )
              ( Nucleus (leaf 8) (rel2par List) (text _!and will work with David Maxwell, chairman and chief executive officer, and Roger Birk, president and chief operating officer, on strategic planning._!) )
            )
            ( Satellite (span 9 14) (rel2par elaboration-additional)
              ( Satellite (leaf 9) (rel2par background) (text _!Mr. Johnson, 45 years old, has been a consultant on strategy to Fannie Mae for the past 3 1/2 years._!) )
              ( Nucleus (span 10 14) (rel2par span)
                ( Nucleus (span 10 12) (rel2par span)
                  ( Satellite (leaf 10) (rel2par attribution) (text _!In an interview, he said_!) )
                  ( Nucleus (span 11 12) (rel2par span)
                    ( Nucleus (leaf 11) (rel2par span) (text _!Fannie Mae faces a number of challenges with the restructuring of the thrift industry and the push_!) )
                    ( Satellite (leaf 12) (rel2par elaboration-object-attribute-e) (text _!to broaden its activities overseas._!) )
                  )
                )
                ( Satellite (span 13 14) (rel2par restatement)
                  ( Nucleus (leaf 13) (rel2par span) (text _!"There's no shortage of major things to do,"_!) )
                  ( Satellite (leaf 14) (rel2par attribution) (text _!he said._!) )
                )
              )
            )
          )
        )
        ( Nucleus (span 15 17) (rel2par List)
          ( Satellite (leaf 15) (rel2par attribution) (text _!Fannie Mae also said_!) )
          ( Nucleus (span 16 17) (rel2par span)
            ( Nucleus (leaf 16) (rel2par span) (text _!James A. Aliber, chairman of First Federal of Michigan and a director since 1985, moved up the date of his retirement from the board_!) )
            ( Satellite (leaf 17) (rel2par purpose) (text _!to accommodate Mr. Johnson's election as a director._!) )
