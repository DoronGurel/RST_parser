                ( Satellite (leaf 110) (rel2par elaboration-object-attribute-e) (text _!the investor owns._!) )
              )
              ( Satellite (span 111 113) (rel2par elaboration-additional-e)
                ( Nucleus (leaf 111) (rel2par span) (text _!A call option is similar to a put,_!) )
