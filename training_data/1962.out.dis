                            )
                            ( Satellite (span 61 67) (rel2par example)
                              ( Nucleus (span 61 66) (rel2par span)
                                ( Nucleus (span 61 63) (rel2par span)
                                  ( Nucleus (leaf 61) (rel2par span) (text _!For example, Tenneco Inc. pays a quarterly dividend of 76 cents,_!) )
                                  ( Satellite (span 62 63) (rel2par elaboration-object-attribute-e)
                                    ( Nucleus (leaf 62) (rel2par span) (text _!which would be received_!) )
                                    ( Satellite (leaf 63) (rel2par temporal-before) (text _!before the February option expires_!) )
                                  )
                                )
                                ( Satellite (span 64 66) (rel2par consequence-s)
                                  ( Nucleus (span 64 65) (rel2par Same-Unit)
                                    ( Nucleus (leaf 64) (rel2par span) (text _!and, thus, reduce the cost_!) )
                                    ( Satellite (leaf 65) (rel2par elaboration-object-attribute-e) (text _!of using the technique_!) )
                                  )
                                  ( Nucleus (leaf 66) (rel2par Same-Unit) (text _!by that amount._!) )
                                )
                              )
                              ( Satellite (leaf 67) (rel2par elaboration-additional) (text _!In this case, the investor's risk wouldn't exceed 3.6% of the total investment._!) )
                            )
                          )
                        )
                        ( Satellite (span 68 69) (rel2par elaboration-additional)
                          ( Satellite (leaf 68) (rel2par purpose) (text _!To simplify the calculations,_!) )
                          ( Nucleus (leaf 69) (rel2par span) (text _!commissions on the option and underlying stock aren't included in the table._!) )
                        )
                      )
                    )
                  )
                )
