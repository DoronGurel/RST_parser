              )
              ( Nucleus (span 92 94) (rel2par span)
                ( Nucleus (span 92 93) (rel2par Same-Unit)
                  ( Nucleus (leaf 92) (rel2par span) (text _!So an investor_!) )
                  ( Satellite (leaf 93) (rel2par elaboration-object-attribute-e) (text _!who wanted to realize a profit on OEX puts after the trading suspension_!) )
                )
                ( Nucleus (leaf 94) (rel2par Same-Unit) (text _!would have been out of luck._!) )
              )
            )
            ( Satellite (span 95 98) (rel2par antithesis)
              ( Nucleus (leaf 95) (rel2par span) (text _!On the other hand, only a handful of individual issues were suspended from trading on Friday._!) )
              ( Satellite (span 96 98) (rel2par elaboration-additional)
                ( Nucleus (span 96 97) (rel2par Same-Unit)
                  ( Nucleus (leaf 96) (rel2par span) (text _!Normally,_!) )
                  ( Satellite (leaf 97) (rel2par circumstance-e) (text _!once the underlying investment is suspended from trading,_!) )
                )
                ( Nucleus (leaf 98) (rel2par Same-Unit) (text _!the options on those investments also don't trade._!) )
