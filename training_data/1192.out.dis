( Root (span 1 91)
  ( Nucleus (span 1 26) (rel2par span)
    ( Nucleus (span 1 18) (rel2par span)
      ( Nucleus (span 1 15) (rel2par span)
        ( Satellite (span 1 11) (rel2par circumstance)
          ( Nucleus (span 1 8) (rel2par span)
            ( Nucleus (span 1 4) (rel2par Sequence)
              ( Nucleus (leaf 1) (rel2par span) (text _!The airline industry's fortunes, in dazzling shape for most of the year, have taken a sudden turn for the worse in the past few weeks._!) )
              ( Satellite (span 2 4) (rel2par elaboration-additional)
                ( Nucleus (span 2 3) (rel2par span)
                  ( Satellite (leaf 2) (rel2par circumstance) (text _!Citing rising fuel costs, promotional fare cuts and a general slowdown in travel,_!) )
                  ( Nucleus (leaf 3) (rel2par span) (text _!several major carriers have posted or are expected to post relatively poor third-quarter results._!) )
                )
                ( Satellite (leaf 4) (rel2par example) (text _!Yesterday, USAir Group Inc., recently one of the industry's stellar performers, posted a worse-than-expected $77.7 million net loss for the period._!) )
              )
            )
            ( Nucleus (span 5 6) (rel2par Sequence)
              ( Nucleus (leaf 5) (rel2par span) (text _!So far, the industry's fourth quarter isn't looking too strong either,_!) )
              ( Satellite (leaf 6) (rel2par consequence-s) (text _!prompting many analysts to slash earning projections for the rest of the year by as much as one-fourth._!) )
            )
            ( Nucleus (span 7 8) (rel2par Sequence)
              ( Satellite (leaf 7) (rel2par attribution) (text _!And they say_!) )
              ( Nucleus (leaf 8) (rel2par span) (text _!the outlook for 1990 is nearly as bad._!) )
            )
          )
          ( Satellite (span 9 11) (rel2par summary-s)
            ( Nucleus (span 9 10) (rel2par span)
              ( Nucleus (leaf 9) (rel2par Sequence) (text _!Airlines in 1989 "came in like a bang_!) )
              ( Nucleus (leaf 10) (rel2par Sequence) (text _!and are going out like a whimper,"_!) )
            )
            ( Satellite (leaf 11) (rel2par attribution) (text _!said Kevin Murphy, an airline analyst at Morgan Stanley & Co._!) )
          )
        )
        ( Nucleus (span 12 15) (rel2par span)
          ( Nucleus (leaf 12) (rel2par span) (text _!This turn of events has put a big damper on an industry_!) )
          ( Satellite (span 13 15) (rel2par elaboration-object-attribute-e)
            ( Nucleus (leaf 13) (rel2par span) (text _!that seemed almost invincible last spring,_!) )
            ( Satellite (span 14 15) (rel2par circumstance)
              ( Nucleus (leaf 14) (rel2par List) (text _!when fares were rising at double-digit rates_!) )
              ( Nucleus (leaf 15) (rel2par List) (text _!and many carriers seemed to be growing fat on near-monopolies in certain markets._!) )
            )
          )
        )
      )
      ( Satellite (span 16 18) (rel2par consequence-s)
        ( Nucleus (leaf 16) (rel2par List) (text _!Now, many airline companies might become a lot less attractive as takeover targets on Wall Street._!) )
        ( Nucleus (span 17 18) (rel2par List)
          ( Nucleus (leaf 17) (rel2par span) (text _!The downturn also raises questions about the carriers' ambitious orders for new airplanes,_!) )
          ( Satellite (leaf 18) (rel2par elaboration-additional-e) (text _!which currently total $32.5 billion over the next three years._!) )
        )
      )
    )
    ( Satellite (span 19 26) (rel2par antithesis)
      ( Nucleus (leaf 19) (rel2par span) (text _!For travelers, though, the industry's problems have had some positive effects._!) )
      ( Satellite (span 20 26) (rel2par example)
        ( Nucleus (span 20 21) (rel2par List)
          ( Nucleus (leaf 20) (rel2par span) (text _!In recent weeks, airlines have cut numerous fares in leisure markets_!) )
          ( Satellite (leaf 21) (rel2par purpose) (text _!to try to win back customers._!) )
        )
        ( Nucleus (span 22 26) (rel2par List)
          ( Nucleus (span 22 24) (rel2par span)
            ( Nucleus (leaf 22) (rel2par span) (text _!Others have tried to spruce up frequent-flier programs._!) )
            ( Satellite (span 23 24) (rel2par antithesis)
              ( Nucleus (leaf 23) (rel2par span) (text _!Previously, airlines were limiting the programs_!) )
              ( Satellite (leaf 24) (rel2par reason) (text _!because they were becoming too expensive._!) )
            )
          )
          ( Satellite (span 25 26) (rel2par example)
            ( Nucleus (leaf 25) (rel2par span) (text _!Just last week, for example, Trans World Airlines and Pan Am Corp. 's Pan American World Airways went so far as to offer cash rebates or gift checks of $200 to $1,000 to certain frequent-flier members_!) )
            ( Satellite (leaf 26) (rel2par elaboration-object-attribute-e) (text _!making trans-Atlantic flights in business class or first class._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 27 91) (rel2par elaboration-additional)
    ( Nucleus (span 27 86) (rel2par span)
      ( Nucleus (span 27 81) (rel2par span)
        ( Nucleus (span 27 75) (rel2par span)
          ( Nucleus (span 27 69) (rel2par span)
            ( Nucleus (span 27 56) (rel2par span)
              ( Nucleus (leaf 27) (rel2par span) (text _!The industry's slowdown became apparent this month_!) )
              ( Satellite (span 28 56) (rel2par circumstance)
                ( Nucleus (span 28 30) (rel2par List)
                  ( Nucleus (leaf 28) (rel2par List) (text _!when AMR Corp., parent of American Airlines, reported an 8.8% drop in third-quarter net income_!) )
                  ( Nucleus (span 29 30) (rel2par List)
                    ( Satellite (leaf 29) (rel2par attribution) (text _!and said_!) )
                    ( Nucleus (leaf 30) (rel2par span) (text _!its fourth quarter would be "disappointing."_!) )
                  )
                )
                ( Nucleus (span 31 56) (rel2par List)
                  ( Nucleus (span 31 49) (rel2par span)
                    ( Nucleus (span 31 41) (rel2par span)
                      ( Nucleus (span 31 37) (rel2par span)
                        ( Nucleus (span 31 32) (rel2par span)
                          ( Satellite (leaf 31) (rel2par attribution) (text _!Shortly before that, USAir had said_!) )
                          ( Nucleus (leaf 32) (rel2par span) (text _!its third-quarter results would be "significantly lower" than a year earlier._!) )
                        )
                        ( Satellite (span 33 37) (rel2par elaboration-additional)
                          ( Nucleus (leaf 33) (rel2par span) (text _!Yesterday, it provided the details:_!) )
                          ( Satellite (span 34 37) (rel2par elaboration-general-specific)
                            ( Nucleus (span 34 35) (rel2par span)
                              ( Nucleus (leaf 34) (rel2par span) (text _!Its loss of $77.7 million, or $1.86 a share, contrasted with net of $68.5 million, or $1.58 a share, in the 1988 third quarter._!) )
                              ( Satellite (leaf 35) (rel2par elaboration-additional) (text _!Revenue rose only 3.3% in the latest period, to $1.53 billion from $1.48 billion._!) )
                            )
                            ( Satellite (span 36 37) (rel2par elaboration-additional)
                              ( Nucleus (leaf 36) (rel2par span) (text _!For the nine months, the Arlington, Va., company's net plunged 73% to $38.5 million, or 76 cents a share, from $142.2 million, or $3.28 a share._!) )
                              ( Satellite (leaf 37) (rel2par elaboration-additional) (text _!Revenue rose 12% to $4.75 billion from $4.22 billion._!) )
                            )
                          )
                        )
                      )
                      ( Satellite (span 38 41) (rel2par antithesis)
                        ( Nucleus (leaf 38) (rel2par span) (text _!The results surprised many analysts,_!) )
                        ( Satellite (span 39 41) (rel2par reason)
                          ( Nucleus (leaf 39) (rel2par List) (text _!because USAir has almost no competition in its Pittsburgh hub_!) )
                          ( Nucleus (span 40 41) (rel2par List)
                            ( Nucleus (leaf 40) (rel2par span) (text _!and has expanded operations_!) )
                            ( Satellite (leaf 41) (rel2par means) (text _!by completing its acquisition of Piedmont Airlines._!) )
                          )
                        )
                      )
                    )
                    ( Satellite (span 42 49) (rel2par elaboration-additional)
                      ( Nucleus (span 42 44) (rel2par span)
                        ( Nucleus (span 42 43) (rel2par span)
                          ( Satellite (leaf 42) (rel2par consequence-n) (text _!Shortly after announcing its quarterly loss,_!) )
                          ( Nucleus (leaf 43) (rel2par span) (text _!USAir's stock tumbled $3 a share._!) )
                        )
                        ( Satellite (leaf 44) (rel2par elaboration-additional) (text _!It ended at $40.125, down $2.375, in New York Stock Exchange composite trading._!) )
                      )
                      ( Satellite (span 45 49) (rel2par elaboration-additional)
                        ( Nucleus (span 45 46) (rel2par span)
                          ( Nucleus (leaf 45) (rel2par span) (text _!"Nobody was expecting this size of a loss,"_!) )
                          ( Satellite (leaf 46) (rel2par attribution) (text _!said Paul Karos, an analyst with First Boston Corp._!) )
                        )
                        ( Satellite (span 47 49) (rel2par elaboration-additional)
                          ( Nucleus (span 47 48) (rel2par Same-Unit)
                            ( Nucleus (leaf 47) (rel2par span) (text _!One airline executive,_!) )
                            ( Satellite (leaf 48) (rel2par elaboration-additional-e) (text _!who declined to be identified,_!) )
                          )
                          ( Nucleus (leaf 49) (rel2par Same-Unit) (text _!called the loss "amazing."_!) )
                        )
                      )
                    )
                  )
                  ( Satellite (span 50 56) (rel2par elaboration-additional)
                    ( Nucleus (span 50 52) (rel2par span)
                      ( Satellite (leaf 50) (rel2par circumstance) (text _!In announcing the results,_!) )
                      ( Nucleus (span 51 52) (rel2par span)
                        ( Nucleus (leaf 51) (rel2par span) (text _!USAir cited many of the same problems_!) )
                        ( Satellite (leaf 52) (rel2par elaboration-object-attribute-e) (text _!that several other industry officials have named recently._!) )
                      )
                    )
                    ( Satellite (span 53 56) (rel2par elaboration-additional)
                      ( Nucleus (span 53 54) (rel2par span)
                        ( Satellite (leaf 53) (rel2par attribution) (text _!It said_!) )
                        ( Nucleus (leaf 54) (rel2par span) (text _!the industry's domestic traffic was flat in the third quarter;_!) )
                      )
                      ( Satellite (span 55 56) (rel2par explanation-argumentative)
                        ( Satellite (leaf 55) (rel2par attribution) (text _!analysts say_!) )
                        ( Nucleus (leaf 56) (rel2par span) (text _!this was because hefty fare increases earlier in the year scared off many leisure travelers this summer._!) )
                      )
                    )
                  )
                )
              )
            )
            ( Satellite (span 57 69) (rel2par elaboration-additional)
              ( Nucleus (span 57 61) (rel2par span)
                ( Satellite (span 57 59) (rel2par concession)
                  ( Nucleus (span 57 58) (rel2par span)
                    ( Satellite (leaf 57) (rel2par purpose) (text _!To try to combat the traffic slowdown,_!) )
                    ( Nucleus (leaf 58) (rel2par span) (text _!airlines started reducing fares;_!) )
                  )
                  ( Satellite (leaf 59) (rel2par consequence-s) (text _!average fares rose only 1.7% in August, in contrast to increases of 16% each in February and March._!) )
                )
                ( Nucleus (span 60 61) (rel2par span)
                  ( Nucleus (leaf 60) (rel2par Cause-Result) (text _!But so far, the effort has failed,_!) )
                  ( Nucleus (leaf 61) (rel2par Cause-Result) (text _!and traffic is still slow._!) )
                )
              )
              ( Satellite (span 62 69) (rel2par elaboration-additional)
                ( Nucleus (leaf 62) (rel2par span) (text _!Some other fare promotions have backfired._!) )
                ( Satellite (span 63 69) (rel2par evidence)
                  ( Satellite (span 63 65) (rel2par circumstance)
                    ( Nucleus (leaf 63) (rel2par span) (text _!This summer, the industry introduced a "kids fly free" program,_!) )
                    ( Satellite (span 64 65) (rel2par elaboration-additional-e)
                      ( Nucleus (leaf 64) (rel2par span) (text _!in which children were allowed to fly free_!) )
                      ( Satellite (leaf 65) (rel2par condition) (text _!if they were traveling with an adult._!) )
                    )
                  )
                  ( Nucleus (span 66 69) (rel2par span)
                    ( Satellite (span 66 67) (rel2par antithesis)
                      ( Nucleus (leaf 66) (rel2par span) (text _!Airlines tried to restrict the program substantially_!) )
                      ( Satellite (leaf 67) (rel2par means) (text _!by limiting the offer to certain days of the week,_!) )
                    )
                    ( Nucleus (span 68 69) (rel2par span)
                      ( Nucleus (leaf 68) (rel2par span) (text _!but it still was apparently used far more heavily_!) )
                      ( Satellite (leaf 69) (rel2par comparison) (text _!than the airlines expected._!) )
                    )
                  )
                )
              )
            )
          )
          ( Satellite (span 70 75) (rel2par elaboration-additional)
            ( Nucleus (span 70 72) (rel2par span)
              ( Satellite (leaf 70) (rel2par attribution) (text _!Airlines also say_!) )
              ( Nucleus (span 71 72) (rel2par span)
                ( Nucleus (leaf 71) (rel2par span) (text _!their frequent-flier programs are squeezing profits_!) )
                ( Satellite (leaf 72) (rel2par explanation-argumentative) (text _!because awards are being redeemed at a heavier-than-normal rate._!) )
              )
            )
            ( Satellite (span 73 75) (rel2par elaboration-additional)
              ( Satellite (leaf 73) (rel2par attribution) (text _!One airline official said_!) )
              ( Nucleus (leaf 74) (rel2par span) (text _!about three times as many free-travel coupons are being turned in as in previous years_!) )
              ( Satellite (leaf 75) (rel2par explanation-argumentative) (text _!-- not surprisingly, as the airlines last year allowed many travelers to build up mileage at triple the normal rate._!) )
            )
          )
        )
        ( Satellite (span 76 81) (rel2par elaboration-additional)
          ( Nucleus (leaf 76) (rel2par span) (text _!Rising operating expenses are another problem._!) )
          ( Satellite (span 77 81) (rel2par elaboration-additional)
            ( Nucleus (leaf 77) (rel2par List) (text _!Fuel costs were up 10% in the third quarter._!) )
            ( Nucleus (span 78 81) (rel2par List)
              ( Nucleus (span 78 80) (rel2par Same-Unit)
                ( Nucleus (leaf 78) (rel2par span) (text _!Labor costs,_!) )
                ( Satellite (span 79 80) (rel2par elaboration-additional-e)
                  ( Nucleus (leaf 79) (rel2par span) (text _!which leveled off in the past few years_!) )
                  ( Satellite (leaf 80) (rel2par explanation-argumentative) (text _!because of lower pay scales for newer employees,_!) )
                )
              )
              ( Nucleus (leaf 81) (rel2par Same-Unit) (text _!are on the upswing again at many carriers._!) )
            )
          )
        )
      )
      ( Satellite (span 82 86) (rel2par elaboration-additional)
        ( Nucleus (leaf 82) (rel2par span) (text _!And some carriers are facing other unexpected headaches:_!) )
        ( Satellite (span 83 86) (rel2par example)
          ( Nucleus (span 83 84) (rel2par span)
            ( Nucleus (leaf 83) (rel2par span) (text _!USAir, for example, blamed some of its loss on merger expenses and on disruptions_!) )
            ( Satellite (leaf 84) (rel2par elaboration-object-attribute-e) (text _!caused by Hurricane Hugo last month._!) )
          )
          ( Satellite (span 85 86) (rel2par elaboration-additional)
            ( Nucleus (leaf 85) (rel2par span) (text _!"We cannot quantify the total adverse effects of Hugo,"_!) )
            ( Satellite (leaf 86) (rel2par attribution) (text _!said Edwin Colodny, chairman and president of USAir Group._!) )
          )
        )
      )
    )
    ( Satellite (span 87 91) (rel2par elaboration-additional)
      ( Nucleus (leaf 87) (rel2par span) (text _!Whatever the cause for the downturn, few people are predicting any sudden improvement._!) )
      ( Satellite (span 88 91) (rel2par example)
        ( Nucleus (span 88 89) (rel2par span)
          ( Nucleus (leaf 88) (rel2par span) (text _!Airline Economics Inc., an aviation consulting firm, is projecting an industrywide operating profit of $2.5 billion for 1989,_!) )
          ( Satellite (leaf 89) (rel2par comparison) (text _!compared with earlier forecasts of $3 billion to $3.5 billion._!) )
        )
        ( Satellite (span 90 91) (rel2par elaboration-additional)
          ( Satellite (leaf 90) (rel2par attribution) (text _!As for 1990, the firm predicts_!) )
          ( Nucleus (leaf 91) (rel2par span) (text _!that profit will slip to between $1 billion and $1.5 billion._!) )
        )
      )
    )
  )
)
