              )
            )
          )
          ( Satellite (span 62 69) (rel2par elaboration-additional)
            ( Nucleus (span 62 65) (rel2par span)
              ( Nucleus (leaf 62) (rel2par Cause-Result) (text _!Because the stock prices of some paper companies already reflect an expected profit slump,_!) )
              ( Nucleus (span 63 65) (rel2par Cause-Result)
                ( Satellite (leaf 63) (rel2par attribution) (text _!PaineWebber's Mr. Ross says_!) )
                ( Nucleus (span 64 65) (rel2par span)
