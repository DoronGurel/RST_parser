                ( Nucleus (leaf 65) (rel2par Same-Unit) (text _!said_!) )
              )
              ( Nucleus (span 66 67) (rel2par span)
                ( Nucleus (leaf 66) (rel2par span) (text _!the Memphis project was "pulled in for a reconsideration of costs," an indication_!) )
                ( Satellite (leaf 67) (rel2par elaboration-object-attribute-e) (text _!that the ambitious plans might be scaled back in any future construction._!) )
              )
            )
            ( Satellite (span 68 70) (rel2par elaboration-additional)
              ( Nucleus (span 68 69) (rel2par Same-Unit)
                ( Nucleus (leaf 68) (rel2par span) (text _!Initial cost estimates for the plant,_!) )
                ( Satellite (leaf 69) (rel2par elaboration-additional-e) (text _!which was to have been built in phases,_!) )
              )
              ( Nucleus (leaf 70) (rel2par Same-Unit) (text _!ranged from $1 billion to $1.2 billion._!) )
            )
          )
