                  ( Nucleus (leaf 76) (rel2par span) (text _!by purchasing "index puts,"_!) )
                  ( Satellite (span 77 78) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 77) (rel2par span) (text _!which are simply put options on indexes_!) )
                    ( Satellite (leaf 78) (rel2par elaboration-object-attribute-e) (text _!that match broad baskets of stocks._!) )
