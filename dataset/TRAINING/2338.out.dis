( Root (span 1 87)
  ( Nucleus (span 1 18) (rel2par span)
    ( Nucleus (span 1 7) (rel2par span)
      ( Nucleus (span 1 5) (rel2par span)
        ( Satellite (span 1 3) (rel2par attribution)
          ( Nucleus (span 1 2) (rel2par Same-Unit)
            ( Nucleus (leaf 1) (rel2par span) (text _!General Motors Corp., in a series of moves_!) )
            ( Satellite (leaf 2) (rel2par elaboration-object-attribute-e) (text _!that angered union officials in the U.S. and Canada,_!) )
          )
          ( Nucleus (leaf 3) (rel2par Same-Unit) (text _!has signaled_!) )
        )
        ( Nucleus (span 4 5) (rel2par span)
          ( Nucleus (leaf 4) (rel2par span) (text _!that as many as five North American assembly plants may not survive the mid-1990s_!) )
          ( Satellite (leaf 5) (rel2par circumstance) (text _!as the corporation struggles to cut its excess vehicle-making capacity._!) )
        )
      )
      ( Satellite (span 6 7) (rel2par elaboration-additional)
        ( Nucleus (leaf 6) (rel2par List) (text _!In announcements to workers late last week, GM effectively signed death notices for two full-sized van assembly plants,_!) )
        ( Nucleus (leaf 7) (rel2par List) (text _!and cast serious doubt on the futures of three U.S. car factories._!) )
      )
    )
    ( Satellite (span 8 18) (rel2par circumstance)
      ( Nucleus (span 8 11) (rel2par span)
        ( Nucleus (leaf 8) (rel2par span) (text _!GM is under intense pressure_!) )
        ( Satellite (span 9 11) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 9) (rel2par span) (text _!to close factories_!) )
          ( Satellite (span 10 11) (rel2par elaboration-object-attribute-e)
            ( Nucleus (leaf 10) (rel2par span) (text _!that became unprofitable_!) )
            ( Satellite (leaf 11) (rel2par result) (text _!as the giant auto maker's U.S. market share skidded during the past decade._!) )
          )
        )
      )
      ( Satellite (span 12 18) (rel2par elaboration-additional)
        ( Nucleus (span 12 15) (rel2par span)
          ( Satellite (span 12 14) (rel2par attribution)
            ( Nucleus (leaf 12) (rel2par Same-Unit) (text _!The company,_!) )
            ( Nucleus (span 13 14) (rel2par Same-Unit)
              ( Satellite (leaf 13) (rel2par circumstance-e) (text _!currently using about 80% of its North American vehicle capacity,_!) )
              ( Nucleus (leaf 14) (rel2par span) (text _!has vowed_!) )
            )
          )
          ( Nucleus (leaf 15) (rel2par span) (text _!it will run at 100% of capacity by 1992._!) )
        )
        ( Satellite (span 16 18) (rel2par background)
          ( Satellite (leaf 16) (rel2par attribution) (text _!Just a month ago, GM announced_!) )
          ( Nucleus (span 17 18) (rel2par span)
            ( Nucleus (leaf 17) (rel2par span) (text _!it would make an aging assembly plant in Lakewood, Ga., the eighth U.S. assembly facility_!) )
            ( Satellite (leaf 18) (rel2par elaboration-object-attribute-e) (text _!to close since 1987._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 19 87) (rel2par elaboration-additional)
    ( Nucleus (span 19 30) (rel2par span)
      ( Nucleus (span 19 26) (rel2par span)
        ( Nucleus (span 19 20) (rel2par span)
          ( Nucleus (leaf 19) (rel2par span) (text _!Now, GM appears to be stepping up the pace of its factory consolidation_!) )
          ( Satellite (leaf 20) (rel2par purpose) (text _!to get in shape for the 1990s._!) )
        )
        ( Satellite (span 21 26) (rel2par reason)
          ( Nucleus (span 21 23) (rel2par List)
            ( Nucleus (leaf 21) (rel2par span) (text _!One reason is mounting competition from new Japanese car plants in the U.S._!) )
            ( Satellite (span 22 23) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 22) (rel2par span) (text _!that are pouring out more than one million vehicles a year at costs lower_!) )
              ( Satellite (leaf 23) (rel2par comparison-e) (text _!than GM can match._!) )
            )
          )
          ( Nucleus (span 24 26) (rel2par List)
            ( Satellite (leaf 24) (rel2par attribution) (text _!Another is that United Auto Workers union officials have signaled_!) )
            ( Nucleus (span 25 26) (rel2par span)
              ( Nucleus (leaf 25) (rel2par span) (text _!they want tighter no-layoff provisions in the new Big Three national contract_!) )
              ( Satellite (leaf 26) (rel2par elaboration-object-attribute-e) (text _!that will be negotiated next year._!) )
            )
          )
        )
      )
      ( Satellite (span 27 30) (rel2par elaboration-additional)
        ( Satellite (span 27 29) (rel2par circumstance)
          ( Nucleus (span 27 28) (rel2par Same-Unit)
            ( Nucleus (leaf 27) (rel2par span) (text _!GM officials want to get their strategy_!) )
            ( Satellite (leaf 28) (rel2par elaboration-object-attribute-e) (text _!to reduce capacity and the work force_!) )
          )
          ( Nucleus (leaf 29) (rel2par Same-Unit) (text _!in place_!) )
        )
        ( Nucleus (leaf 30) (rel2par span) (text _!before those talks begin._!) )
      )
    )
    ( Satellite (span 31 87) (rel2par elaboration-additional)
      ( Nucleus (span 31 34) (rel2par span)
        ( Nucleus (leaf 31) (rel2par span) (text _!The problem, however, is that GM's moves are coming at a time_!) )
        ( Satellite (span 32 34) (rel2par elaboration-object-attribute-e)
          ( Nucleus (leaf 32) (rel2par span) (text _!when UAW leaders are trying to silence dissidents_!) )
          ( Satellite (span 33 34) (rel2par elaboration-object-attribute-e)
            ( Satellite (leaf 33) (rel2par attribution) (text _!who charge_!) )
            ( Nucleus (leaf 34) (rel2par span) (text _!the union is too passive in the face of GM layoffs._!) )
          )
        )
      )
      ( Satellite (span 35 87) (rel2par elaboration-additional)
        ( Nucleus (span 35 43) (rel2par span)
          ( Nucleus (span 35 38) (rel2par span)
            ( Nucleus (span 35 36) (rel2par Same-Unit)
              ( Nucleus (leaf 35) (rel2par span) (text _!Against that backdrop, UAW Vice President Stephen P. Yokich,_!) )
              ( Satellite (leaf 36) (rel2par elaboration-object-attribute-e) (text _!who recently became head of the union's GM department,_!) )
            )
            ( Nucleus (span 37 38) (rel2par Same-Unit)
              ( Nucleus (leaf 37) (rel2par span) (text _!issued a statement Friday_!) )
              ( Satellite (leaf 38) (rel2par elaboration-object-attribute-e) (text _!blasting GM's "flagrant insensitivity" toward union members._!) )
            )
          )
          ( Satellite (span 39 43) (rel2par explanation-argumentative)
            ( Nucleus (span 39 42) (rel2par span)
              ( Nucleus (span 39 40) (rel2par Same-Unit)
                ( Nucleus (leaf 39) (rel2par span) (text _!The auto maker's decision_!) )
                ( Satellite (leaf 40) (rel2par elaboration-object-attribute-e) (text _!to let word of the latest shutdowns and product reassignments trickle out in separate communiques to the affected plants_!) )
              )
              ( Nucleus (span 41 42) (rel2par Same-Unit)
                ( Nucleus (leaf 41) (rel2par span) (text _!showed "disarray" and an "inability or unwillingness_!) )
                ( Satellite (leaf 42) (rel2par elaboration-object-attribute-e) (text _!to provide consistent information,"_!) )
              )
            )
            ( Satellite (leaf 43) (rel2par attribution) (text _!Mr. Yokich said._!) )
          )
        )
        ( Satellite (span 44 87) (rel2par elaboration-additional)
          ( Nucleus (leaf 44) (rel2par span) (text _!GM officials told workers late last week of the following moves:_!) )
          ( Satellite (span 45 87) (rel2par elaboration-set-member)
            ( Nucleus (span 45 53) (rel2par List)
              ( Nucleus (leaf 45) (rel2par span) (text _!Production of full-sized vans will be consolidated into a single plant in Flint, Mich._!) )
              ( Satellite (span 46 53) (rel2par interpretation-s)
                ( Nucleus (span 46 49) (rel2par span)
                  ( Nucleus (span 46 48) (rel2par span)
                    ( Nucleus (span 46 47) (rel2par Same-Unit)
                      ( Nucleus (leaf 46) (rel2par span) (text _!That means two plants_!) )
                      ( Satellite (leaf 47) (rel2par elaboration-general-specific-e) (text _!-- one in Scarborough, Ontario, and the other in Lordstown, Ohio --_!) )
                    )
                    ( Nucleus (leaf 48) (rel2par Same-Unit) (text _!probably will be shut down after the end of 1991._!) )
                  )
                  ( Satellite (leaf 49) (rel2par elaboration-additional) (text _!The shutdowns will idle about 3,000 Canadian assembly workers and about 2,500 workers in Ohio._!) )
                )
                ( Satellite (span 50 53) (rel2par elaboration-additional)
                  ( Nucleus (span 50 51) (rel2par List)
                    ( Nucleus (leaf 50) (rel2par span) (text _!Robert White, Canadian Auto Workers union president, used the impending Scarborough shutdown_!) )
                    ( Satellite (leaf 51) (rel2par purpose) (text _!to criticize the U.S.-Canada free trade agreement and its champion, Prime Minister Brian Mulroney._!) )
                  )
                  ( Nucleus (span 52 53) (rel2par List)
                    ( Nucleus (leaf 52) (rel2par span) (text _!But Canadian auto workers may benefit from a separate GM move_!) )
                    ( Satellite (leaf 53) (rel2par elaboration-object-attribute-e) (text _!that affects three U.S. car plants and one in Quebec._!) )
                  )
                )
              )
            )
            ( Nucleus (span 54 87) (rel2par List)
              ( Nucleus (span 54 55) (rel2par span)
                ( Satellite (leaf 54) (rel2par attribution) (text _!Workers at plants in Van Nuys, Calif., Oklahoma City and Pontiac, Mich., were told_!) )
                ( Nucleus (leaf 55) (rel2par span) (text _!their facilities are no longer being considered to build the next generation of the Pontiac Firebird and Chevrolet Camaro muscle cars._!) )
              )
              ( Satellite (span 56 87) (rel2par elaboration-additional)
                ( Nucleus (span 56 58) (rel2par span)
                  ( Nucleus (span 56 57) (rel2par span)
                    ( Satellite (leaf 56) (rel2par attribution) (text _!GM is studying_!) )
                    ( Nucleus (leaf 57) (rel2par span) (text _!whether it can build the new Camaro-Firebird profitably at a plant in St. Therese, Quebec,_!) )
                  )
                  ( Satellite (leaf 58) (rel2par attribution) (text _!company and union officials said._!) )
                )
                ( Satellite (span 59 87) (rel2par elaboration-additional)
                  ( Nucleus (leaf 59) (rel2par span) (text _!That announcement left union officials in Van Nuys and Oklahoma City uncertain about their futures._!) )
                  ( Satellite (span 60 87) (rel2par explanation-argumentative)
                    ( Nucleus (span 60 68) (rel2par List)
                      ( Nucleus (span 60 63) (rel2par span)
                        ( Nucleus (span 60 61) (rel2par Same-Unit)
                          ( Nucleus (leaf 60) (rel2par span) (text _!The Van Nuys plant,_!) )
                          ( Satellite (leaf 61) (rel2par elaboration-additional-e) (text _!which employs about 3,000 workers,_!) )
                        )
                        ( Nucleus (span 62 63) (rel2par Same-Unit)
                          ( Nucleus (leaf 62) (rel2par span) (text _!doesn't have a product_!) )
                          ( Satellite (leaf 63) (rel2par elaboration-object-attribute-e) (text _!to build after 1993._!) )
                        )
                      )
                      ( Satellite (span 64 68) (rel2par elaboration-additional)
                        ( Satellite (leaf 64) (rel2par attribution) (text _!Jerry Shrieves, UAW local president, said_!) )
                        ( Nucleus (span 65 68) (rel2par span)
                          ( Nucleus (leaf 65) (rel2par span) (text _!the facility was asked to draw up plans_!) )
                          ( Satellite (span 66 68) (rel2par elaboration-object-attribute-e)
                            ( Nucleus (leaf 66) (rel2par span) (text _!to continue working as a "flex plant,"_!) )
                            ( Satellite (span 67 68) (rel2par elaboration-additional-e)
                              ( Nucleus (leaf 67) (rel2par span) (text _!which could build several different types of products on short notice_!) )
                              ( Satellite (leaf 68) (rel2par purpose) (text _!to satisfy demand._!) )
                            )
                          )
                        )
                      )
                    )
                    ( Nucleus (span 69 87) (rel2par List)
                      ( Satellite (span 69 72) (rel2par attribution)
                        ( Nucleus (span 69 71) (rel2par Same-Unit)
                          ( Nucleus (leaf 69) (rel2par span) (text _!At the Oklahoma City plant,_!) )
                          ( Satellite (span 70 71) (rel2par elaboration-additional-e)
                            ( Nucleus (leaf 70) (rel2par span) (text _!which employs about 6,000 workers_!) )
                            ( Satellite (leaf 71) (rel2par elaboration-object-attribute-e) (text _!building the eight-year-old A-body mid-sized cars,_!) )
                          )
                        )
                        ( Nucleus (leaf 72) (rel2par Same-Unit) (text _!Steve Featherston, UAW local vice president, said_!) )
                      )
                      ( Nucleus (span 73 87) (rel2par span)
                        ( Nucleus (span 73 76) (rel2par span)
                          ( Nucleus (span 73 74) (rel2par List)
                            ( Nucleus (leaf 73) (rel2par span) (text _!the plant has no new product_!) )
                            ( Satellite (leaf 74) (rel2par elaboration-object-attribute-e) (text _!lined up,_!) )
                          )
                          ( Nucleus (span 75 76) (rel2par List)
                            ( Satellite (leaf 75) (rel2par attribution-n) (text _!and "none of us knows"_!) )
                            ( Nucleus (leaf 76) (rel2par span) (text _!when the A-body cars will die._!) )
                          )
                        )
                        ( Satellite (span 77 87) (rel2par elaboration-additional)
                          ( Nucleus (span 77 80) (rel2par Contrast)
                            ( Satellite (leaf 77) (rel2par attribution) (text _!He said_!) )
                            ( Nucleus (span 78 80) (rel2par span)
                              ( Satellite (leaf 78) (rel2par attribution) (text _!he believes_!) )
                              ( Nucleus (span 79 80) (rel2par span)
                                ( Nucleus (leaf 79) (rel2par span) (text _!GM has plans_!) )
                                ( Satellite (leaf 80) (rel2par elaboration-object-attribute-e) (text _!to keep building A-body cars into the mid-1990s._!) )
                              )
                            )
                          )
                          ( Nucleus (span 81 87) (rel2par Contrast)
                            ( Nucleus (span 81 86) (rel2par span)
                              ( Nucleus (span 81 83) (rel2par span)
                                ( Nucleus (leaf 81) (rel2par span) (text _!At Pontiac, however, the Camaro-Firebird decision appears to erase UAW hopes_!) )
                                ( Satellite (span 82 83) (rel2par elaboration-object-attribute-e)
                                  ( Nucleus (leaf 82) (rel2par span) (text _!that GM would reopen the shuttered assembly plant_!) )
                                  ( Satellite (leaf 83) (rel2par elaboration-object-attribute-e) (text _!that last built the plastic-bodied, two-seater Pontiac Fiero model._!) )
                                )
                              )
                              ( Satellite (span 84 86) (rel2par elaboration-additional)
                                ( Nucleus (span 84 85) (rel2par span)
                                  ( Nucleus (leaf 84) (rel2par Sequence) (text _!The Fiero plant was viewed as a model of union-management cooperation at GM_!) )
                                  ( Nucleus (leaf 85) (rel2par Sequence) (text _!before slow sales of the Fiero forced the company to close the factory last year._!) )
                                )
                                ( Satellite (leaf 86) (rel2par cause) (text _!Union officials have taken a beating politically as a result._!) )
                              )
                            )
                            ( Satellite (leaf 87) (rel2par elaboration-additional) (text _!Dissident UAW members have used the Fiero plant as a symbol of labor-management cooperation's failure._!) )
                          )
                        )
                      )
                    )
                  )
                )
              )
            )
          )
        )
      )
    )
  )
)
