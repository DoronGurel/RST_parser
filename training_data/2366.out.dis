( Root (span 1 119)
  ( Nucleus (span 1 33) (rel2par TextualOrganization)
    ( Nucleus (leaf 1) (rel2par TextualOrganization) (text _!Population Drain Ends For Midwestern States_!) )
    ( Nucleus (span 2 33) (rel2par TextualOrganization)
      ( Nucleus (span 2 13) (rel2par span)
        ( Nucleus (span 2 6) (rel2par span)
          ( Satellite (span 2 3) (rel2par summary-n)
            ( Nucleus (leaf 2) (rel2par span) (text _!IOWA IS MAKING a comeback._!) )
            ( Satellite (leaf 3) (rel2par elaboration-additional) (text _!So are Indiana, Ohio and Michigan._!) )
          )
          ( Nucleus (span 4 6) (rel2par span)
            ( Nucleus (span 4 5) (rel2par span)
              ( Nucleus (leaf 4) (rel2par span) (text _!The population of all four states is on the upswing,_!) )
              ( Satellite (leaf 5) (rel2par attribution) (text _!according to new Census Bureau estimates,_!) )
            )
            ( Satellite (leaf 6) (rel2par circumstance) (text _!following declines throughout the early 1980s._!) )
          )
        )
        ( Satellite (span 7 13) (rel2par elaboration-additional)
          ( Nucleus (span 7 9) (rel2par span)
            ( Nucleus (leaf 7) (rel2par Same-Unit) (text _!The gains,_!) )
            ( Nucleus (span 8 9) (rel2par Same-Unit)
              ( Satellite (leaf 8) (rel2par comment-e) (text _!to be sure,_!) )
              ( Nucleus (leaf 9) (rel2par span) (text _!are rather small._!) )
            )
          )
          ( Satellite (span 10 13) (rel2par example)
            ( Nucleus (span 10 11) (rel2par span)
              ( Nucleus (leaf 10) (rel2par span) (text _!Iowa, for instance, saw its population grow by 11,000 people, or 0.4%, between 1987 and 1988,_!) )
              ( Satellite (leaf 11) (rel2par attribution) (text _!the Census Bureau says._!) )
            )
            ( Satellite (span 12 13) (rel2par evaluation-s)
              ( Nucleus (leaf 12) (rel2par span) (text _!Still, even that modest increase is good news for a state_!) )
              ( Satellite (leaf 13) (rel2par elaboration-object-attribute-e) (text _!that hadn't grown at all since 1981._!) )
            )
          )
        )
      )
      ( Satellite (span 14 33) (rel2par background)
        ( Nucleus (span 14 15) (rel2par List)
          ( Nucleus (leaf 14) (rel2par span) (text _!Between 1987 and 1988, North Dakota was the only state in the Midwest_!) )
          ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!to lose population, a loss of 4,000 people._!) )
        )
        ( Nucleus (span 16 17) (rel2par List)
          ( Nucleus (leaf 16) (rel2par span) (text _!Six of the 12 midwestern states have been growing steadily since 1980_!) )
          ( Satellite (leaf 17) (rel2par elaboration-set-member-e) (text _!-- Illinois, Kansas, Minnesota, Missouri, South Dakota and Wisconsin._!) )
        )
        ( Nucleus (span 18 24) (rel2par List)
          ( Nucleus (leaf 18) (rel2par span) (text _!The Northeast has been holding its own in the population race._!) )
          ( Satellite (span 19 24) (rel2par elaboration-additional)
            ( Nucleus (span 19 21) (rel2par List)
              ( Nucleus (leaf 19) (rel2par span) (text _!Seven of nine states have grown each year since 1980,_!) )
              ( Satellite (span 20 21) (rel2par elaboration-set-member-e)
                ( Nucleus (leaf 20) (rel2par span) (text _!including New York,_!) )
                ( Satellite (leaf 21) (rel2par elaboration-additional-e) (text _!which lost 4% of its population during the 1970s._!) )
              )
            )
            ( Nucleus (span 22 24) (rel2par List)
              ( Nucleus (leaf 22) (rel2par Same-Unit) (text _!And_!) )
              ( Nucleus (span 23 24) (rel2par Same-Unit)
                ( Satellite (leaf 23) (rel2par concession-e) (text _!although Pennsylvania and Massachusetts suffered slight declines earlier in the decade,_!) )
                ( Nucleus (leaf 24) (rel2par span) (text _!they are growing again._!) )
              )
            )
          )
        )
        ( Nucleus (span 25 33) (rel2par List)
          ( Nucleus (span 25 29) (rel2par span)
            ( Nucleus (leaf 25) (rel2par span) (text _!At the same time, several states in the South and West have had their own population turnaround._!) )
            ( Satellite (span 26 29) (rel2par elaboration-additional)
              ( Nucleus (span 26 28) (rel2par span)
                ( Nucleus (span 26 27) (rel2par Same-Unit)
                  ( Nucleus (leaf 26) (rel2par span) (text _!Seven states_!) )
                  ( Satellite (leaf 27) (rel2par elaboration-object-attribute-e) (text _!that grew in the early 1980s_!) )
                )
                ( Nucleus (leaf 28) (rel2par Same-Unit) (text _!are now losing population_!) )
              )
              ( Satellite (leaf 29) (rel2par elaboration-set-member-e) (text _!-- West Virginia, Mississippi, Louisiana, Oklahoma, Montana, Wyoming and Alaska._!) )
            )
          )
          ( Satellite (span 30 33) (rel2par concession)
            ( Satellite (span 30 32) (rel2par antithesis)
              ( Nucleus (leaf 30) (rel2par List) (text _!Overall, though, the South and West still outpace the Northeast and Midwest,_!) )
              ( Nucleus (span 31 32) (rel2par List)
                ( Satellite (leaf 31) (rel2par attribution) (text _!and fast-growing states like Florida and California ensure_!) )
                ( Nucleus (leaf 32) (rel2par span) (text _!that the pattern will continue._!) )
              )
            )
            ( Nucleus (leaf 33) (rel2par span) (text _!But the growth gap between the Sun Belt and other regions has clearly started narrowing._!) )
          )
        )
      )
    )
  )
  ( Nucleus (span 34 64) (rel2par TextualOrganization)
    ( Nucleus (leaf 34) (rel2par TextualOrganization) (text _!More Elderly Maintain Their Independence_!) )
    ( Nucleus (span 35 64) (rel2par TextualOrganization)
      ( Nucleus (span 35 38) (rel2par span)
        ( Nucleus (leaf 35) (rel2par span) (text _!THANKS TO modern medicine, more couples are growing old together._!) )
        ( Satellite (span 36 38) (rel2par elaboration-additional)
          ( Nucleus (leaf 36) (rel2par Same-Unit) (text _!And_!) )
          ( Nucleus (span 37 38) (rel2par Same-Unit)
            ( Satellite (leaf 37) (rel2par concession-e) (text _!even after losing a spouse,_!) )
            ( Nucleus (leaf 38) (rel2par span) (text _!more of the elderly are staying independent._!) )
          )
        )
      )
      ( Satellite (span 39 64) (rel2par explanation-argumentative)
        ( Nucleus (span 39 42) (rel2par span)
          ( Satellite (leaf 39) (rel2par attribution) (text _!A new Census Bureau study of the noninstitutionalized population shows_!) )
          ( Nucleus (span 40 42) (rel2par span)
            ( Nucleus (span 40 41) (rel2par Same-Unit)
              ( Nucleus (leaf 40) (rel2par span) (text _!that 64% of people_!) )
              ( Satellite (leaf 41) (rel2par elaboration-object-attribute-e) (text _!aged 65 to 74_!) )
            )
            ( Nucleus (leaf 42) (rel2par Same-Unit) (text _!were living with a spouse in 1988, up from 59% in 1970._!) )
          )
        )
        ( Satellite (span 43 64) (rel2par interpretation-s)
          ( Nucleus (span 43 44) (rel2par span)
            ( Nucleus (leaf 43) (rel2par span) (text _!This doesn't mean they're less likely to live alone, however._!) )
            ( Satellite (leaf 44) (rel2par explanation-argumentative) (text _!That share has remained at about 24% since 1970._!) )
          )
          ( Satellite (span 45 64) (rel2par elaboration-additional)
            ( Nucleus (span 45 48) (rel2par span)
              ( Nucleus (leaf 45) (rel2par span) (text _!What has changed is that more of the young elderly are living with spouses rather than with other relatives, such as children._!) )
              ( Satellite (span 46 48) (rel2par elaboration-additional)
                ( Nucleus (span 46 47) (rel2par Same-Unit)
                  ( Nucleus (leaf 46) (rel2par span) (text _!In 1988, 10% of those_!) )
                  ( Satellite (leaf 47) (rel2par elaboration-object-attribute-e) (text _!aged 65 to 74_!) )
                )
                ( Nucleus (leaf 48) (rel2par Same-Unit) (text _!lived with relatives other than spouses, down from 15% in 1970._!) )
              )
            )
            ( Satellite (span 49 64) (rel2par elaboration-additional)
              ( Nucleus (span 49 50) (rel2par Contrast)
                ( Nucleus (leaf 49) (rel2par Sequence) (text _!As people get even older,_!) )
                ( Nucleus (leaf 50) (rel2par Sequence) (text _!many become widowed._!) )
              )
              ( Nucleus (span 51 64) (rel2par Contrast)
                ( Nucleus (span 51 59) (rel2par span)
                  ( Nucleus (span 51 55) (rel2par span)
                    ( Nucleus (span 51 52) (rel2par Same-Unit)
                      ( Nucleus (leaf 51) (rel2par span) (text _!But even among those_!) )
                      ( Satellite (leaf 52) (rel2par elaboration-object-attribute-e) (text _!aged 75 and older,_!) )
                    )
                    ( Nucleus (span 53 54) (rel2par Same-Unit)
                      ( Nucleus (leaf 53) (rel2par span) (text _!the share_!) )
                      ( Satellite (leaf 54) (rel2par elaboration-object-attribute-e) (text _!living with a spouse_!) )
                    )
                    ( Nucleus (leaf 55) (rel2par Same-Unit) (text _!rose slightly, to 40% in 1988 from 38% in 1970._!) )
                  )
                  ( Satellite (span 56 59) (rel2par elaboration-additional)
                    ( Nucleus (leaf 56) (rel2par span) (text _!Like their younger counterparts, the older elderly are less likely to live with other relatives._!) )
                    ( Satellite (span 57 59) (rel2par explanation-argumentative)
                      ( Nucleus (span 57 58) (rel2par Same-Unit)
                        ( Nucleus (leaf 57) (rel2par span) (text _!Only 17% of those_!) )
                        ( Satellite (leaf 58) (rel2par elaboration-object-attribute-e) (text _!aged 75 and older_!) )
                      )
                      ( Nucleus (leaf 59) (rel2par Same-Unit) (text _!lived with relatives other than spouses in 1988, down from 26% in 1970._!) )
                    )
                  )
                )
                ( Satellite (span 60 64) (rel2par elaboration-additional)
                  ( Nucleus (span 60 62) (rel2par span)
                    ( Nucleus (span 60 61) (rel2par Same-Unit)
                      ( Nucleus (leaf 60) (rel2par span) (text _!The likelihood_!) )
                      ( Satellite (leaf 61) (rel2par elaboration-object-attribute-e) (text _!of living alone beyond the age of 75_!) )
                    )
                    ( Nucleus (leaf 62) (rel2par Same-Unit) (text _!has increased to 40% from 32%._!) )
                  )
                  ( Satellite (span 63 64) (rel2par elaboration-additional)
                    ( Nucleus (leaf 63) (rel2par span) (text _!More people are remaining independent longer_!) )
                    ( Satellite (leaf 64) (rel2par circumstance) (text _!presumably because they are better off physically and financially._!) )
                  )
                )
              )
            )
          )
        )
      )
    )
  )
  ( Nucleus (span 65 102) (rel2par TextualOrganization)
    ( Nucleus (leaf 65) (rel2par TextualOrganization) (text _!Careers Count Most For the Well-to-Do_!) )
    ( Nucleus (span 66 102) (rel2par TextualOrganization)
      ( Nucleus (leaf 66) (rel2par span) (text _!MANY AFFLUENT people place personal success and money above family._!) )
      ( Satellite (span 67 102) (rel2par elaboration-additional)
        ( Nucleus (span 67 76) (rel2par span)
          ( Nucleus (leaf 67) (rel2par span) (text _!At least that's what a survey by Ernst & Young and Yankelovich, Clancy, Shulman indicates._!) )
          ( Satellite (span 68 76) (rel2par explanation-argumentative)
            ( Nucleus (span 68 73) (rel2par span)
              ( Nucleus (span 68 70) (rel2par Contrast)
                ( Satellite (leaf 68) (rel2par attribution) (text _!Two-thirds of respondents said_!) )
                ( Nucleus (span 69 70) (rel2par span)
                  ( Nucleus (leaf 69) (rel2par span) (text _!they strongly felt the need_!) )
                  ( Satellite (leaf 70) (rel2par elaboration-object-attribute-e) (text _!to be successful in their jobs,_!) )
                )
              )
              ( Nucleus (span 71 73) (rel2par Contrast)
                ( Satellite (leaf 71) (rel2par attribution) (text _!while fewer than half said_!) )
                ( Nucleus (span 72 73) (rel2par span)
                  ( Nucleus (leaf 72) (rel2par span) (text _!they strongly felt the need_!) )
                  ( Satellite (leaf 73) (rel2par elaboration-object-attribute-e) (text _!to spend more time with their families._!) )
                )
              )
            )
            ( Satellite (span 74 76) (rel2par interpretation-s)
              ( Nucleus (span 74 75) (rel2par Same-Unit)
                ( Nucleus (leaf 74) (rel2par span) (text _!Being successful in careers and spending the money_!) )
                ( Satellite (leaf 75) (rel2par elaboration-object-attribute-e) (text _!they make_!) )
              )
              ( Nucleus (leaf 76) (rel2par Same-Unit) (text _!are top priorities for this group._!) )
            )
          )
        )
        ( Satellite (span 77 102) (rel2par elaboration-additional)
          ( Nucleus (span 77 99) (rel2par span)
            ( Nucleus (span 77 87) (rel2par span)
              ( Nucleus (span 77 79) (rel2par span)
                ( Nucleus (leaf 77) (rel2par span) (text _!Unlike most studies of the affluent market, this survey excluded the super-rich._!) )
                ( Satellite (span 78 79) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 78) (rel2par List) (text _!Average household income for the sample was $194,000,_!) )
                  ( Nucleus (leaf 79) (rel2par List) (text _!and average net assets were reported as $775,000._!) )
                )
              )
              ( Satellite (span 80 87) (rel2par elaboration-additional)
                ( Nucleus (span 80 82) (rel2par span)
                  ( Nucleus (leaf 80) (rel2par span) (text _!The goal was to learn about one of today's fastest-growing income groups, the upper-middle class._!) )
                  ( Satellite (span 81 82) (rel2par elaboration-additional)
                    ( Satellite (leaf 81) (rel2par concession) (text _!Although they represent only 2% of the population,_!) )
                    ( Nucleus (leaf 82) (rel2par span) (text _!they control nearly one-third of discretionary income._!) )
                  )
                )
                ( Satellite (span 83 87) (rel2par elaboration-additional)
                  ( Nucleus (leaf 83) (rel2par List) (text _!Across the board, these consumers value quality,_!) )
                  ( Nucleus (span 84 85) (rel2par List)
                    ( Nucleus (leaf 84) (rel2par span) (text _!buy what they like_!) )
                    ( Satellite (leaf 85) (rel2par preference) (text _!rather than just what they need,_!) )
                  )
                  ( Nucleus (span 86 87) (rel2par List)
                    ( Nucleus (leaf 86) (rel2par span) (text _!and appreciate products_!) )
                    ( Satellite (leaf 87) (rel2par elaboration-object-attribute-e) (text _!that are distinctive._!) )
                  )
                )
              )
            )
            ( Satellite (span 88 99) (rel2par elaboration-additional)
              ( Nucleus (span 88 93) (rel2par span)
                ( Satellite (leaf 88) (rel2par concession) (text _!Despite their considerable incomes and assets,_!) )
                ( Nucleus (span 89 93) (rel2par span)
                  ( Nucleus (span 89 91) (rel2par List)
                    ( Nucleus (leaf 89) (rel2par List) (text _!40% of the respondents in the study don't feel financially secure,_!) )
                    ( Nucleus (span 90 91) (rel2par List)
                      ( Satellite (leaf 90) (rel2par attribution-n) (text _!and one-fourth don't feel_!) )
                      ( Nucleus (leaf 91) (rel2par span) (text _!that they have made it._!) )
                    )
                  )
                  ( Nucleus (span 92 93) (rel2par List)
                    ( Satellite (leaf 92) (rel2par attribution-n) (text _!Twenty percent don't even feel_!) )
                    ( Nucleus (leaf 93) (rel2par span) (text _!they are financially well off._!) )
                  )
                )
              )
              ( Satellite (span 94 99) (rel2par elaboration-additional)
                ( Nucleus (leaf 94) (rel2par span) (text _!Many of the affluent aren't comfortable with themselves, either._!) )
                ( Satellite (span 95 99) (rel2par explanation-argumentative)
                  ( Nucleus (span 95 96) (rel2par List)
                    ( Satellite (leaf 95) (rel2par attribution-n) (text _!About 40% don't feel_!) )
                    ( Nucleus (leaf 96) (rel2par span) (text _!they're more able than others._!) )
                  )
                  ( Nucleus (span 97 99) (rel2par List)
                    ( Satellite (span 97 98) (rel2par antithesis)
                      ( Nucleus (leaf 97) (rel2par span) (text _!While twothirds feel some guilt_!) )
                      ( Satellite (leaf 98) (rel2par elaboration-object-attribute-e) (text _!about being affluent,_!) )
                    )
                    ( Nucleus (leaf 99) (rel2par span) (text _!only 25% give $2,500 or more to charity each year._!) )
                  )
                )
              )
            )
          )
          ( Satellite (span 100 102) (rel2par elaboration-additional)
            ( Nucleus (leaf 100) (rel2par Temporal-Same-Time) (text _!Thirty-five percent attend religious services regularly;_!) )
            ( Nucleus (span 101 102) (rel2par Temporal-Same-Time)
              ( Satellite (leaf 101) (rel2par attribution) (text _!at the same time, 60% feel_!) )
              ( Nucleus (leaf 102) (rel2par span) (text _!that in life one sometimes has to compromise one's principles._!) )
            )
          )
        )
      )
    )
  )
  ( Nucleus (span 103 119) (rel2par TextualOrganization)
    ( Nucleus (leaf 103) (rel2par TextualOrganization) (text _!Odds and Ends_!) )
    ( Nucleus (span 104 119) (rel2par TextualOrganization)
      ( Nucleus (span 104 109) (rel2par Topic-Shift)
        ( Nucleus (span 104 106) (rel2par span)
          ( Nucleus (span 104 105) (rel2par Same-Unit)
            ( Nucleus (leaf 104) (rel2par span) (text _!THE NUMBER of women and minorities_!) )
            ( Satellite (leaf 105) (rel2par elaboration-object-attribute-e) (text _!who hold jobs in top management in the nation's largest banks_!) )
          )
          ( Nucleus (leaf 106) (rel2par Same-Unit) (text _!has more than doubled since 1978._!) )
        )
        ( Satellite (span 107 109) (rel2par explanation-argumentative)
          ( Nucleus (span 107 108) (rel2par span)
            ( Satellite (leaf 107) (rel2par attribution) (text _!The American Bankers Association says_!) )
            ( Nucleus (leaf 108) (rel2par span) (text _!that women make up 47% of officials and managers in the top 50 banks, up from 33% in 1978._!) )
          )
          ( Satellite (leaf 109) (rel2par elaboration-additional) (text _!The share of minorities in those positions has risen to 16% from 12%. . . ._!) )
        )
      )
      ( Nucleus (span 110 115) (rel2par Topic-Shift)
        ( Nucleus (span 110 114) (rel2par span)
          ( Nucleus (span 110 111) (rel2par span)
            ( Nucleus (leaf 110) (rel2par span) (text _!Per-capita personal income in the U.S. grew faster than inflation last year,_!) )
            ( Satellite (leaf 111) (rel2par attribution) (text _!according to the Bureau of Economic Analysis._!) )
          )
          ( Satellite (span 112 114) (rel2par elaboration-additional)
            ( Nucleus (span 112 113) (rel2par Same-Unit)
              ( Nucleus (leaf 112) (rel2par span) (text _!The amount of income_!) )
              ( Satellite (leaf 113) (rel2par elaboration-object-attribute-e) (text _!divvied up for each man, woman and child_!) )
            )
            ( Nucleus (leaf 114) (rel2par Same-Unit) (text _!was $16,489 in 1988, up 6.6% from $15,472 in 1987._!) )
          )
        )
        ( Satellite (leaf 115) (rel2par elaboration-additional) (text _!Per capita personal income ranged from $11,116 in Mississippi to $23,059 in Connecticut. . . ._!) )
      )
      ( Nucleus (span 116 119) (rel2par Topic-Shift)
        ( Nucleus (span 116 117) (rel2par span)
          ( Nucleus (leaf 116) (rel2par span) (text _!There are 13.1 million students in college this fall, up 2% from 1988,_!) )
          ( Satellite (leaf 117) (rel2par attribution) (text _!the National Center for Education Statistics estimates._!) )
        )
        ( Satellite (span 118 119) (rel2par elaboration-set-member)
          ( Nucleus (leaf 118) (rel2par List) (text _!About 54% are women,_!) )
          ( Nucleus (leaf 119) (rel2par List) (text _!and 44% are part-time students._!) )
        )
      )
    )
  )
)
