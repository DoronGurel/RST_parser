( Root (span 1 81)
  ( Nucleus (leaf 1) (rel2par Question-Answer) (text _!Are consumers too deep in hock?_!) )
  ( Nucleus (span 2 81) (rel2par Question-Answer)
    ( Satellite (span 2 70) (rel2par explanation-argumentative)
      ( Nucleus (span 2 17) (rel2par Contrast)
        ( Nucleus (leaf 2) (rel2par List) (text _!A lot of observers think so,_!) )
        ( Nucleus (span 3 17) (rel2par List)
          ( Nucleus (leaf 3) (rel2par Same-Unit) (text _!and,_!) )
          ( Nucleus (span 4 17) (rel2par Same-Unit)
            ( Nucleus (span 4 5) (rel2par span)
              ( Satellite (leaf 4) (rel2par condition) (text _!if they're right,_!) )
              ( Nucleus (leaf 5) (rel2par span) (text _!the whole economy as well as the spendthrifts among us could be hurt._!) )
            )
            ( Satellite (span 6 17) (rel2par explanation-argumentative)
              ( Nucleus (span 6 11) (rel2par span)
                ( Nucleus (span 6 10) (rel2par List)
                  ( Nucleus (span 6 8) (rel2par span)
                    ( Nucleus (span 6 7) (rel2par Same-Unit)
                      ( Nucleus (leaf 6) (rel2par span) (text _!A sudden, forced cutback by consumers,_!) )
                      ( Satellite (leaf 7) (rel2par elaboration-additional-e) (text _!who normally account for about two-thirds of economic activity,_!) )
                    )
                    ( Nucleus (leaf 8) (rel2par Same-Unit) (text _!would damp the economy at a time_!) )
                  )
                  ( Satellite (span 9 10) (rel2par circumstance)
                    ( Nucleus (leaf 9) (rel2par List) (text _!when plant-and-equipment spending is slowing_!) )
                    ( Nucleus (leaf 10) (rel2par List) (text _!and deficit-racked governments can't readily take up the slack._!) )
                  )
                )
                ( Nucleus (leaf 11) (rel2par List) (text _!And another wave of bad loans would further batter many already-shaky lending institutions._!) )
              )
              ( Satellite (span 12 17) (rel2par evidence)
                ( Nucleus (leaf 12) (rel2par span) (text _!The worriers cite some worrisome trends._!) )
                ( Satellite (span 13 17) (rel2par example)
                  ( Nucleus (span 13 14) (rel2par List)
                    ( Nucleus (leaf 13) (rel2par Contrast) (text _!During the almost seven-year-old economic expansion, inflation-adjusted gross national product, disposable personal income and personal consumption expenditures have risen 30%,_!) )
                    ( Nucleus (leaf 14) (rel2par Contrast) (text _!but inflation-adjusted consumer installment credit has surged 66%._!) )
                  )
                  ( Nucleus (span 15 17) (rel2par List)
                    ( Nucleus (span 15 16) (rel2par Same-Unit)
                      ( Nucleus (leaf 15) (rel2par span) (text _!And the ratio of installment debt to disposable personal income_!) )
                      ( Satellite (leaf 16) (rel2par definition-e) (text _!-- personal income after taxes --_!) )
                    )
                    ( Nucleus (leaf 17) (rel2par Same-Unit) (text _!has hit a high of about 18 1/2%._!) )
                  )
                )
              )
            )
          )
        )
      )
      ( Nucleus (span 18 70) (rel2par Contrast)
        ( Nucleus (leaf 18) (rel2par span) (text _!However, these figures don't seem to worry Thomas A. Durkin, an economist at the Federal Reserve Board._!) )
        ( Satellite (span 19 70) (rel2par evidence)
          ( Nucleus (span 19 28) (rel2par List)
            ( Nucleus (span 19 23) (rel2par span)
              ( Nucleus (span 19 20) (rel2par Same-Unit)
                ( Nucleus (leaf 19) (rel2par span) (text _!In a paper_!) )
                ( Satellite (leaf 20) (rel2par elaboration-additional-e) (text _!presented at the recent annual meeting of the National Association of Business Economists in San Francisco,_!) )
              )
              ( Nucleus (span 21 23) (rel2par Same-Unit)
                ( Satellite (leaf 21) (rel2par attribution) (text _!Mr. Durkin comments_!) )
                ( Nucleus (span 22 23) (rel2par span)
                  ( Nucleus (leaf 22) (rel2par List) (text _!that "installment credit always grows rapidly in cyclical advances,_!) )
                  ( Nucleus (leaf 23) (rel2par List) (text _!and growth in this cycle is very typical of earlier experiences."_!) )
                )
              )
            )
            ( Satellite (span 24 28) (rel2par circumstance)
              ( Satellite (leaf 24) (rel2par attribution) (text _!He adds:_!) )
              ( Nucleus (span 25 28) (rel2par span)
                ( Nucleus (leaf 25) (rel2par span) (text _!"We are now witnessing a slowdown_!) )
                ( Satellite (span 26 28) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 26) (rel2par Same-Unit) (text _!which,_!) )
                  ( Nucleus (span 27 28) (rel2par Same-Unit)
                    ( Satellite (leaf 27) (rel2par condition) (text _!if history is a guide,_!) )
                    ( Nucleus (leaf 28) (rel2par span) (text _!could persist for a while."_!) )
                  )
                )
              )
            )
          )
          ( Nucleus (span 29 70) (rel2par List)
            ( Nucleus (leaf 29) (rel2par Question-Answer) (text _!But what about the debt burden?_!) )
            ( Nucleus (span 30 70) (rel2par Question-Answer)
              ( Nucleus (span 30 37) (rel2par span)
                ( Nucleus (span 30 32) (rel2par List)
                  ( Satellite (leaf 30) (rel2par attribution-n) (text _!Mr. Durkin doubts_!) )
                  ( Nucleus (span 31 32) (rel2par span)
                    ( Nucleus (leaf 31) (rel2par span) (text _!that "there is some magic level"_!) )
                    ( Satellite (leaf 32) (rel2par elaboration-object-attribute-e) (text _!at which the ratio of installment debt to disposable income "indicates economic problems."_!) )
                  )
                )
                ( Nucleus (span 33 37) (rel2par List)
                  ( Nucleus (span 33 34) (rel2par Same-Unit)
                    ( Nucleus (leaf 33) (rel2par span) (text _!And, "more importantly,"_!) )
                    ( Satellite (leaf 34) (rel2par attribution-e) (text _!he says,_!) )
                  )
                  ( Nucleus (span 35 37) (rel2par Same-Unit)
                    ( Nucleus (span 35 36) (rel2par Same-Unit)
                      ( Nucleus (leaf 35) (rel2par span) (text _!"the debt burden_!) )
                      ( Satellite (leaf 36) (rel2par elaboration-object-attribute-e) (text _!measured other ways_!) )
                    )
                    ( Nucleus (leaf 37) (rel2par Same-Unit) (text _!is not really in uncharted waters."_!) )
                  )
                )
              )
              ( Satellite (span 38 70) (rel2par explanation-argumentative)
                ( Satellite (span 38 65) (rel2par evidence)
                  ( Nucleus (span 38 40) (rel2par span)
                    ( Nucleus (leaf 38) (rel2par span) (text _!The chart below shows why_!) )
                    ( Satellite (span 39 40) (rel2par comment-e)
                      ( Nucleus (leaf 39) (rel2par span) (text _!(see accompanying illustration_!) )
                      ( Satellite (leaf 40) (rel2par attribution-e) (text _!-- WSJ Oct. 23, 1989)._!) )
                    )
                  )
                  ( Satellite (span 41 65) (rel2par interpretation-s)
                    ( Nucleus (span 41 42) (rel2par List)
                      ( Nucleus (leaf 41) (rel2par span) (text _!The ratio of consumer installment credit to disposable income, though up a bit, hasn't climbed steeply,_!) )
                      ( Satellite (leaf 42) (rel2par consequence-s) (text _!and such debt as a percent of household assets is little changed._!) )
                    )
                    ( Nucleus (span 43 52) (rel2par List)
                      ( Nucleus (span 43 44) (rel2par span)
                        ( Nucleus (leaf 43) (rel2par span) (text _!Moreover, the burden of consumer credit payments relative to disposable income may be "lower in this cycle than earlier,"_!) )
                        ( Satellite (leaf 44) (rel2par attribution) (text _!Mr. Durkin says._!) )
                      )
                      ( Satellite (span 45 52) (rel2par explanation-argumentative)
                        ( Nucleus (span 45 50) (rel2par List)
                          ( Satellite (leaf 45) (rel2par attribution) (text _!He notes_!) )
                          ( Nucleus (span 46 50) (rel2par span)
                            ( Nucleus (leaf 46) (rel2par span) (text _!that some "revolving credit-card credit is actually convenience credit"_!) )
                            ( Satellite (span 47 50) (rel2par elaboration-object-attribute-e)
                              ( Nucleus (span 47 48) (rel2par Comparison)
                                ( Nucleus (leaf 47) (rel2par span) (text _!being used simply as a handy way_!) )
                                ( Satellite (leaf 48) (rel2par elaboration-object-attribute-e) (text _!of paying bills_!) )
                              )
                              ( Nucleus (span 49 50) (rel2par Comparison)
                                ( Nucleus (leaf 49) (rel2par span) (text _!rather than a handy way_!) )
                                ( Satellite (leaf 50) (rel2par elaboration-object-attribute-e) (text _!of borrowing._!) )
                              )
                            )
                          )
                        )
                        ( Nucleus (span 51 52) (rel2par List)
                          ( Satellite (leaf 51) (rel2par attribution) (text _!In addition, he says,_!) )
                          ( Nucleus (leaf 52) (rel2par span) (text _!"longer maturities on automobile and other forms of installment credit boost the stock of debt faster than the flow of repayments and the accompanying payment burden."_!) )
                        )
                      )
                    )
                    ( Nucleus (span 53 65) (rel2par List)
                      ( Nucleus (span 53 57) (rel2par span)
                        ( Nucleus (leaf 53) (rel2par Same-Unit) (text _!And_!) )
                        ( Nucleus (span 54 57) (rel2par Same-Unit)
                          ( Satellite (span 54 55) (rel2par condition)
                            ( Nucleus (leaf 54) (rel2par span) (text _!if you "consider the changing distribution of credit,"_!) )
                            ( Satellite (leaf 55) (rel2par attribution-e) (text _!Mr. Durkin says,_!) )
                          )
                          ( Nucleus (span 56 57) (rel2par span)
                            ( Nucleus (leaf 56) (rel2par span) (text _!"much of the increase in debt in recent years is due to increasing credit use by higher-income families,"_!) )
                            ( Satellite (leaf 57) (rel2par elaboration-additional-e) (text _!that is, "those probably best able to handle it."_!) )
                          )
                        )
                      )
                      ( Satellite (span 58 65) (rel2par example)
                        ( Nucleus (span 58 62) (rel2par span)
                          ( Nucleus (span 58 61) (rel2par span)
                            ( Satellite (span 58 59) (rel2par attribution)
                              ( Satellite (leaf 58) (rel2par manner) (text _!Citing figures on home-equity loans,_!) )
                              ( Nucleus (leaf 59) (rel2par span) (text _!he notes_!) )
                            )
                            ( Nucleus (span 60 61) (rel2par span)
                              ( Nucleus (leaf 60) (rel2par span) (text _!that "11% of homeowners had home-equity credit accounts,_!) )
                              ( Satellite (leaf 61) (rel2par antithesis) (text _!but the proportion rises to 16% of homeowners in the $45,000-$60,000 income range and 23% of homeowners with income above $60,000."_!) )
                            )
                          )
                          ( Satellite (leaf 62) (rel2par elaboration-additional) (text _!And much home-equity credit is used conservatively._!) )
                        )
                        ( Satellite (span 63 65) (rel2par elaboration-additional)
                          ( Nucleus (span 63 64) (rel2par span)
                            ( Nucleus (leaf 63) (rel2par span) (text _!"The most frequent use is home improvement,_!) )
                            ( Satellite (leaf 64) (rel2par elaboration-additional-e) (text _!which presumably improves the value of the property,"_!) )
                          )
                          ( Satellite (leaf 65) (rel2par attribution) (text _!Mr. Durkin says._!) )
                        )
                      )
                    )
                  )
                )
                ( Nucleus (span 66 70) (rel2par span)
                  ( Nucleus (span 66 67) (rel2par Same-Unit)
                    ( Nucleus (leaf 66) (rel2par span) (text _!So, it isn't surprising that consumer-credit delinquencies at banks remain,_!) )
                    ( Satellite (leaf 67) (rel2par attribution-e) (text _!as the chart shows,_!) )
                  )
                  ( Nucleus (span 68 70) (rel2par Same-Unit)
                    ( Nucleus (leaf 68) (rel2par span) (text _!reassuringly below some earlier highs_!) )
                    ( Satellite (span 69 70) (rel2par comment-e)
                      ( Nucleus (leaf 69) (rel2par span) (text _!(see accompanying illustration_!) )
                      ( Satellite (leaf 70) (rel2par attribution-e) (text _!-- WSJ Oct. 23, 1989)._!) )
                    )
                  )
                )
              )
            )
          )
        )
      )
    )
    ( Nucleus (span 71 81) (rel2par span)
      ( Satellite (span 71 79) (rel2par circumstance)
        ( Nucleus (span 71 74) (rel2par span)
          ( Nucleus (span 71 72) (rel2par Contrast)
            ( Satellite (leaf 71) (rel2par concession) (text _!A severe recession could, of course, raise delinquency rates,_!) )
            ( Nucleus (leaf 72) (rel2par span) (text _!but so far the current levels of consumer debt don't seem to loom as a major threat._!) )
          )
          ( Nucleus (span 73 74) (rel2par Contrast)
            ( Satellite (leaf 73) (rel2par attribution) (text _!In fact, the current weakness in auto buying and department-store sales and the gradual upturn in the household saving rate suggest_!) )
            ( Nucleus (leaf 74) (rel2par span) (text _!that consumers, conservative as ever, are already clutching their purses a bit more tightly._!) )
          )
        )
        ( Satellite (span 75 79) (rel2par evidence)
          ( Nucleus (leaf 75) (rel2par span) (text _!In July, consumer installment credit outstanding fell for the first time since January 1987._!) )
          ( Satellite (span 76 79) (rel2par interpretation-s)
            ( Nucleus (span 76 78) (rel2par Cause-Result)
              ( Nucleus (span 76 77) (rel2par span)
                ( Nucleus (leaf 76) (rel2par span) (text _!"Consumers appear unwilling to add to their leverage_!) )
                ( Satellite (leaf 77) (rel2par purpose-e) (text _!to support their spending,"_!) )
              )
              ( Satellite (leaf 78) (rel2par attribution) (text _!Bruce Steinberg, a Merrill Lynch economist, says._!) )
            )
            ( Nucleus (leaf 79) (rel2par Cause-Result) (text _!"As a result, household debt appears to be stabilizing at around 63% of GNP."_!) )
