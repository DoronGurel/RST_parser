( Root (span 1 104)
  ( Nucleus (span 1 38) (rel2par TextualOrganization)
    ( Nucleus (span 1 34) (rel2par TextualOrganization)
      ( Satellite (span 1 27) (rel2par explanation-argumentative)
        ( Nucleus (span 1 2) (rel2par span)
          ( Nucleus (leaf 1) (rel2par span) (text _!Dennis Farney's Oct. 13 page-one article "River of Despair," about the poverty along the Mississippi, fanned childhood memories_!) )
          ( Satellite (leaf 2) (rel2par elaboration-object-attribute-e) (text _!of when my parents were sharecroppers in southeastern Arkansas, only a few miles from the river._!) )
        )
        ( Satellite (span 3 27) (rel2par elaboration-additional)
          ( Satellite (span 3 6) (rel2par circumstance)
            ( Nucleus (span 3 4) (rel2par span)
              ( Satellite (leaf 3) (rel2par concession) (text _!Although we were white,_!) )
              ( Nucleus (leaf 4) (rel2par span) (text _!the same economic factors affected us_!) )
            )
            ( Satellite (span 5 6) (rel2par comparison)
              ( Nucleus (leaf 5) (rel2par span) (text _!as affects the black people_!) )
              ( Satellite (leaf 6) (rel2par elaboration-object-attribute-e) (text _!Mr. Farney writes about._!) )
            )
          )
          ( Nucleus (span 7 27) (rel2par span)
            ( Nucleus (leaf 7) (rel2par Sequence) (text _!Fortunately, an aunt with a college degree bought a small farm_!) )
            ( Nucleus (span 8 9) (rel2par Sequence)
              ( Nucleus (leaf 8) (rel2par span) (text _!and moved us 50 miles north to good schools and an environment_!) )
              ( Satellite (leaf 9) (rel2par elaboration-object-attribute-e) (text _!that opened the world of opportunity for me as an eight-year-old._!) )
            )
            ( Nucleus (span 10 27) (rel2par Sequence)
              ( Nucleus (span 10 11) (rel2par Temporal-Same-Time)
                ( Satellite (leaf 10) (rel2par concession) (text _!Though I've been blessed with academic degrees and some success in the materialistic world,_!) )
                ( Nucleus (leaf 11) (rel2par span) (text _!I've never forgotten or lost contact with those memories of the 1930s._!) )
              )
              ( Nucleus (span 12 27) (rel2par Temporal-Same-Time)
                ( Nucleus (span 12 17) (rel2par span)
                  ( Nucleus (leaf 12) (rel2par span) (text _!Most of the land in that and other parts of the Delta are now owned by second, third or fourth generations of the same families._!) )
                  ( Satellite (span 13 17) (rel2par elaboration-additional)
                    ( Nucleus (leaf 13) (rel2par span) (text _!These are the families_!) )
                    ( Satellite (span 14 17) (rel2par elaboration-object-attribute-e)
                      ( Nucleus (span 14 15) (rel2par Same-Unit)
                        ( Nucleus (leaf 14) (rel2par List) (text _!who used_!) )
                        ( Nucleus (leaf 15) (rel2par List) (text _!-- and sometime abused_!) )
                      )
                      ( Nucleus (span 16 17) (rel2par Same-Unit)
                        ( Nucleus (leaf 16) (rel2par span) (text _!-- their sharecroppers, people_!) )
                        ( Satellite (leaf 17) (rel2par elaboration-object-attribute-e) (text _!who had no encouragement and little access to an education or training for a better life._!) )
                      )
                    )
                  )
                )
                ( Satellite (span 18 27) (rel2par elaboration-additional)
                  ( Nucleus (span 18 22) (rel2par Contrast)
                    ( Nucleus (span 18 21) (rel2par Cause-Result)
                      ( Satellite (span 18 20) (rel2par circumstance)
                        ( Satellite (leaf 18) (rel2par circumstance) (text _!Following World War II,_!) )
                        ( Nucleus (span 19 20) (rel2par span)
                          ( Nucleus (leaf 19) (rel2par span) (text _!when one family with mechanized equipment could farm crops_!) )
                          ( Satellite (leaf 20) (rel2par comparison) (text _!formerly requiring 20 families,_!) )
                        )
                      )
                      ( Nucleus (leaf 21) (rel2par span) (text _!the surplus people were dumped into the mainstream of society with no Social Security, no skills in the workplace, no hope for their future except welfare._!) )
                    )
                    ( Nucleus (leaf 22) (rel2par Cause-Result) (text _!And today, many of their children, grandchildren and great-grandchildren remain on welfare._!) )
                  )
                  ( Nucleus (span 23 27) (rel2par Contrast)
                    ( Nucleus (span 23 24) (rel2par Disjunction)
                      ( Nucleus (leaf 23) (rel2par span) (text _!In the meantime, the landowners continue receiving generous subsidies_!) )
                      ( Satellite (leaf 24) (rel2par elaboration-object-attribute-e) (text _!that began during New Deal days._!) )
                    )
                    ( Nucleus (span 25 27) (rel2par Disjunction)
                      ( Nucleus (span 25 26) (rel2par Same-Unit)
                        ( Nucleus (leaf 25) (rel2par span) (text _!Or those_!) )
                        ( Satellite (leaf 26) (rel2par elaboration-object-attribute-e) (text _!who choose not to farm_!) )
                      )
                      ( Nucleus (leaf 27) (rel2par Same-Unit) (text _!can lease their lands and crop allotments for handsome sums._!) )
                    )
                  )
                )
              )
            )
          )
        )
      )
      ( Nucleus (span 28 34) (rel2par span)
        ( Nucleus (leaf 28) (rel2par Contrast) (text _!Farmers in the Midwest and other areas have suffered,_!) )
        ( Nucleus (span 29 34) (rel2par Contrast)
          ( Nucleus (span 29 31) (rel2par span)
            ( Nucleus (leaf 29) (rel2par span) (text _!but those along the Mississippi continue to prosper with holdings_!) )
            ( Satellite (span 30 31) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 30) (rel2par span) (text _!that were built with the sweat of men and women_!) )
              ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!living in economic slavery._!) )
            )
          )
          ( Satellite (span 32 34) (rel2par elaboration-additional)
            ( Satellite (leaf 32) (rel2par circumstance) (text _!And when they were no longer needed,_!) )
            ( Nucleus (span 33 34) (rel2par span)
              ( Nucleus (leaf 33) (rel2par List) (text _!they were turned loose_!) )
              ( Nucleus (leaf 34) (rel2par List) (text _!unprepared to build lives of their own._!) )
            )
          )
        )
      )
    )
    ( Nucleus (span 35 38) (rel2par TextualOrganization)
      ( Nucleus (leaf 35) (rel2par List) (text _!Denton Harris_!) )
      ( Nucleus (leaf 36) (rel2par List) (text _!Chairman_!) )
      ( Nucleus (leaf 37) (rel2par List) (text _!Metro Bank_!) )
      ( Nucleus (leaf 38) (rel2par List) (text _!Atlanta_!) )
    )
  )
  ( Nucleus (span 39 62) (rel2par TextualOrganization)
    ( Nucleus (span 39 60) (rel2par TextualOrganization)
      ( Nucleus (span 39 57) (rel2par span)
        ( Nucleus (span 39 49) (rel2par span)
          ( Nucleus (span 39 45) (rel2par span)
            ( Satellite (span 39 41) (rel2par evaluation-s)
              ( Nucleus (span 39 40) (rel2par span)
                ( Satellite (leaf 39) (rel2par result) (text _!Because the cycle of poverty along the lower Mississippi goes back so many generations,_!) )
                ( Nucleus (leaf 40) (rel2par span) (text _!breaking this cycle will be next to impossible._!) )
              )
              ( Satellite (leaf 41) (rel2par elaboration-additional) (text _!Sadly, the cycle appears not as waves but as a downward spiral._!) )
            )
            ( Nucleus (span 42 45) (rel2par span)
              ( Nucleus (span 42 43) (rel2par Same-Unit)
                ( Nucleus (leaf 42) (rel2par span) (text _!Yet the evidence_!) )
                ( Satellite (leaf 43) (rel2par elaboration-object-attribute-e) (text _!that we have not hit bottom_!) )
              )
              ( Nucleus (span 44 45) (rel2par Same-Unit)
                ( Nucleus (leaf 44) (rel2par span) (text _!is found in the fact_!) )
                ( Satellite (leaf 45) (rel2par elaboration-object-attribute-e) (text _!that we are not yet helping ourselves._!) )
              )
            )
          )
          ( Satellite (span 46 49) (rel2par evidence)
            ( Nucleus (leaf 46) (rel2par List) (text _!The people of the Delta are waiting for that big factory to open, river traffic to increase, government spending to fund job-training programs or public schools to educate apathetic students._!) )
            ( Nucleus (span 47 48) (rel2par List)
              ( Satellite (leaf 47) (rel2par reason) (text _!Because we refuse to face the tough answers,_!) )
              ( Nucleus (leaf 48) (rel2par span) (text _!the questions continue as fodder for the commissions and committees, for the media and politicians._!) )
            )
            ( Nucleus (leaf 49) (rel2par List) (text _!Coffee-shop chatter does not lend itself to solving the problems of racism, teen pregnancy or lack of parental support or guidance._!) )
          )
        )
        ( Satellite (span 50 57) (rel2par rhetorical-question)
          ( Nucleus (span 50 53) (rel2par List)
            ( Nucleus (span 50 51) (rel2par span)
              ( Nucleus (leaf 50) (rel2par span) (text _!Does the Delta deserve government help_!) )
              ( Satellite (leaf 51) (rel2par elaboration-object-attribute-e) (text _!in attracting industry_!) )
            )
            ( Satellite (span 52 53) (rel2par circumstance)
              ( Satellite (leaf 52) (rel2par attribution) (text _!when the majority of residents, black and white, do not realize_!) )
              ( Nucleus (leaf 53) (rel2par span) (text _!racism alienates potential employers?_!) )
            )
          )
          ( Nucleus (span 54 57) (rel2par List)
            ( Nucleus (leaf 54) (rel2par span) (text _!Should we focus on the region's infant-mortality rate_!) )
            ( Satellite (span 55 57) (rel2par circumstance)
              ( Nucleus (span 55 56) (rel2par Same-Unit)
                ( Nucleus (leaf 55) (rel2par span) (text _!when the vocal right-wingers and the school boards, mayors and legislators prohibit schools from teaching the two ways_!) )
                ( Satellite (leaf 56) (rel2par elaboration-set-member-e) (text _!(abstinence or contraceptives)_!) )
              )
              ( Nucleus (leaf 57) (rel2par Same-Unit) (text _!of decreasing teen pregnancy?_!) )
            )
          )
        )
      )
      ( Satellite (span 58 60) (rel2par conclusion)
        ( Satellite (leaf 58) (rel2par concession) (text _!Delta problems are difficult, not impossible, to solve_!) )
        ( Nucleus (span 59 60) (rel2par span)
          ( Satellite (leaf 59) (rel2par attribution) (text _!-- I am just not convinced_!) )
          ( Nucleus (leaf 60) (rel2par span) (text _!that we are ready to solve them yet._!) )
        )
      )
    )
    ( Nucleus (span 61 62) (rel2par TextualOrganization)
      ( Nucleus (leaf 61) (rel2par List) (text _!Leslie Falls Humphries_!) )
      ( Nucleus (leaf 62) (rel2par List) (text _!Little Rock, Ark._!) )
    )
  )
  ( Nucleus (span 63 77) (rel2par TextualOrganization)
    ( Nucleus (span 63 75) (rel2par TextualOrganization)
      ( Nucleus (span 63 73) (rel2par span)
        ( Nucleus (leaf 63) (rel2par span) (text _!I would like to issue a challenge to corporate America._!) )
        ( Satellite (span 64 73) (rel2par elaboration-general-specific)
          ( Nucleus (span 64 66) (rel2par Question-Answer)
            ( Nucleus (leaf 64) (rel2par Sequence) (text _!The next time expansion plans are mentioned at the old company_!) )
            ( Nucleus (span 65 66) (rel2par Sequence)
              ( Satellite (leaf 65) (rel2par attribution) (text _!and somebody says,_!) )
              ( Nucleus (leaf 66) (rel2par span) (text _!"Aw heck, guys, nobody can do it like Japan or South Korea,"_!) )
            )
          )
          ( Nucleus (span 67 73) (rel2par Question-Answer)
            ( Nucleus (span 67 68) (rel2par Sequence)
              ( Satellite (leaf 67) (rel2par attribution) (text _!I wish you_!) )
              ( Nucleus (leaf 68) (rel2par span) (text _!would butt in_!) )
            )
            ( Nucleus (span 69 73) (rel2par Sequence)
              ( Nucleus (span 69 70) (rel2par Sequence)
                ( Satellite (leaf 69) (rel2par attribution) (text _!and say,_!) )
                ( Nucleus (leaf 70) (rel2par span) (text _!"Hold it, fellas,_!) )
              )
              ( Nucleus (leaf 71) (rel2par Sequence) (text _!why don't we compare prices_!) )
              ( Nucleus (span 72 73) (rel2par Sequence)
                ( Nucleus (leaf 72) (rel2par span) (text _!and use our own little Third World country._!) )
                ( Satellite (leaf 73) (rel2par consequence-s) (text _!We would even save on freight."_!) )
              )
            )
          )
        )
      )
      ( Satellite (span 74 75) (rel2par interpretation-s)
        ( Nucleus (leaf 74) (rel2par span) (text _!There is no mystery_!) )
        ( Satellite (leaf 75) (rel2par elaboration-object-attribute-e) (text _!why the Delta originated "Singin' the Blues."_!) )
      )
    )
    ( Nucleus (span 76 77) (rel2par TextualOrganization)
      ( Nucleus (leaf 76) (rel2par List) (text _!Eugene S. Clarke IV_!) )
      ( Nucleus (leaf 77) (rel2par List) (text _!Hollandale, Miss._!) )
