( Root (span 1 57)
  ( Nucleus (span 1 54) (rel2par span)
    ( Nucleus (span 1 43) (rel2par span)
      ( Nucleus (span 1 9) (rel2par span)
        ( Nucleus (leaf 1) (rel2par span) (text _!For the real estate industry, a watchword for the 1990s will be buy, more than build._!) )
        ( Satellite (span 2 9) (rel2par elaboration-additional)
          ( Nucleus (leaf 2) (rel2par span) (text _!That's the word expected_!) )
          ( Satellite (span 3 9) (rel2par elaboration-object-attribute-e)
            ( Nucleus (leaf 3) (rel2par span) (text _!to be on the lips of the more than 3,000 developers, pension-fund advisers and real estate financiers_!) )
            ( Satellite (span 4 9) (rel2par elaboration-object-attribute-e)
              ( Nucleus (span 4 5) (rel2par span)
                ( Nucleus (leaf 4) (rel2par span) (text _!slated to attend a four-day conference,_!) )
                ( Satellite (leaf 5) (rel2par elaboration-additional-e) (text _!beginning here today,_!) )
              )
              ( Satellite (span 6 9) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 6) (rel2par span) (text _!sponsored by the Urban Land Institute._!) )
                ( Satellite (span 7 9) (rel2par elaboration-additional)
                  ( Nucleus (span 7 8) (rel2par Same-Unit)
                    ( Nucleus (leaf 7) (rel2par span) (text _!The ULI is a non-profit research and education group_!) )
                    ( Satellite (leaf 8) (rel2par elaboration-object-attribute-e) (text _!based in Washington, D.C.,_!) )
                  )
                  ( Nucleus (leaf 9) (rel2par Same-Unit) (text _!with 14,000 members nationwide._!) )
                )
              )
            )
          )
        )
      )
      ( Satellite (span 10 43) (rel2par evidence)
        ( Nucleus (span 10 15) (rel2par span)
          ( Nucleus (span 10 12) (rel2par span)
            ( Nucleus (leaf 10) (rel2par List) (text _!With the market overbuilt, builders are finding limited opportunities and increased risks._!) )
            ( Nucleus (leaf 11) (rel2par List) (text _!Developers and money managers are looking for bargains among the thousands of financially troubled properties around the country._!) )
            ( Nucleus (leaf 12) (rel2par List) (text _!Real estate professionals now often bill themselves as "turnaround experts" and "workout specialists."_!) )
          )
          ( Satellite (span 13 15) (rel2par elaboration-additional)
            ( Nucleus (leaf 13) (rel2par span) (text _!Conference attendees are expected to be buzzing about the workings of the recently formed Resolution Trust Corp., a federal agency_!) )
            ( Satellite (span 14 15) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 14) (rel2par span) (text _!charged with disposing of an estimated $200 billion of real estate_!) )
              ( Satellite (leaf 15) (rel2par elaboration-object-attribute-e) (text _!dumped in government hands by insolvent savings and loans._!) )
            )
          )
        )
        ( Satellite (span 16 43) (rel2par elaboration-additional)
          ( Nucleus (span 16 27) (rel2par List)
            ( Nucleus (span 16 25) (rel2par List)
              ( Nucleus (span 16 18) (rel2par span)
                ( Nucleus (leaf 16) (rel2par List) (text _!Developers are also eyeing the real estate portfolios of major corporations._!) )
                ( Nucleus (leaf 17) (rel2par List) (text _!Some plan to pursue foreign development ventures, mostly in Europe._!) )
                ( Nucleus (leaf 18) (rel2par List) (text _!And other developers may shift from commercial to residential development in the U.S._!) )
              )
              ( Satellite (span 19 25) (rel2par explanation-argumentative)
                ( Satellite (span 19 20) (rel2par result)
                  ( Nucleus (leaf 19) (rel2par span) (text _!"There aren't as many economically viable alternatives for real estate developers in this country as 10 years ago,"_!) )
                  ( Satellite (leaf 20) (rel2par attribution) (text _!says Charles Shaw, a Chicago-based real estate developer._!) )
                )
                ( Nucleus (span 21 25) (rel2par span)
                  ( Nucleus (span 21 22) (rel2par span)
                    ( Satellite (leaf 21) (rel2par attribution) (text _!"So developers are saying_!) )
                    ( Nucleus (leaf 22) (rel2par span) (text _!they will look into distressed properties._!) )
                  )
                  ( Satellite (span 23 25) (rel2par elaboration-additional)
                    ( Nucleus (leaf 23) (rel2par span) (text _!They'll go into someone else's pasture_!) )
                    ( Satellite (span 24 25) (rel2par condition)
                      ( Nucleus (leaf 24) (rel2par span) (text _!as long as it's greener than the one_!) )
                      ( Satellite (leaf 25) (rel2par elaboration-object-attribute-e) (text _!they're in now."_!) )
                    )
                  )
                )
              )
            )
            ( Nucleus (span 26 27) (rel2par List)
              ( Nucleus (leaf 26) (rel2par span) (text _!Developers are also forming more joint ventures with pension funds and insurance companies_!) )
              ( Satellite (leaf 27) (rel2par elaboration-object-attribute-e) (text _!that can finance big projects._!) )
            )
          )
          ( Nucleus (span 28 34) (rel2par List)
            ( Nucleus (span 28 30) (rel2par span)
              ( Nucleus (span 28 29) (rel2par span)
                ( Nucleus (leaf 28) (rel2par List) (text _!The builders are more willing to give up some equity_!) )
                ( Nucleus (leaf 29) (rel2par List) (text _!and rely on management and consulting fees_!) )
              )
              ( Satellite (leaf 30) (rel2par purpose) (text _!to stay afloat in the soft market._!) )
            )
            ( Satellite (span 31 34) (rel2par explanation-argumentative)
              ( Nucleus (span 31 33) (rel2par span)
                ( Nucleus (span 31 32) (rel2par span)
                  ( Nucleus (leaf 31) (rel2par span) (text _!"Developers are teaming up with institutions_!) )
                  ( Satellite (leaf 32) (rel2par elaboration-additional-e) (text _!often acting as project managers,"_!) )
                )
                ( Satellite (leaf 33) (rel2par attribution) (text _!says Smedes York, ULI president and president of York Properties Inc., of Raleigh, N.C._!) )
              )
              ( Satellite (leaf 34) (rel2par interpretation-s) (text _!"They are growing more pragmatic about their role."_!) )
            )
          )
          ( Nucleus (span 35 43) (rel2par List)
            ( Nucleus (span 35 36) (rel2par span)
              ( Nucleus (leaf 35) (rel2par span) (text _!Real estate firms are also using their alliances with financial institutions_!) )
              ( Satellite (leaf 36) (rel2par purpose) (text _!to amass acquisition funds._!) )
            )
            ( Satellite (span 37 43) (rel2par rhetorical-question)
              ( Nucleus (span 37 42) (rel2par span)
                ( Nucleus (span 37 38) (rel2par span)
                  ( Nucleus (leaf 37) (rel2par Sequence) (text _!"Why should you beat your brains out fighting the environmentalists, the neighborhood groups, dealing with traffic mitigation, sewers and fighting city hall,_!) )
                  ( Nucleus (leaf 38) (rel2par Sequence) (text _!then try to convince a lender to lend you money in an overbuilt market_!) )
                )
                ( Satellite (span 39 42) (rel2par circumstance)
                  ( Nucleus (leaf 39) (rel2par Sequence) (text _!when you can get pension fund money,_!) )
                  ( Nucleus (leaf 40) (rel2par Sequence) (text _!buy a portfolio,_!) )
                  ( Nucleus (leaf 41) (rel2par Sequence) (text _!sell off pieces off it_!) )
                  ( Nucleus (leaf 42) (rel2par Sequence) (text _!and play your own game?"_!) )
                )
              )
              ( Satellite (leaf 43) (rel2par attribution) (text _!says Jack Rodman, managing partner of the Los Angeles office of Kenneth Leventhal Inc. a national accounting firm._!) )
            )
          )
        )
      )
    )
    ( Satellite (span 44 54) (rel2par evaluation-s)
      ( Nucleus (span 44 47) (rel2par List)
        ( Satellite (leaf 44) (rel2par attribution) (text _!But experts say_!) )
        ( Nucleus (span 45 47) (rel2par span)
          ( Nucleus (span 45 46) (rel2par Same-Unit)
            ( Nucleus (leaf 45) (rel2par span) (text _!that_!) )
            ( Satellite (leaf 46) (rel2par circumstance-e) (text _!when it comes to distressed properties,_!) )
          )
          ( Nucleus (leaf 47) (rel2par Same-Unit) (text _!finding diamonds in the rough isn't easy._!) )
        )
      )
      ( Nucleus (span 48 54) (rel2par List)
        ( Nucleus (span 48 50) (rel2par List)
          ( Nucleus (span 48 49) (rel2par span)
            ( Nucleus (leaf 48) (rel2par List) (text _!The level of interest in the RTC's properties has been greater than expected,_!) )
            ( Nucleus (leaf 49) (rel2par List) (text _!and has come from larger companies than initially anticipated,_!) )
          )
          ( Satellite (leaf 50) (rel2par attribution) (text _!says Stan Ross, Leventhal's co-managing partner._!) )
        )
        ( Nucleus (span 51 54) (rel2par List)
          ( Nucleus (leaf 51) (rel2par Same-Unit) (text _!And_!) )
          ( Nucleus (span 52 54) (rel2par Same-Unit)
            ( Satellite (span 52 53) (rel2par purpose-e)
              ( Nucleus (leaf 52) (rel2par span) (text _!to succeed in the turnaround business,_!) )
              ( Satellite (leaf 53) (rel2par attribution) (text _!he says,_!) )
            )
            ( Nucleus (leaf 54) (rel2par span) (text _!developers may have to put in a lot of money and time._!) )
          )
        )
      )
    )
  )
  ( Satellite (span 55 57) (rel2par interpretation-s)
    ( Nucleus (leaf 55) (rel2par span) (text _!Finding pension funds and other sources willing to invest is a high priority._!) )
    ( Satellite (span 56 57) (rel2par elaboration-additional)
      ( Satellite (leaf 56) (rel2par attribution) (text _!Quips David Shulman, director of real estate research for Salomon Brothers Inc.:_!) )
      ( Nucleus (leaf 57) (rel2par span) (text _!"A theme of the Urban Land conference will be `take a pension fund manager to lunch. '"_!) )
    )
  )
)
