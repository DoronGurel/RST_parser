( Root (span 1 45)
  ( Nucleus (span 1 44) (rel2par span)
    ( Nucleus (span 1 26) (rel2par span)
      ( Nucleus (span 1 19) (rel2par span)
        ( Nucleus (span 1 10) (rel2par span)
          ( Nucleus (span 1 7) (rel2par span)
            ( Nucleus (span 1 3) (rel2par span)
              ( Nucleus (leaf 1) (rel2par span) (text _!Ashton-Tate Corp. reported a net loss of $19.4 million, or 74 cents a share, for the third quarter,_!) )
              ( Satellite (span 2 3) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 2) (rel2par span) (text _!which was burdened by severance costs and the expense_!) )
                ( Satellite (leaf 3) (rel2par elaboration-object-attribute-e) (text _!of upgrading its database software inventories._!) )
              )
            )
            ( Satellite (span 4 7) (rel2par elaboration-additional)
              ( Nucleus (span 4 5) (rel2par span)
                ( Satellite (leaf 4) (rel2par attribution) (text _!The software company said_!) )
                ( Nucleus (leaf 5) (rel2par span) (text _!revenue slid 28% to $53.9 million._!) )
              )
              ( Satellite (span 6 7) (rel2par comparison)
                ( Nucleus (leaf 6) (rel2par span) (text _!This contrasts with the year-ago quarter,_!) )
                ( Satellite (leaf 7) (rel2par elaboration-object-attribute-e) (text _!when the company had net income of $11.7 million, or 45 cents a share, on revenue of $75.7 million._!) )
              )
            )
          )
          ( Satellite (span 8 10) (rel2par elaboration-additional)
            ( Nucleus (span 8 9) (rel2par span)
              ( Nucleus (leaf 8) (rel2par span) (text _!For the nine months, Ashton-Tate had a loss of $27.6 million, or $1.05 a share._!) )
              ( Satellite (leaf 9) (rel2par comparison) (text _!In the year-ago period, the company had profit of $34.3 million, or $1.32 a share._!) )
            )
            ( Satellite (leaf 10) (rel2par elaboration-additional) (text _!Revenue in the period slid almost 8% to $203.2 million from about $220 million last year._!) )
          )
        )
        ( Satellite (span 11 19) (rel2par elaboration-additional)
          ( Nucleus (span 11 16) (rel2par span)
            ( Nucleus (leaf 11) (rel2par span) (text _!Edward M. Esber, chairman, president and chief executive officer, attributed the decline to reduced domestic revenue_!) )
            ( Satellite (span 12 16) (rel2par explanation-argumentative)
              ( Nucleus (span 12 14) (rel2par Same-Unit)
                ( Nucleus (leaf 12) (rel2par span) (text _!because of $4.9 million_!) )
                ( Satellite (span 13 14) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 13) (rel2par span) (text _!spent_!) )
                  ( Satellite (leaf 14) (rel2par purpose) (text _!to upgrade existing software inventories to the new database IV Version 1.1,_!) )
                )
              )
              ( Nucleus (span 15 16) (rel2par Same-Unit)
                ( Nucleus (leaf 15) (rel2par span) (text _!and $1.8 million_!) )
                ( Satellite (leaf 16) (rel2par elaboration-object-attribute-e) (text _!spent on the recent reduction in work force._!) )
              )
            )
          )
          ( Satellite (span 17 19) (rel2par elaboration-additional)
            ( Satellite (leaf 17) (rel2par attribution) (text _!He said_!) )
            ( Nucleus (span 18 19) (rel2par span)
              ( Nucleus (leaf 18) (rel2par span) (text _!the company was "encouraged by feedback"_!) )
              ( Satellite (leaf 19) (rel2par elaboration-object-attribute-e) (text _!it received from selected customers now testing Version 1.1._!) )
            )
          )
        )
      )
      ( Satellite (span 20 26) (rel2par elaboration-additional)
        ( Nucleus (span 20 22) (rel2par span)
          ( Nucleus (leaf 20) (rel2par span) (text _!The red ink came as no surprise to Wall Street,_!) )
          ( Satellite (span 21 22) (rel2par elaboration-additional)
            ( Satellite (leaf 21) (rel2par attribution) (text _!but analysts said_!) )
            ( Nucleus (leaf 22) (rel2par span) (text _!they saw ominous hints of a further delay in volume shipments of Version 1.1, a harbinger of continued losses in the fourth quarter._!) )
          )
        )
        ( Satellite (span 23 26) (rel2par example)
          ( Nucleus (span 23 24) (rel2par span)
            ( Nucleus (leaf 23) (rel2par span) (text _!"The loss is in line with our expectations,"_!) )
            ( Satellite (leaf 24) (rel2par attribution) (text _!said John C. Maxwell III, an analyst with Dillon, Read & Co. in New York._!) )
          )
          ( Satellite (span 25 26) (rel2par elaboration-additional)
            ( Satellite (leaf 25) (rel2par attribution) (text _!He added_!) )
            ( Nucleus (leaf 26) (rel2par span) (text _!gross margins and operating profit "eroded quite dramatically" from the prior quarter, along with sales of existing software product lines like Multimate and Framework._!) )
          )
        )
      )
    )
    ( Satellite (span 27 44) (rel2par elaboration-additional)
      ( Nucleus (span 27 38) (rel2par span)
        ( Satellite (leaf 27) (rel2par antithesis) (text _!"The success of a new product in the database line is needed._!) )
        ( Nucleus (span 28 38) (rel2par span)
          ( Nucleus (span 28 31) (rel2par span)
            ( Nucleus (span 28 30) (rel2par span)
              ( Nucleus (span 28 29) (rel2par Same-Unit)
                ( Nucleus (leaf 28) (rel2par span) (text _!And_!) )
                ( Satellite (leaf 29) (rel2par antithesis-e) (text _!while the company hasn't made a definite statement,_!) )
              )
              ( Nucleus (leaf 30) (rel2par Same-Unit) (text _!it now looks like that's not going to be anytime soon,"_!) )
            )
            ( Satellite (leaf 31) (rel2par attribution) (text _!Mr. Maxwell said._!) )
          )
          ( Satellite (span 32 38) (rel2par evidence)
            ( Nucleus (span 32 33) (rel2par span)
              ( Satellite (leaf 32) (rel2par attribution) (text _!The company said in a statement_!) )
              ( Nucleus (leaf 33) (rel2par span) (text _!that it expects to ship new products "during the next two quarters."_!) )
            )
            ( Satellite (span 34 38) (rel2par elaboration-additional)
              ( Nucleus (span 34 35) (rel2par span)
                ( Nucleus (leaf 34) (rel2par span) (text _!"It now looks like database IV Version 1.1 isn't going to be {widely} available until the first quarter of 1990,"_!) )
                ( Satellite (leaf 35) (rel2par attribution) (text _!said David Bayer, an analyst with Montgomery Securities in San Francisco._!) )
              )
              ( Satellite (span 36 38) (rel2par elaboration-additional)
                ( Nucleus (span 36 37) (rel2par span)
                  ( Nucleus (leaf 36) (rel2par span) (text _!"This is the second delay now_!) )
                  ( Satellite (leaf 37) (rel2par elaboration-object-attribute-e) (text _!in getting the product out the door._!) )
                )
                ( Satellite (leaf 38) (rel2par consequence-s) (text _!It does prolong the pain somewhat."_!) )
              )
            )
          )
        )
      )
      ( Satellite (span 39 44) (rel2par elaboration-additional)
        ( Nucleus (span 39 41) (rel2par Problem-Solution)
          ( Satellite (leaf 39) (rel2par attribution) (text _!Mr. Maxwell said_!) )
          ( Nucleus (span 40 41) (rel2par span)
            ( Satellite (leaf 40) (rel2par otherwise) (text _!unless the company can start shipments of the new product sometime this quarter,_!) )
            ( Nucleus (leaf 41) (rel2par span) (text _!the fourth-quarter loss is likely to be "comparable to the third quarter's."_!) )
          )
        )
        ( Nucleus (span 42 44) (rel2par Problem-Solution)
          ( Nucleus (span 42 43) (rel2par span)
            ( Satellite (leaf 42) (rel2par condition) (text _!If the company can start to ship during this quarter,_!) )
            ( Nucleus (leaf 43) (rel2par span) (text _!it could stem some, if not all of the red ink,_!) )
          )
          ( Satellite (leaf 44) (rel2par attribution) (text _!he said._!) )
        )
      )
    )
  )
  ( Satellite (leaf 45) (rel2par elaboration-additional) (text _!In national over-the-counter trading, Ashton-Tate closed yesterday at $10 a share, up 62.5 cents._!) )
)
