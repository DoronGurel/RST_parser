            ( Nucleus (span 13 20) (rel2par span)
              ( Nucleus (span 13 16) (rel2par span)
                ( Nucleus (leaf 13) (rel2par span) (text _!Both men fit the mood of the times_!) )
                ( Satellite (span 14 16) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 14) (rel2par span) (text _!-- the mood being one of a public_!) )
                  ( Satellite (span 15 16) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 15) (rel2par span) (text _!fed up with officials' rationalizations_!) )
                    ( Satellite (leaf 16) (rel2par elaboration-object-attribute-e) (text _!for why their schools don't work._!) )
                  )
                )
              )
              ( Satellite (span 17 20) (rel2par example)
                ( Nucleus (leaf 17) (rel2par span) (text _!Former Patterson, N.J., principal Joe Clark was no doubt the general public's first experience with this new breed of no-nonsense administrator._!) )
                ( Satellite (span 18 20) (rel2par elaboration-additional)
                  ( Nucleus (leaf 18) (rel2par span) (text _!The subject of the movie "Lean on Me," Mr. Clark controlled his school with a bullhorn and a baseball bat._!) )
                  ( Satellite (span 19 20) (rel2par elaboration-additional)
                    ( Nucleus (leaf 19) (rel2par span) (text _!He may have gone overboard in his pursuit of good discipline,_!) )
                    ( Satellite (leaf 20) (rel2par rhetorical-question) (text _!but isn't it interesting that some of the country's biggest, most troubled school districts are choosing new chiefs from the same gravel-chewing mold?_!) )
                  )
                )
              )
            )
            ( Satellite (span 21 25) (rel2par example)
              ( Satellite (span 21 24) (rel2par attribution)
                ( Nucleus (span 21 23) (rel2par Same-Unit)
                  ( Nucleus (leaf 21) (rel2par span) (text _!Elena Scambio, the woman_!) )
                  ( Satellite (span 22 23) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 22) (rel2par span) (text _!assigned to run the Jersey City school system_!) )
                    ( Satellite (leaf 23) (rel2par elaboration-object-attribute-e) (text _!that was taken over by the state,_!) )
                  )
                )
                ( Nucleus (leaf 24) (rel2par Same-Unit) (text _!says_!) )
              )
              ( Nucleus (leaf 25) (rel2par span) (text _!her top priority will be to "cut through the dead hand of bureaucracy."_!) )
