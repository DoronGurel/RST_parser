( Root (span 1 24)
  ( Nucleus (span 1 2) (rel2par span)
    ( Nucleus (leaf 1) (rel2par Sequence) (text _!The Federal National Mortgage Association set up a three-member office of the chairman_!) )
    ( Nucleus (leaf 2) (rel2par Sequence) (text _!and elected James A. Johnson as vice chairman, effective Jan. 1._!) )
  )
