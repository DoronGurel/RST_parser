        ( Nucleus (span 106 113) (rel2par span)
          ( Nucleus (leaf 106) (rel2par span) (text _!The protection from using married puts is clearly superior to that_!) )
          ( Satellite (span 107 113) (rel2par elaboration-object-attribute-e)
            ( Nucleus (span 107 108) (rel2par span)
              ( Nucleus (leaf 107) (rel2par span) (text _!afforded by another options strategy_!) )
              ( Satellite (leaf 108) (rel2par elaboration-object-attribute-e) (text _!some investors consider using during troubled times:_!) )
            )
