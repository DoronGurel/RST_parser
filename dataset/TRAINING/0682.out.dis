( Root (span 1 43)
  ( Nucleus (span 1 3) (rel2par span)
    ( Nucleus (leaf 1) (rel2par span) (text _!Federal investigators have identified the problem in last July's crash of a United Airlines flight in Iowa:_!) )
    ( Satellite (span 2 3) (rel2par elaboration-object-attribute-e)
      ( Nucleus (leaf 2) (rel2par span) (text _!a structural flaw_!) )
      ( Satellite (leaf 3) (rel2par elaboration-object-attribute-e) (text _!that developed during the making of a titanium engine disk._!) )
    )
  )
  ( Satellite (span 4 43) (rel2par summary-n)
    ( Nucleus (span 4 15) (rel2par span)
      ( Nucleus (span 4 11) (rel2par span)
        ( Nucleus (span 4 10) (rel2par span)
          ( Nucleus (span 4 6) (rel2par span)
            ( Satellite (leaf 4) (rel2par attribution) (text _!For several months, officials at the Federal Aviation Administration and the National Transportation Safety Board have suspected_!) )
            ( Nucleus (span 5 6) (rel2par span)
              ( Nucleus (leaf 5) (rel2par span) (text _!that a metallurgical flaw in the disk led to a crack_!) )
              ( Satellite (leaf 6) (rel2par elaboration-object-attribute-e) (text _!that ultimately caused the tail engine to break apart in flight._!) )
            )
          )
          ( Satellite (span 7 10) (rel2par elaboration-additional)
            ( Satellite (span 7 8) (rel2par result)
              ( Nucleus (leaf 7) (rel2par span) (text _!The explosion sent shards of metal flying,_!) )
              ( Satellite (leaf 8) (rel2par elaboration-object-attribute-e) (text _!severing the DC-10's hydraulic or control systems,_!) )
            )
            ( Nucleus (span 9 10) (rel2par span)
              ( Nucleus (leaf 9) (rel2par span) (text _!and led to the crash_!) )
              ( Satellite (leaf 10) (rel2par elaboration-object-attribute-e) (text _!that killed 112 people._!) )
            )
          )
        )
        ( Satellite (leaf 11) (rel2par elaboration-additional) (text _!But investigators could confirm their theory only after the recent retrieval of a big chunk of Flight 232's tail engine from a cornfield near the Sioux City Airport in Iowa._!) )
      )
      ( Satellite (span 12 15) (rel2par elaboration-additional)
        ( Nucleus (leaf 12) (rel2par span) (text _!The safety board will begin four days of hearings on the accident tomorrow in Sioux City._!) )
        ( Satellite (span 13 15) (rel2par elaboration-additional)
          ( Nucleus (span 13 14) (rel2par Same-Unit)
            ( Nucleus (leaf 13) (rel2par span) (text _!Among the issues_!) )
            ( Satellite (leaf 14) (rel2par elaboration-object-attribute-e) (text _!the board will examine_!) )
          )
          ( Nucleus (leaf 15) (rel2par Same-Unit) (text _!is whether United Airlines, a unit of UAL Corp., should have been able to detect the cracks through maintenance checks._!) )
        )
      )
    )
    ( Satellite (span 16 43) (rel2par elaboration-additional)
      ( Satellite (span 16 28) (rel2par background)
        ( Satellite (span 16 22) (rel2par result)
          ( Satellite (span 16 17) (rel2par background)
            ( Nucleus (leaf 16) (rel2par span) (text _!The engine involved was a CF6-6_!) )
            ( Satellite (leaf 17) (rel2par elaboration-additional-e) (text _!made by General Electric Co._!) )
          )
          ( Nucleus (span 18 22) (rel2par span)
            ( Satellite (leaf 18) (rel2par attribution) (text _!Anthony Broderick, the FAA's acting executive director for regulatory standards and compliance, said_!) )
            ( Nucleus (span 19 22) (rel2par span)
              ( Satellite (leaf 19) (rel2par attribution) (text _!that recent tests of the failed engine disk indicate_!) )
              ( Nucleus (span 20 22) (rel2par span)
                ( Nucleus (span 20 21) (rel2par Same-Unit)
                  ( Nucleus (leaf 20) (rel2par span) (text _!that a flaw_!) )
                  ( Satellite (leaf 21) (rel2par elaboration-additional-e) (text _!-- known as "hard alpha" --_!) )
                )
                ( Nucleus (leaf 22) (rel2par Same-Unit) (text _!occurred in the titanium during its production almost 20 years ago._!) )
              )
            )
          )
        )
        ( Nucleus (span 23 28) (rel2par span)
          ( Satellite (leaf 23) (rel2par attribution) (text _!He said_!) )
          ( Nucleus (span 24 28) (rel2par span)
            ( Satellite (span 24 25) (rel2par antithesis)
              ( Nucleus (leaf 24) (rel2par span) (text _!there wasn't any way_!) )
              ( Satellite (leaf 25) (rel2par elaboration-object-attribute-e) (text _!to detect the flaw at that time,_!) )
            )
            ( Nucleus (span 26 28) (rel2par span)
              ( Nucleus (leaf 26) (rel2par span) (text _!and that the process has since been changed_!) )
              ( Satellite (span 27 28) (rel2par purpose)
                ( Nucleus (leaf 27) (rel2par span) (text _!to decrease the chance_!) )
                ( Satellite (leaf 28) (rel2par elaboration-object-attribute-e) (text _!that such flaws would occur._!) )
