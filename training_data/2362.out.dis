        ( Nucleus (span 57 61) (rel2par span)
          ( Nucleus (span 57 59) (rel2par span)
            ( Nucleus (span 57 58) (rel2par Same-Unit)
              ( Nucleus (leaf 57) (rel2par span) (text _!The missile venture,_!) )
              ( Satellite (leaf 58) (rel2par attribution-e) (text _!the British Aerospace spokeswoman said,_!) )
            )
            ( Nucleus (leaf 59) (rel2par Same-Unit) (text _!is a needed response to the "new environment" in defense contracting._!) )
          )
          ( Satellite (span 60 61) (rel2par elaboration-additional)
            ( Nucleus (leaf 60) (rel2par List) (text _!For both Thomson and British Aerospace, earnings in their home markets have come under pressure from increasingly tight-fisted defense ministries;_!) )
            ( Nucleus (leaf 61) (rel2par List) (text _!and Middle East sales, a traditional mainstay for both companies' exports, have been hurt by five years of weak oil prices._!) )
          )
