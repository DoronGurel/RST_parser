            ( Nucleus (span 63 67) (rel2par span)
              ( Satellite (span 63 65) (rel2par attribution)
                ( Nucleus (span 63 64) (rel2par Same-Unit)
                  ( Nucleus (leaf 63) (rel2par span) (text _!A Kellogg officer,_!) )
                  ( Satellite (leaf 64) (rel2par elaboration-additional-e) (text _!who asked not to be named,_!) )
