              )
            )
            ( Satellite (span 46 55) (rel2par comment)
              ( Nucleus (span 46 48) (rel2par span)
                ( Nucleus (leaf 46) (rel2par span) (text _!But the no-more-nonsense superintendents are going to have to be judicious as well;_!) )
                ( Satellite (span 47 48) (rel2par explanation-argumentative)
                  ( Nucleus (leaf 47) (rel2par Contrast) (text _!incompetent principals and administrators should go,_!) )
                  ( Nucleus (leaf 48) (rel2par Contrast) (text _!but the good ones ought to be left alone._!) )
                )
              )
              ( Satellite (span 49 55) (rel2par elaboration-additional)
                ( Nucleus (leaf 49) (rel2par span) (text _!The situation will be especially delicate for Mr. Kimbrough._!) )
                ( Satellite (span 50 55) (rel2par explanation-argumentative)
                  ( Nucleus (span 50 52) (rel2par span)
                    ( Nucleus (span 50 51) (rel2par Comparison)
                      ( Nucleus (leaf 50) (rel2par span) (text _!He takes over a school system in the midst of radical reform._!) )
                      ( Satellite (leaf 51) (rel2par elaboration-additional) (text _!Chicagoans have just elected 540 neophyte school boards, one for each school._!) )
                    )
                    ( Nucleus (leaf 52) (rel2par Comparison) (text _!This of course led to disaster in New York City._!) )
                  )
                  ( Satellite (span 53 55) (rel2par elaboration-additional)
                    ( Nucleus (leaf 53) (rel2par span) (text _!Getting a community of parents to care again about its schools is essential,_!) )
                    ( Satellite (span 54 55) (rel2par elaboration-additional)
                      ( Nucleus (leaf 54) (rel2par List) (text _!but in Chicago the new boards will make mistakes_!) )
                      ( Nucleus (leaf 55) (rel2par List) (text _!and Mr. Kimbrough will have to identify them._!) )
                    )
